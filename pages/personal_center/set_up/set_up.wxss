.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.index {
  width: 100vw;
  min-height: 100vh;
  background: #ebedf0;
}
.index::before {
  display: block;
  content: '';
  width: 100%;
  height: 20rpx;
}
.index::after {
  display: block;
  content: '';
  width: 100%;
  height: 20rpx;
}
.index .module {
  padding: 28rpx;
  margin: 20rpx 28rpx;
  border-radius: 16rpx;
  background-color: #fff;
  font-family: PingFangSC-Regular, PingFang SC;
  box-shadow: 0 4rpx 2rpx 0 rgba(255,255,255,0.5);
}
.index .module1 {
  margin-top: 0rpx;
  padding-right: 0rpx;
  font-size: 24rpx;
  color: #262626;
  letter-spacing: 0.32rpx;
}
.index .module1 .pub1 {
  margin-top: 20rpx;
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #e0e0e0;
}
.index .module1 .pub1.listOne0 {
  margin-top: 0rpx;
}
.index .module1 .pub1 .accountCancellation {
  display: block;
  color: #fff;
  margin-right: 28rpx;
  padding: 8rpx 34rpx;
  border-radius: 30rpx;
  background: #2a4f44;
}
