<view class="my-etc">
    <view class="tab-choice" wx:if="{{!notAllCar && carList.length}}">
        <view data-index="1" class="{{activeIndex === 1 ? 'active' : ''}}" catchtap="onClickChoiceType">
            客车<view class="line"></view>
        </view>
        <view data-index="2" class="{{activeIndex === 2 ? 'active' : ''}}" catchtap="onClickChoiceType">
            货车<view class="line"></view>
        </view>
    </view>
	<!--	etc列表开始-->
	<block wx:for="{{carList}}" wx:key="index">
		<view class="item animated fadeIn" catchtap="onClickGoETCDetailHandle" data-index="{{index}}">
			<!--上部分开始-->
			<view class="top">
				<view class="left">
					<view class="car-no">{{item.vehPlates}}</view>
					<view class="channel">{{item.cardName ? item.cardName === '贵州黔通卡' ? '中国ETC' : item.cardName : ''}}</view>
				</view>
                <view class="right caution {{item.selfStatus === 7 || item.selfStatus === 8 ? 'failed' : ''}} {{item.selfStatus === 12 ? 'succeed' : ''}}">
                    {{item.selfStatus === 1 ? '已解约' : ''}}
                    {{item.selfStatus === 2 || item.selfStatus === 4 || item.selfStatus === 14 ? '办理中' : ''}}
                    {{item.selfStatus === 3 ? '待支付' : ''}}
                    {{item.selfStatus === 5 ? '待签约' : ''}}
                    {{item.selfStatus === 6 ? '待审核' : ''}}
                    {{item.selfStatus === 7 ? '审核不通过' : ''}}
                    {{item.selfStatus === 8 ? '审核不通过' : ''}}
                    {{item.selfStatus === 9 ? item.flowVersion === 2 ? '待签约高速' : '待签约银行' : ''}}
                    {{item.selfStatus === 10 ? '审核通过' : ''}}
                    {{item.selfStatus === 11 ? '待激活' : ''}}
                    {{item.selfStatus === 12 ? '已激活' : ''}}
                    {{item.selfStatus === 13 ? '待开户' : ''}}
                    {{item.selfStatus === 15 ? '待充保证金' : ''}}
                    {{item.selfStatus === 16 ? '完成验证绑定代扣' : ''}}
                </view>
			</view>
			<!--上部分结束-->
			<!--下部分开始-->
			<view class="bottom">
				<view><text>扣款方式：{{item.productName ? (item.productProcess === 1 ? '微信支付' : item.productName) : ''}}</text></view>
				<view class="{{item.selfStatus === 12 ? '' : 'iconfont'}}" data-index="{{index}}" catchtap="onClickVehicle">
                    {{item.selfStatus === 1 ? '恢复签约' : ''}}
                    {{item.selfStatus === 2 || item.selfStatus === 4 || item.selfStatus === 14 ? '继续办理' : ''}}
                    {{item.selfStatus === 3 ? '去支付' : ''}}
                    {{item.selfStatus === 5 || item.selfStatus === 9 ? '去签约' : ''}}
                    {{item.selfStatus === 6 ? '查看进度' : ''}}
                    {{item.selfStatus === 7 ? '修改资料' : ''}}
                    {{item.selfStatus === 8 ? '查看进度' : ''}}
                    {{item.selfStatus === 10 ? '查看进度' : ''}}
                    {{item.selfStatus === 11 ? '去激活' : ''}}
                    {{item.selfStatus === 12 ? '' : ''}}
                    {{item.selfStatus === 13 ? '去开户' : ''}}
                    {{item.selfStatus === 15 ? '去预充' : ''}}
                    {{item.selfStatus === 16 ? '继续办理' : ''}}
                </view>
			</view>
			<!--下部分结束-->
		</view>
	</block>
	<!--	etc列表结束-->
	<!--	没有车辆开始-->
	<view class="no-car" wx:if="{{carList.length === 0}}">
		<image src="/pages/personal_center/assets/no_car.jpg" mode="widthFix"></image>
		<view>您还没有办理ETC~</view>
	</view>
	<!--	没有车辆结束-->
	<!--	高度填充开始-->
	<view class="auto-fill"></view>
	<!--	高度填充结束-->
	<!--	新增办理开始-->
	<view class="btn" catchtap="onClickAddNewHandle">新增办理</view>
	<!--	新增办理结束-->
	<!--	津易行弹窗提示-->
	<jinyixing-prompt id="notJinYiXingPrompt"></jinyixing-prompt>
	<!--	激活弹窗提示-->
	<signing-prompt id="notSigningPrompt"></signing-prompt>
</view>
