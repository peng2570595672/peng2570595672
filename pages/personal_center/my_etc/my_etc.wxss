.my-etc {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}
.my-etc .tab-choice {
  display: flex;
  margin: 40rpx 66rpx 30rpx;
}
.my-etc .tab-choice > view {
  color: #66666d;
  font-size: 48rpx;
  line-height: 80rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  text-align: center;
  margin-right: 38rpx;
}
.my-etc .tab-choice > view .line {
  width: 120rpx;
  height: 12rpx;
}
.my-etc .tab-choice > view.active {
  color: #33333d;
  font-size: 56rpx;
}
.my-etc .tab-choice > view.active .line {
  background: #2fb565;
}
.my-etc .item {
  margin: 0 34rpx;
  margin-top: 20rpx;
  padding: 0 24rpx;
  height: auto;
  box-shadow: 4rpx 4rpx 46rpx 0 rgba(126,132,156,0.1);
  border-radius: 32rpx;
  background: #fff;
}
.my-etc .item:first-of-type {
  margin-top: 48rpx;
}
.my-etc .item:last-of-type {
  margin-bottom: 20rpx;
}
.my-etc .item .top {
  display: flex;
  height: 130rpx;
  padding-top: 24rpx;
  border-bottom: 1rpx solid #f2f2f2;
  box-sizing: border-box;
}
.my-etc .item .top .left {
  flex: 1;
  padding-left: 6rpx;
}
.my-etc .item .top .left .car-no {
  display: flex;
  align-items: center;
  height: 56rpx;
  font-size: 40rpx;
  font-weight: 500;
}
.my-etc .item .top .left .car-no .center {
  margin-left: 12rpx;
  font-weight: 500;
  font-size: 22rpx;
  width: 68rpx;
  height: 36rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 8rpx;
  box-sizing: border-box;
  font-weight: 500;
  border: 1rpx solid #2a4f44;
  color: #2a4f44;
}
.my-etc .item .top .left .channel {
  margin-top: -4rpx;
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
  font-size: 24rpx;
  color: #99999d;
}
.my-etc .item .top .right {
  margin-top: 8rpx;
  padding-right: 12rpx;
  font-size: 28rpx;
}
.my-etc .item .top .right.caution {
  color: #f7b500;
}
.my-etc .item .top .right.failed {
  color: #ff563c;
}
.my-etc .item .top .right.succeed {
  color: #2fb565;
}
.my-etc .item .bottom {
  position: relative;
  display: flex;
  padding-top: 24rpx;
  padding-bottom: 28rpx;
}
.my-etc .item .bottom > view {
  display: flex;
  height: 44rpx;
  color: #99999d;
}
.my-etc .item .bottom > view:first-of-type {
  flex: 1;
}
.my-etc .item .bottom > view.iconfont {
  display: flex;
  align-items: center;
  padding-right: 12rpx;
}
.my-etc .item .bottom > view.iconfont::after {
  content: '\e60e';
  font-size: 32rpx;
  transform: rotateZ(-180deg);
}
.my-etc .item .truck {
  position: relative;
  display: flex;
  padding-top: 24rpx;
  padding-bottom: 30rpx;
  align-items: center;
  justify-content: space-around;
}
.my-etc .item .truck .sub {
  width: 300rpx;
  height: 64rpx;
  border-radius: 338rpx;
  background: #fffaf6;
  text-align: center;
  line-height: 64rpx;
  font-family: PingFang SC;
  font-size: 28rpx;
  font-weight: normal;
  letter-spacing: 0rpx;
}
.my-etc .item .truck .sub:first-child {
  color: #ff8f1f;
}
.my-etc .item .truck .sub:last-child {
  width: 298rpx;
  height: 62rpx;
  background: none;
  border: 1rpx solid #1ba566;
  color: #1ba566;
}
.my-etc .item .truck .sub_success {
  width: 300rpx;
  height: 64rpx;
  border-radius: 338rpx;
  background: #fffaf6;
  text-align: center;
  line-height: 64rpx;
  font-family: PingFang SC;
  font-size: 28rpx;
  font-weight: normal;
  letter-spacing: 0rpx;
  background: #f9f9f9;
  color: #2a4f44;
}
.my-etc .item .truck .subs {
  width: 300rpx;
  height: 64rpx;
  border-radius: 338rpx;
  background: #fff;
}
.my-etc .item .moreStatus {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  position: absolute;
  left: 0;
  bottom: -60rpx;
  z-index: 9901;
}
.my-etc .item .moreStatus view {
  background-color: #fff;
  color: #000;
  line-height: 2;
}
.my-etc .item .moreStatus view:hover {
  border-bottom: 1px solid #000;
}
.my-etc .auto-fill {
  flex: 1;
}
.my-etc .no-car {
  flex: 1;
  text-align: center;
  height: 80vh;
}
.my-etc .no-car image {
  width: 414rpx;
  margin: 30vh 0 22rpx;
}
.my-etc .no-car view {
  color: #66666d;
  line-height: 44rpx;
  font-size: 32rpx;
  font-weight: 500;
}
.my-etc .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 26rpx 60rpx 40rpx;
  height: 98rpx;
  font-size: 40rpx;
  font-weight: 500;
  color: #fff;
  background: linear-gradient(to right, #15ad69, #84d157);
  border-radius: 49rpx;
}
