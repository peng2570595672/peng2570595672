page {
  background: #f7f7f7;
}
.my_crowdsourcing_gains {
  padding-bottom: 30rpx;
}
.my_crowdsourcing_gains .list {
  width: 690rpx;
  height: 186rpx;
  background: #fff;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(234,233,233,0.5);
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  margin: 31rpx auto 0;
}
.my_crowdsourcing_gains .list .vehicle {
  display: flex;
  line-height: 56rpx;
}
.my_crowdsourcing_gains .list .vehicle > view:first-child {
  flex: 1 1 auto;
  font-size: 40rpx;
  color: #33333d;
}
.my_crowdsourcing_gains .list .vehicle > view:last-child {
  width: 148rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: #2fb565;
  border-radius: 20rpx;
  opacity: 0.6;
  font-size: 20rpx;
  color: #fff;
}
.my_crowdsourcing_gains .list .vehicle > view:last-child.close {
  background: #fa6400;
}
.my_crowdsourcing_gains .list .vehicle > view:last-child.activate {
  background: #039eeb;
}
.my_crowdsourcing_gains .list .vehicle > view:last-child.transact {
  background: #f7b500;
}
.my_crowdsourcing_gains .list .name,
.my_crowdsourcing_gains .list .add_time {
  padding: 10rpx 0;
  font-size: 24rpx;
  color: #66666d;
}
.my_crowdsourcing_gains .list .add_time {
  color: #99999d;
}
.empty {
  text-align: center;
  color: #66666d;
  font-size: 32rpx;
}
.empty image {
  width: 192rpx;
  margin: 23.5vh 0 5vh;
}
.empty view {
  line-height: 44rpx;
}
