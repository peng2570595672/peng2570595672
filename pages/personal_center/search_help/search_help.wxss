.list-loading {
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  background: rgba(0,0,0,0);
}
.list-loading > text {
  color: #777;
  font-size: 14px !important;
}
.list-loading .loading {
  display: inline-block;
  width: 20px !important;
  height: 20px !important;
  vertical-align: middle;
  margin-right: 10rpx;
  background: url("data:image/png;base64,");
  -webkit-background-size: auto 20px;
  background-size: auto 20px;
  animation: am-rotate 1s steps(12) infinite;
  -webkit-animation: am-rotate 1s steps(12) infinite;
  opacity: 0.6;
}
page {
  background: #f7f7f7;
  height: 100vh;
  overflow: hidden;
}
page .help-center .help-list .help-search {
  background: #fff;
  padding: 60rpx 0 40rpx;
  margin-bottom: 20rpx;
}
page .help-center .help-list .help-search>input {
  width: 92%;
  height: 70rpx;
  line-height: 70rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #17ad69;
  padding-left: 40rpx;
  background: #f5f5f5;
  border-radius: 35rpx;
  margin: 0 auto;
}
page .help-center .help-list .list {
  background: #fff;
}
page .help-center .help-list .list .list-title {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 32rpx;
  text-indent: 44rpx;
  color: #33333d;
  border-bottom: 2rpx solid #ececec;
}
page .help-center .help-list .list .list-content {
  height: 98rpx;
  line-height: 98rpx;
  font-size: 28rpx;
  color: #66666d;
  border-bottom: 2rpx solid #ececec;
  margin-left: 40rpx;
}
page .help-center .help-list .list>view:last-child {
  border-bottom: 0;
}
page .help-center .help-list .list .no-search {
  text-align: center;
  height: 100vh;
}
page .help-center .help-list .list .no-search image {
  width: 320rpx;
  margin: 192rpx 0 58rpx;
}
page .help-center .help-list .list .no-search view {
  color: #66666d;
  line-height: 44rpx;
  font-size: 32rpx;
  font-weight: 500;
}
page .mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  background: rgba(0,0,0,0.6);
}
page .mask .top {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  height: 25%;
  background: rgba(0,0,0,0);
}
page .mask .wrapper {
  position: fixed;
  left: 0rpx;
  top: 25%;
  right: 0;
  bottom: 0;
  height: 75%;
  background: #fff;
  border-radius: 32rpx 32rpx 0px 0px;
}
page .mask .wrapper .details-title {
  padding: 90rpx 52rpx 50rpx;
  font-size: 40rpx;
  color: #33333d;
}
page .mask .wrapper .details-list {
  height: 60%;
  overflow-y: scroll;
}
page .mask .wrapper .details-list .details-list-box .cont1 {
  font-size: 30rpx;
  text-align: justify;
  padding: 0 54rpx 50rpx;
  display: flex;
  line-height: 44rpx;
  color: #66666d;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: flex-start;
}
page .mask .wrapper .details-list .details-list-box .cont1 .label::before {
  flex: 0.5;
  content: '\e604';
  line-height: 28rpx;
  color: #33333d;
  font-size: 24rpx;
  margin-right: 18rpx;
}
page .mask .wrapper .details-list .details-list-box .cont1 .char {
  flex: 9.5;
}
page .mask .wrapper .details-list .details-list-box>view:last-child {
  padding-bottom: 0;
}
page .mask .wrapper .is-scroll {
  position: fixed;
  bottom: 0;
  z-index: 2;
  width: 100%;
  font-size: 28rpx;
  line-height: 184rpx;
  text-align: center;
  color: #66666d;
  background: linear-gradient(180deg, rgba(255,255,255,0.2) 0%, #fff 100%);
  padding-top: 74rpx;
}
