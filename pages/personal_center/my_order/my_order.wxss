page {
  min-height: 100vh;
  background: #fff;
}
.my_order {
  padding-top: 88rpx;
}
.my_order .notice {
  position: fixed;
  top: 0;
  z-index: 999;
  width: 100%;
  height: 120rpx;
  background: #f4f4f4;
}
.my_order .notice .notice-box {
  background: #fff;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  padding: 0 30rpx;
  margin: 30rpx;
}
.my_order .notice .notice-box>view:first-child {
  flex: 8;
  font-size: 24rpx;
  color: #ff563c;
}
.my_order .notice .notice-box>view:last-child {
  flex: 2;
  font-size: 20rpx;
  color: #6d7278;
  text-align: right;
}
.my_order .total-cost {
  padding: 20rpx 0 18rpx;
  font-size: 24rpx;
  color: #99999d;
  line-height: 40rpx;
  text-indent: 40rpx;
  border-bottom: 1rpx solid #e7e7e7;
}
.my_order .total-cost>view:first-child {
  color: #66666d;
  font-weight: 600;
}
.my_order .order-list .order-list-content {
  padding: 34rpx 40rpx 28rpx;
  border-bottom: 1rpx solid #e7e7e7;
}
.my_order .order-list .order-list-content .order-list-top {
  display: flex;
  height: 64rpx;
  line-height: 56rpx;
}
.my_order .order-list .order-list-content .order-list-top .license-plate {
  flex: 6;
  font-size: 40rpx;
  font-weight: 500;
  color: #33333d;
}
.my_order .order-list .order-list-content .order-list-top .license-plate text {
  vertical-align: middle;
  font-size: 20rpx;
  color: #39a6f8;
  margin-left: 28rpx;
}
.my_order .order-list .order-list-content .order-list-top .fail-money,
.my_order .order-list .order-list-content .order-list-top .success-money {
  flex: 4;
  font-size: 40rpx;
  color: #ff563c;
  line-height: 64rpx;
  text-align: right;
}
.my_order .order-list .order-list-content .order-list-top .success-money {
  color: #2fb565;
}
.my_order .order-list .order-list-content .site {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #66666d;
  padding: 10rpx 0;
}
.my_order .order-list .order-list-content .site text::after {
  content: '\e651';
  color: #66666d;
  font-size: 24rpx;
}
.my_order .order-list .order-list-content .order-list-bottom {
  display: flex;
  font-size: 24rpx;
  color: #99999d;
  line-height: 30rpx;
}
.my_order .order-list .order-list-content .order-list-bottom .accounting-time,
.my_order .order-list .order-list-content .order-list-bottom .processing-fail,
.my_order .order-list .order-list-content .order-list-bottom .processing-time {
  flex: 5;
}
.my_order .order-list .order-list-content .order-list-bottom .processing-fail {
  text-align: right;
  color: #ff563c;
  line-height: 40rpx;
}
.my_order .order-list .order-list-content .order-list-bottom .processing-time {
  text-align: right;
}
.is_owes {
  padding-top: 208rpx;
}
.empty {
  text-align: center;
  color: #66666d;
  font-size: 32rpx;
}
.empty image {
  width: 192rpx;
  height: 242rpx;
  margin: 23.5vh 0 9.5vh;
}
.empty view {
  line-height: 44rpx;
}
