@font-face {
  font-family: 'iconfont';  /* Project id 1514406 */
  src: url('//at.alicdn.com/t/c/font_1514406_ama3lhhku3u.woff2?t=1696820516639') format('woff2'),
       url('//at.alicdn.com/t/c/font_1514406_ama3lhhku3u.woff?t=1696820516639') format('woff'),
       url('//at.alicdn.com/t/c/font_1514406_ama3lhhku3u.ttf?t=1696820516639') format('truetype');
}
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
mixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    height $height
minWidthHeight($width = 100%, $height = 100%)
    width $width
    min-height $height
mixinsIconfont($content='',$size=16px,$color=#5EBE68)
    content $content
    font-size $size
    color $color
mixinsPositionPack($position=relative)
    position $position
    left 50%
    transform translateX(-50%)
textLineCenter($height)
    text-align center
    line-height $height
flexStyle($align = center, $justify = center)
    display flex
    align-items $align
    justify-content $justify
.index
    minWidthHeight(100vw,100vh)
    background #ededed
    &::after
        display block
        content ''
        mixinsWidthHeight(100%,20rpx)
    .module
        padding 28rpx
        margin 20rpx 28rpx
        border-radius 16rpx
        background-color #ffffff
        font-family PingFangSC-Regular, PingFang SC
        box-shadow 0 4rpx 2rpx 0 rgba(255, 255, 255, 0.5)
    .module1
        height 100rpx
        background rgba(0,0,0,0.4)
        .picker
            mixinsWidthHeight(100%,100rpx)
            background #0f0
    .module2
        .networkName
            margin-top 0rpx
            font-size 28rpx
            line-height 50rpx
            font-weight bold
        .addrNavPhone
            font-size 24rpx
            flexStyle(center,space-between)
            .navPhone
                width 120rpx
                flexStyle(center,space-between)
                .nav
                    &::before
                        mixinsIconfont('\e6bc',18px,#4B576E)
                .phone
                    &::before
                        mixinsIconfont('\e6bd',18px,#4B576E)