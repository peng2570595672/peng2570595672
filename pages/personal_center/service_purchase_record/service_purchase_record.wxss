.content {
  background: #fafafa;
  height: 100vh;
}
.content .list-vehicle {
  padding: 64rpx 76rpx 32rpx 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content .list-vehicle .label {
  width: 190rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.5);
}
.content .list-vehicle .list-title {
  display: flex;
  align-items: center;
  justify-content: center;
  overflow-x: auto;
  white-space: nowrap;
}
.content .list-vehicle .list-title .list-title-child {
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
  color: rgba(0,0,0,0.8);
  line-height: 50rpx;
  margin-right: 26rpx;
}
.content .list-vehicle .list-title .tab-index {
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000;
  border-bottom: 6rpx solid #1ba566;
}
.content .service {
  margin: 0rpx 32rpx;
  background: #fff;
  box-shadow: 0rpx 0rpx 18rpx 0rpx rgba(0,0,0,0.04);
  border-radius: 16rpx;
}
.content .service .service-detail {
  display: flex;
  align-items: center;
  height: 150rpx;
  margin: 0rpx 32rpx;
  border-bottom: 1rpx dashed rgba(0,0,0,0.1);
}
.content .service .service-detail .left {
  flex: 1;
  display: flex;
  flex-direction: column;
  color: rgba(0,0,0,0.3);
  line-height: 34rpx;
}
.content .service .service-detail .left > view:first-child {
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000;
  line-height: 48rpx;
}
.content .service .service-detail .right {
  display: flex;
  flex-direction: column;
}
.content .service .service-detail .right > view:first-child {
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 500;
  color: #000;
  line-height: 48rpx;
}
.content .service .service-detail .right .status {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2fb565;
  line-height: 34rpx;
}
.content .service .list-service {
  margin: 26rpx 32rpx 0;
  display: flex;
  flex-wrap: wrap;
}
.content .service .list-service .item {
  flex: 1;
  display: flex;
  height: 100rpx;
  align-items: center;
  background: #2fb565;
  margin-bottom: 12rpx;
  color: #fff;
}
.content .service .list-service .item::after,
.content .service .list-service .item::before {
  display: inline-block;
  content: '';
  width: 22rpx;
  height: 22rpx;
  border-radius: 50%;
  background: #fff;
  margin-right: -16rpx;
}
.content .service .list-service .item::before {
  margin-left: -16rpx;
}
.content .service .list-service .item .list-name {
  flex: 1;
  display: flex;
  height: 80rpx;
  align-items: center;
  font-size: 28rpx;
  line-height: 34rpx;
}
.content .service .list-service .item .list-name::before {
  display: flex;
  width: 44rpx;
  height: 44rpx;
  align-items: center;
  justify-content: center;
  background: #fff;
  content: '\e66c';
  color: #2fb565;
  font-size: 44rpx;
  border-radius: 50%;
  margin-left: 24rpx;
  margin-right: 8rpx;
}
.content .service .list-service .item .list-name.parking::before {
  content: '\e66d';
}
.content .service .list-service .item .amount {
  display: flex;
  height: 40rpx;
  align-items: center;
  font-size: 24rpx;
  margin-right: 8rpx;
}
.content .service .list-service .item .amount::before {
  content: '\e63d';
  font-size: 16rpx;
  margin-right: 8rpx;
}
.content .service .list-service .item:last-child {
  margin-left: 32rpx;
}
.content .service .service-tips {
  padding-top: 22rpx;
  padding-bottom: 32rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0,0,0,0.3);
  line-height: 34rpx;
}
