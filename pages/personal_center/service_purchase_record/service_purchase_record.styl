.content
	background #FFFFFF
	height 100vh
	.list
		padding 50rpx 32rpx 0
		.item
			margin-bottom 32rpx
			.item-top
				display flex
				height 176rpx
				align-items center
				background #F7F7F7
				padding 0 32rpx
				.item-info
					flex 1
					.title
						font-size 30rpx
						color #000000
						line-height 42rpx
					.time
						font-size 24rpx
						color rgba(0,0,0,5)
						line-height 34rpx
				.item-right
					text-align right
					.price
						font-size 30rpx
						font-family PingFangSC-Semibold, PingFang SC
						font-weight 600
						color #000000
						line-height 42rpx
					.status
						font-size 24rpx
						color rgba(0,0,0,5)
						line-height 34rpx
						&.take-effect
							color #00B85C
				&::before
					display flex
					width 80rpx
					height 80rpx
					align-items center
					justify-content center
					background #00B85C
					content '\e66c'
					color #fff
					font-size 80rpx
					border-radius 50%
					margin-right 32rpx
				&.parking
					&::before
						content '\e66d'
				&.combination
					&::before
						content '\e68e'
						font-size 36rpx
				&::after
					content '\e60e'
					color rgba(0,0,0,.18)
					font-size 26rpx
					transform rotateZ(-180deg)
					margin-left 36rpx
			.item-bottom
				display flex
				align-items center
				height 58rpx
				font-size 24rpx
				color rgba(0,0,0,5)
				background #EEEEEE
				padding-left 32rpx
