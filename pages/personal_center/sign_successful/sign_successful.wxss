.signed_successfully {
  display: flex;
  flex-direction: column;
  justify-content: center;
  background: #fff;
  min-height: 100vh;
}
.signed_successfully > image {
  display: block;
  margin: 80rpx auto 0;
  width: 304rpx;
  height: 238rpx;
}
.signed_successfully .signed_title {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 76rpx;
  font-size: 60rpx;
  font-weight: 500;
}
.signed_successfully .btn {
  width: 590rpx;
  height: 110rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #5ebe68;
  font-size: 36rpx;
  font-family: PingFang SC-Semibold, PingFang SC;
  font-weight: 600;
  color: #fff;
  border-radius: 55rpx;
  margin: auto;
}
