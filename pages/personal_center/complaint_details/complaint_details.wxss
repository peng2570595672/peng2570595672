page {
  background: #f7f7f7;
}
.complaint_details .prompt {
  text-align: center;
  height: 102rpx;
  line-height: 102rpx;
  font-size: 24rpx;
  color: #fff;
  background: #2fb565;
  opacity: 0.59;
}
.complaint_details .complaint_information {
  padding: 60rpx 40rpx 10rpx 56rpx;
}
.complaint_details .complaint_information>view {
  display: flex;
  font-size: 28rpx;
  padding-top: 20rpx;
  color: #66666d;
}
.complaint_details .complaint_information>view>view:first-child {
  flex: 4;
  height: 48rpx;
}
.complaint_details .complaint_information>view>view:last-child {
  flex: 6;
  text-align: right;
}
.complaint_details .complaint_problem {
  margin: 0 38rpx 86rpx 40rpx;
  padding: 20rpx 22rpx 0rpx 20rpx;
  color: #66666d;
  background: #fff;
}
.complaint_details .complaint_problem>view:first-child {
  font-size: 28rpx;
  font-weight: 500;
  line-height: 40rpx;
}
.complaint_details .complaint_problem>view:last-child {
  padding: 10rpx 0;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 34rpx;
}
.complaint_details .feedback_problem {
  padding-bottom: 42rpx;
  background: rgba(47,181,101,0.8);
  color: #fff;
}
.complaint_details .feedback_problem>view {
  padding: 0;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 34rpx;
}
