<view class="order_details {{details.deductStatus === 2 || details.deductStatus === 10 ? 'is-arrears': ''}}">
	<!--	扣费金额开始-->
	<view class="deduction-amount">
		<view class="doubt iconfont" catchtap="goOrderInstructions"><text></text></view>
		<view class="deduction-amount-box">
			<view class="deduction-amount-title">扣费金额</view>
			<view class="{{details.deductStatus !== 1 && details.deductStatus !== 10 && details.deductStatus !== 11 && details.deductStatus !== 12 ? 'deduction-amount-red' : ''}}">
				<text  class="{{details.deductStatus === 7 ? 'refund' : ''}}">{{(details.serviceMoney && details.etcMoney) ? (details.etcMoney + details.serviceMoney)/100 : details.etcMoney/100}}</text>
			</view>
<!--			<view class="failure-prompted {{details.deductStatus === 7 ? 'refund' : ''}}" wx:if="{{details.etcMoney > 0}}">{{details.deductStatus === 7 ? '已退款' : (details.errCodeDes === '0' ? '' : details.errCodeDes)}}</view>-->
		</view>
	</view>
	<!--扣费金额结束-->
	<!--扣费详情开始-->
	<view class="deduction-details">
		<view>
			<view>应付金额</view>
			<view>¥{{details.totalMmout ? details.totalMmout/100 : 0}}</view>
		</view>
		<view>
			<view>通行费减免</view>
			<view>{{details.discountMount ? -details.discountMount/100 : 0}}</view>
		</view>
		<view>
			<view>微信减免</view>
			<view>{{details.wxDiscountAmount ? -details.wxDiscountAmount/100 : 0}}</view>
		</view>
		<view>
			<view>服务费</view>
			<view>{{details.serviceMoney ? details.serviceMoney/100 : 0}}</view>
		</view>
		<view>
			<view>扣费类型</view>
			<view>高速扣费</view>
		</view>
		<view>
			<view>扣费时间</view>
			<view>{{details.deductTime}}</view>
		</view>
		<view>
			<view>出账时间</view>
			<view>{{details.addTime}}</view>
		</view>
	</view>
	<!--扣费详情结束-->
	<!--提示开始-->
	<view class="tips" wx:if="{{details.channelTips.tips1}}">
		<view>
			{{details.channelTips.tips1}}
			<text wx:for="{{details.channelTips.tipsArr}}" wx:key="index" data-model="{{item}}" catchtap="callHotLine"><text class="blue">{{item}}</text>{{index === details.channelTips.tipsArr.length-1 ? '' : '/'}}</text>
			{{details.channelTips.tips2}}
		</view>
	</view>
	<!--提示结束-->
	<!--车辆详情开始-->
	<view class="deduction-details">
		<view>
			<view>车牌号码</view>
			<view>{{details.vehPlate}}</view>
		</view>
		<view>
			<view>进站时间</view>
			<view>{{details.inOpTime}}</view>
		</view>
		<view>
			<view>进站口</view>
			<view>{{details.inStationName}}</view>
		</view>
		<view>
			<view>出站时间</view>
			<view>{{details.opTime}}</view>
		</view>
		<view>
			<view>出站口</view>
			<view>{{details.stationName}}</view>
		</view>
	</view>
	<!--车辆详情结束-->
	<!--底部补缴开始-->
	<view class="payment " wx:if="{{details.deductStatus === 2 || details.deductStatus === 10}}">
		<view class="payment-prompt">欠费有可能被列为黑名单，影响您的高速通行，请及时补缴！</view>
		<view>
			<button catchtap="go">补缴</button>
		</view>
	</view>
	<!--底部补缴结束-->
	<!--微保活动开始-->
	<view class="micro-insurance" wx:if="{{!isContinentInsurance}}">
		<image wx:if="{{showWeiBao}}" src="https://file.cyzl.com/g001/M05/3E/5C/oYYBAF6NmpSAUEh8AAAawRpDVKQ238.png"  mode="widthFix" catchtap="goMicroInsurance"></image>
		<view wx:if="{{showWeiBao}}" class="close_button iconfont" catchtap="hide"></view>
	</view>
	<!--微保活动结束-->
</view>
