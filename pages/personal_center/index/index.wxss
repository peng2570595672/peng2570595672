page {
  width: 100%;
  height: 100vh;
  overflow: hidden;
  background: #f6f6f6;
}
page .index .top {
  width: 100%;
  height: 45vh;
  background: linear-gradient(156deg, #17ad69 0%, #7fd058 100%);
}
page .index .top .back {
  display: flex;
  text-align: center;
  padding-top: 8%;
  margin-left: 20rpx;
}
page .index .top .back::before {
  content: '\e60e';
  color: #fff;
  font-size: 50rpx;
}
page .index .top .home::before {
  content: '\e65b';
  color: #33333d;
  font-size: 40rpx;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(255,255,255,0.2);
  text-align: center;
  border: 1rpx solid rgba(51,51,51,0.2);
  border-radius: 50rpx;
}
page .index .top .scan {
  display: flex;
  margin-left: 654rpx;
  padding-top: 60rpx;
}
page .index .top .scan::before {
  content: '\e63c';
  color: #fff;
  font-size: 50rpx;
}
page .index .top .head_portrait {
  position: relative;
  display: flex;
  justify-content: center;
  border-radius: 50%;
  color: rgba(255,255,255,0.4);
}
page .index .top .head_portrait .uesr_head {
  overflow: hidden;
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  text-align: center;
}
page .index .top .landing_status {
  display: flex;
  justify-content: center;
  padding-top: 10rpx;
}
page .index .top .landing_status > view {
  font-size: 36rpx;
  color: #fff;
  font-weight: 400;
  font-family: PingFangSC-Regular, PingFang SC;
}
page .index .top .vip_equity {
  display: flex;
  margin: 20rpx 0;
  justify-content: center;
}
page .index .top .vip_equity > view {
  width: 200rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(255,255,255,0.3);
  border-radius: 30rpx;
  border: 2rpx solid rgba(255,255,255,0.5);
  font-weight: 600;
  align-items: center;
  text-align: center;
  color: #fff;
}
page .index .top .vip_equity > view > text::after {
  font-size: 24rpx;
  margin-left: 14rpx;
  display: inline-block;
  content: '\e60e';
  transform: scaleX(-1);
}
page .index .iphone {
  height: 46vh;
}
page .index .small-screen {
  height: 47vh;
}
page .index .user_nav {
  display: flex;
  width: 682rpx;
  height: 200rpx;
  border-radius: 20rpx;
  margin: -100rpx 30rpx 0rpx;
  background: #fff;
}
page .index .user_nav .user_item {
  flex: 2.5;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  text-align: center;
  width: 682rpx;
  height: 200rpx;
}
page .index .user_nav .user_item .user_item_icon {
  width: 78rpx;
  height: 78rpx;
  background: #ffc75e;
  border-radius: 50rpx;
  margin: auto;
  line-height: 78rpx;
  text-align: center;
  margin-top: 40rpx;
  margin-bottom: 14rpx;
}
page .index .user_nav .user_item .user_item_icon > view {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  margin: -82rpx 0 0 64rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_etc::before {
  content: '\e64b';
  color: #fff;
  font-size: 50rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_order::before {
  content: '\e649';
  color: #fff;
  font-size: 55rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_card::before {
  content: '\e64c';
  color: #fff;
  font-size: 55rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_help::before {
  content: '\e645';
  color: #fff;
  font-size: 55rpx;
}
page .index .user_nav .user_item .user_item_text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  color: #999;
}
page .index .activity_follow {
  display: flex;
  flex-direction: row;
  margin: 30rpx 30rpx 0;
  width: 682rpx;
  height: 140rpx;
  border-radius: 20rpx;
  background: #fff;
  align-items: center;
}
page .index .activity_follow .activity_img {
  margin-left: 30rpx;
}
page .index .activity_follow .activity_img > image {
  width: 110rpx;
  height: 110rpx;
}
page .index .activity_follow .activity_msg {
  margin-left: 20rpx;
  font-size: 32rpx;
  text-align: left;
  line-height: 44rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
page .index .activity_follow .activity_msg>view:first-child {
  color: #333;
}
page .index .activity_follow .activity_msg>view:last-child {
  font-size: 24rpx;
  width: 300rpx;
  color: #999;
}
page .index .activity_follow .activity_right::after {
  margin-left: 140rpx;
  color: #999;
  display: inline-block;
  content: '\e60e';
  transform: scaleX(-1);
  font-size: 40rpx;
}
page .detail_mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.7);
}
page .detail_mask .detail_wrapper {
  position: absolute;
  left: 0;
  right: 0;
  top: 50%;
  transform: translate(0, -50%);
  width: 80%;
  min-height: 400rpx;
  margin: 0 auto;
  z-index: 2;
}
page .detail_mask .detail_wrapper > image {
  width: 100%;
}
page .detail_mask .activity_button {
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  margin: 0 auto;
  text-align: center;
  z-index: 2;
  width: 500rpx;
  height: 90rpx;
}
page .detail_mask .activity_button > button {
  width: 500rpx;
  height: 90rpx;
  border-radius: 45rpx;
  margin-top: -150rpx;
  background: url("data:image/png;base64,");
  background-size: 100%;
  padding: 0rpx;
  margin: 0;
}
page .detail_mask .close_button {
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 20rpx auto;
  text-align: center;
  border-radius: 50%;
  border: 4rpx solid #91a29a;
}
page .detail_mask .close_button::before {
  content: '\e63d';
  color: #91a29a;
  font-size: 40rpx;
}
page .title {
  margin-top: 25rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx 8rpx;
  height: 56rpx;
  font-size: 40rpx;
  font-weight: 500;
}
page .title::before {
  margin-right: 10rpx;
  content: '';
  width: 10rpx;
  height: 10rpx;
  border: 6rpx solid #2fb565;
  border-radius: 50%;
}
page .title > text {
  margin-left: 12rpx;
  font-size: 24rpx;
  color: #7a7f83;
  transform: translateY(8rpx);
}
page .crowdsourcing_msg {
  width: 682rpx;
  height: 318rpx;
  background: #fff;
  padding: 0 30rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  margin: 0 auto 20rpx;
}
page .crowdsourcing_msg > view {
  display: flex;
}
page .crowdsourcing_msg > view > view {
  flex: 1;
  font-size: 29rpx;
  color: #99999d;
  padding: 40rpx 0 20rpx;
}
page .crowdsourcing_msg > view > view:last-child {
  font-size: 40rpx;
  text-align: right;
  color: #33333d;
}
page .crowdsourcing_msg > view:nth-child(2) {
  border-top: 1rpx solid #e8e8e8;
  border-bottom: 1rpx solid #e8e8e8;
}
page .crowdsourcing_msg > view:last-child {
  padding: 36rpx 0 44rpx;
}
page .crowdsourcing_msg button {
  display: flex;
  background: none;
  font-size: 32rpx;
  color: #2fb565;
  font-weight: 500;
  height: 40rpx;
  line-height: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  justify-content: center;
  vertical-align: top;
}
page .crowdsourcing_msg button image {
  width: 40rpx;
  margin-right: 10rpx;
}
page .invite_friends {
  text-align: center;
  margin-top: 20rpx;
}
page .invite_friends image {
  width: 682rpx;
}
.pictorial {
  position: fixed;
  top: 0;
  left: -9999rpx;
  width: 593rpx;
  height: 790rpx;
  z-index: 12;
}
