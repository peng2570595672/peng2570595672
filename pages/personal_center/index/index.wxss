page {
  width: 100%;
  height: 100vh;
  background: #f6f6f6;
}
page .index .top {
  width: 100%;
  height: 504rpx;
  background-image: url("https://file.cyzl.com/g001/M07/35/57/oYYBAGCHavGADs8nAABuRTPYSgw934.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
  -moz-background-size: 100% 100%;
}
page .index .top .title_nav {
  padding-top: 8%;
  display: flex;
  align-items: center;
  justify-content: center;
}
page .index .top .title_nav .title {
  font-size: 34rpx;
  font-family: Helvetica;
  color: #000;
  line-height: 40rpx;
}
page .index .top .title_nav .back {
  position: absolute;
  left: 20rpx;
}
page .index .top .title_nav .back::before {
  content: '\e60e';
  color: #000;
  font-size: 50rpx;
}
page .index .top .title_nav .home::before {
  content: '\e65b';
  line-height: 50rpx;
  background: rgba(255,255,255,0.2);
  text-align: center;
  border: 1rpx solid rgba(51,51,51,0.2);
  border-radius: 50rpx;
}
page .index .top .head_scan {
  display: flex;
  margin: 62rpx 66rpx;
}
page .index .top .head_scan .head_portrait {
  display: flex;
  border-radius: 50%;
  color: rgba(255,255,255,0.4);
}
page .index .top .head_scan .head_portrait .uesr_head {
  overflow: hidden;
  display: block;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  text-align: center;
}
page .index .top .head_scan .landing_status {
  display: flex;
  flex-direction: column;
  flex: 1;
  justify-content: center;
  margin-left: 40rpx;
}
page .index .top .head_scan .landing_status .wx-nick {
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000;
}
page .index .top .head_scan .landing_status > view {
  font-size: 28rpx;
  color: rgba(0,0,0,0.5);
  font-weight: 400;
  font-family: PingFangSC-Regular, PingFang SC;
}
page .index .top .head_scan .scan-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
page .index .top .head_scan .scan-item > text {
  margin-top: 4rpx;
  font-size: 22rpx;
  color: rgba(51,51,61,0.7);
}
page .index .top .head_scan .scan-item .scan::before {
  content: '\e63c';
  color: #fff;
  font-size: 40rpx;
}
page .index .user_nav {
  display: flex;
  width: 686rpx;
  height: 172rpx;
  border-radius: 20rpx;
  margin: -170rpx 30rpx 0rpx;
  background: #fff;
}
page .index .user_nav .user_item {
  flex: 2.5;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  text-align: center;
  width: 682rpx;
}
page .index .user_nav .user_item .user_item_icon {
  width: 72rpx;
  height: 72rpx;
  margin: auto;
  line-height: 72rpx;
  text-align: center;
  margin-top: 40rpx;
}
page .index .user_nav .user_item .user_item_icon .img {
  height: 68rpx;
  width: 68rpx;
}
page .index .user_nav .user_item .user_item_icon .user_icon_card {
  height: 76rpx;
  width: 76rpx;
}
page .index .user_nav .user_item .user_item_icon > view {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  margin: -82rpx 0 0 64rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_order::before {
  content: '\e649';
  color: #fff;
  font-size: 55rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_card::before {
  content: '\e64c';
  color: #fff;
  font-size: 55rpx;
}
page .index .user_nav .user_item .user_item_icon.user_icon_help::before {
  content: '\e645';
  color: #fff;
  font-size: 55rpx;
}
page .index .user_nav .user_item .user_item_text {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  color: #000;
}
page .index .pay-interest {
  display: flex;
  flex-direction: column;
  background: #fff;
  box-shadow: 0rpx 0rpx 18rpx 0rpx rgba(0,0,0,0.04);
  border-radius: 16rpx;
  width: 686rpx;
  margin: 30rpx;
}
page .index .pay-interest .pay-interest-title {
  margin: 32rpx 0rpx 0rpx 32rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000;
}
page .index .pay-interest .pay-interest-content {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  margin-bottom: 32rpx;
}
page .index .pay-interest .pay-interest-content .pay-interest_item {
  width: 158rpx;
  background: transparent;
  padding: 0;
  margin-left: 8rpx;
  margin-top: 28rpx;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  text-align: center;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon {
  text-align: center;
  padding: 0;
  margin: 0;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon::before {
  font-size: 44rpx;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon.purchase_records::before {
  content: '\e688';
  color: #fba820;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon.characteristic_service::before {
  content: '\e68a';
  color: #3fb064;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon.coupon_redemption_centre::before {
  content: '\e689';
  color: #c87d2f;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon.life_service::before {
  content: '\e68c';
  color: #39a6f8;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon.car_service::before {
  content: '\e687';
  color: #6467f0;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon.basic_services::before {
  content: '\e68b';
  color: #ffc300;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon .img {
  height: 46rpx;
  width: 46rp;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon .life_service_icon {
  height: 42rpx;
  width: 42rpx;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest-item-icon .coupon_redemption_icon {
  height: 52rpx;
  width: 52rpx;
}
page .index .pay-interest .pay-interest-content .pay-interest_item .pay-interest_item_text {
  margin-top: 8rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  color: #969393;
}
page .index .activity_follow {
  display: flex;
  flex-direction: row;
  margin: 30rpx 30rpx 0;
  width: 686rpx;
  height: 140rpx;
  border-radius: 20rpx;
  background: #fff;
  align-items: center;
}
page .index .activity_follow .activity_img {
  margin-left: 30rpx;
}
page .index .activity_follow .activity_img::before {
  content: '\e686';
  color: #fa5151;
  font-size: 32rpx;
}
page .index .activity_follow .activity_img.help-center::before {
  content: '\e68f';
  color: #fac451;
}
page .index .activity_follow .activity_msg {
  flex: 1;
  margin-left: 20rpx;
  font-size: 34rpx;
  text-align: left;
  line-height: 44rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  color: #000;
}
page .index .activity_follow .activity_right {
  color: #576b95;
  font-size: 24rpx;
  margin-right: 20rpx;
}
page .detail_mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.7);
}
page .detail_mask .detail_wrapper {
  display: flex;
  flex-direction: column;
  position: absolute;
  left: 56rpx;
  right: 56rpx;
  top: 50%;
  transform: translate(0, -50%);
  background: #fff;
  border-radius: 16px;
  z-index: 2;
}
page .detail_mask .detail_wrapper .public-account-title {
  margin: 64rpx 48rpx 32rpx 48rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000;
  line-height: 48rpx;
  text-align: center;
}
page .detail_mask .detail_wrapper > image {
  margin-left: 32rpx;
  margin-right: 32rpx;
  width: 574rpx;
  height: 306rpx;
}
page .detail_mask .detail_wrapper .public-account-tips {
  text-align: center;
  margin-top: 32rpx;
  padding-bottom: 64rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(0,0,0,0.5);
  line-height: 48rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
page .detail_mask .detail_wrapper .activity_button {
  text-align: center;
}
page .detail_mask .detail_wrapper .activity_button > button {
  text-align: center;
  background-color: transparent;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #576b95;
  line-height: 48rpx;
  margin: 30rpx;
}
page .crowdsourcing_msg {
  width: 682rpx;
  background: #fff;
  padding: 30rpx 30rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  margin: 30rpx;
}
page .crowdsourcing_msg .title {
  display: flex;
  align-items: center;
  color: #000;
  font-size: 34rpx;
  font-weight: 400;
}
page .crowdsourcing_msg > view {
  display: flex;
}
page .crowdsourcing_msg > view > view {
  flex: 1;
  font-size: 29rpx;
  color: rgba(0,0,0,0.5);
  padding: 40rpx 0 20rpx;
}
page .crowdsourcing_msg > view > view:last-child {
  font-size: 40rpx;
  text-align: right;
  color: #33333d;
}
page .crowdsourcing_msg > view:nth-child(3) {
  border-top: 1rpx solid #e8e8e8;
  border-bottom: 1rpx solid #e8e8e8;
}
page .crowdsourcing_msg > view:last-child {
  padding: 36rpx 0 44rpx;
}
page .crowdsourcing_msg button {
  display: flex;
  background: none;
  font-size: 32rpx;
  color: #2fb565;
  font-weight: 500;
  height: 40rpx;
  line-height: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  justify-content: center;
  vertical-align: top;
}
page .crowdsourcing_msg button image {
  width: 40rpx;
  margin-right: 10rpx;
}
page .invite_friends {
  text-align: center;
  margin-top: 20rpx;
}
page .invite_friends image {
  width: 682rpx;
}
.pictorial {
  position: fixed;
  top: 0;
  left: -9999rpx;
  width: 593rpx;
  height: 790rpx;
  z-index: 12;
}
