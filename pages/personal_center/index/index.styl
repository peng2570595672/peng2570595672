@import "../../../style/base.styl";
//渐变主题色
$themeColor1 = #17AD69;
$themeColor2 = #7FD058;
page
	width 100%
	height 100vh
	overflow hidden
	background rgba(246,246,246,1);
	.index
		//顶部
		.top
			width 100%
			height 592rpx
			background: linear-gradient(156deg,$themeColor1 0%,$themeColor2 100%)
			.back
				display flex
				text-align center
				padding-top 8%
				margin-left 20rpx
				&::before
					content '\e60e'
					color rgba(255,255,255,1)
					font-size 50rpx
			.scan
				display flex
				margin-left 654rpx
				padding-top 60rpx
				&::before
					content '\e63c'
					color rgba(255,255,255,1)
					font-size 50rpx
			//头像
			.head_portrait
				position relative
				display flex
				justify-content center
				border-radius 50%
				color rgba(255,255,255,0.4)
				.uesr_head
					overflow hidden
					display: block;
					width: 120rpx;
					height: 120rpx;
					border-radius: 50%;
					text-align center
					//border: 2px solid #fff;
					//box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
			//登录状态
			.landing_status
				display flex
				justify-content center
				& > view
					font-size 36rpx
					color rgba(255,255,255,1)
					font-weight 400
					font-family PingFangSC-Regular,PingFang SC
			//会员权益
			.vip_equity
				display flex
				margin 20rpx 0
				justify-content center
				& > view
					width 200rpx
					height 50rpx
					line-height 50rpx
					background rgba(255,255,255,0.3)
					border-radius 30rpx
					border 2rpx solid rgba(255,255,255,0.5)
					font-weight 600
					align-items center
					text-align center
					color #ffffff
					& > text
						&::after
							font-size 24rpx
							margin-left 16rpx
							display inline-block
							content '\e60e'
							transform scaleX(-1)
		//用户导航栏
		.user_nav
			display flex
			width 682rpx
			height 200rpx
			border-radius 20rpx
			margin -100rpx 30rpx
			background rgba(255,255,255,1)
			.user_item
				flex 2.5
				justify-content center
				align-items center
				flex-direction column
				text-align center
				width 682rpx
				height 200rpx
				.user_item_icon
					width 78rpx
					height 78rpx
					background rgba(255,199,94,1)
					border-radius 50rpx
					margin auto
					line-height 78rpx
					text-align center
					margin-top 40rpx
					margin-bottom 14rpx
					& > view
						width 20rpx
						height 20rpx
						//background rgba(255,0,0,1)   消息通知红点
						border-radius 50%
						margin -82rpx 0 0 64rpx
					&.user_icon_etc
						&::before
							content '\e64b'
							color rgba(255,255,255,1)
							font-size 50rpx
					&.user_icon_order
						&::before
							content '\e649'
							color rgba(255,255,255,1)
							font-size 55rpx
					&.user_icon_card
						&::before
							content '\e64c'
							color rgba(255,255,255,1)
							font-size 55rpx
					&.user_icon_help
						&::before
							content '\e645'
							color rgba(255,255,255,1)
							font-size 55rpx
				.user_item_text
					font-size 24rpx
					font-family PingFangSC-Regular,PingFang SC
					color rgba(153,153,153,1)
		//关注活动
		.activity_follow
			display flex
			flex-direction row
			margin 130rpx 30rpx
			width 682rpx
			height 140rpx
			border-radius 20rpx
			background white
			align-items center
			.activity_img
				margin-left 30rpx
				& > image
					width 110rpx
					height 110rpx
			.activity_msg
				margin-left 20rpx
				font-size 32rpx
				text-align left
				line-height 44rpx
				font-family PingFangSC-Regular,PingFang SC
				&>view:first-child
					color rgba(51,51,51,1)
				&>view:last-child
					font-size 24rpx
					width 300rpx
					color rgba(153,153,153,1)
			.activity_right
				&::after
					margin-left 140rpx
					color rgba(153,153,153,1)
					display inline-block
					content '\e60e'
					transform scaleX(-1)
					font-size 40rpx
		//关注活动弹框
	.detail_mask
		position fixed
		left 0
		top 0
		right 0
		bottom 0
		background rgba(0, 0, 0, .7)
		.detail_wrapper
			position absolute
			left 0
			right 0
			top 50%
			transform translate(0, -50%)
			width 80%
			min-height 400rpx
			margin 0 auto
			z-index 2
			& > image
				width 100%
		.activity_button
			position absolute
			top 50%
			left 0
			right 0
			margin 0 auto
			text-align center
			z-index 2
			width 500rpx
			height 90rpx
			& > button
				width 500rpx
				height 90rpx
				border-radius 45rpx
				margin-top -150rpx
				background url("data:image/png;base64,")
				background-size 100%
				padding 0rpx
				margin 0
		.close_button
			position absolute
			left 615rpx
			right 0
			margin-top -905rpx
			z-index 2
			&::before
				content '\e63d'
				color #ffffff
				font-size 18rpx


