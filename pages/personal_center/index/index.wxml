<view class="index">
    <!--顶部开始-->
    <view class="top">
        <!--返回按钮-->
        <view class="head_scan">
            <!--头像-->
            <view class="head_portrait">
                <view>
                    <open-data class="uesr_head" type="userAvatarUrl"></open-data>
                </view>
            </view>
            <!--登陆状态-->
            <view class="landing_status">
                <view class="wx-nick"><open-data type="userNickName" lang="zh_CN"></open-data></view>
                <view>{{mobilePhone}}</view>
            </view>
            <!--扫一扫-->
            <view class="scan-item" catchtap="scan">
                <view class="scan iconfont"></view>
                <text>扫一扫</text>
            </view>
        </view>
    </view>
    <!--顶部结束-->
    <view class="notice" wx:if="{{isShowNotice}}" catchtap="onClickNotice">
        <view class="iconfont">
            <view class="notice-left iconfont">{{isActivityDate ? '福利！60元通行券限时【19.9】抢！' : '限购！通行券、停车券 40抵60'}}</view>
            <view class="notice-right {{isClickNotice ? 'no-notice-bg' : ''}}"></view>
        </view>
    </view>
    <!--用户导航栏开始-->
    <view class="user_nav {{isShowNotice ? 'user-nav-top' : ''}}">
        <!--我的ETC开始-->
        <view class="user_item" catchtap="go" data-url="my_etc">
            <view class="user_item_icon iconfont">
                <!--更新提示-->
                <image class="img" src="/pages/personal_center/assets/my_etc.svg"></image>
                <view></view>
            </view>
            <view class="user_item_text">我的ETC</view>
        </view>
        <!--我的ETC结束-->
        <!--ETC账单开始-->
        <view class="user_item" catchtap="go" data-url="my_order">
            <view class="user_item_icon">
                <image class="img" src="/pages/personal_center/assets/my_bill.svg"></image>
                <!--更新提示-->
                <view></view>
            </view>
            <view class="user_item_text">ETC账单</view>
        </view>
        <!--ETC账单结束-->
        <!--我的卡劵开始-->
        <view class="user_item" catchtap="go" data-url="service_card_voucher">
            <view class="user_item_icon">
                <image class="user_icon_card" src="/pages/personal_center/assets/coupons.svg"></image>
                <!--更新提示-->
                <view></view>
            </view>
            <view class="user_item_text iconfont">优惠券</view>
        </view>
        <!--我的卡劵结束-->
        <!--帮助中心开始 isPrechargeOrder-->
        <view class="user_item" catchtap="onClickAccountManagement" wx:if="{{isPrechargeOrder}}">
            <view class="user_item_icon">
                <image class="user_icon_card" src="/pages/personal_center/assets/account_management.svg"></image>
                <!--更新提示-->
                <view></view>
            </view>
            <view class="user_item_text">账户管理</view>
        </view>
        <!--帮助中心结束-->
        <!--帮助中心开始-->
        <view class="user_item" catchtap="go" data-url="help_center" wx:else>
            <view class="user_item_icon {{isShowHelpCenterUpdate ? 'update' : ''}}">
                <image class="img" src="/pages/personal_center/assets/help.svg" ></image>
                <!--更新提示-->
                <view></view>
            </view>
            <view class="user_item_text">帮助中心</view>
        </view>
        <!--帮助中心结束-->
    </view>
	<!-- 权益商城即将上线 banner 开始-->
    <view class="equity-mall" wx:if="{{isShowEquityImg}}" catchtap="handleMall">
        <image class="equity-mall-img" src="https://file.cyzl.com/g001/M00/B1/AF/oYYBAGO3jnCAfwjpAAAqIyc6i1w777.png" model="widthFix"></image>
    </view>
	<!-- 权益商城即将上线 banner 结束-->
    <!--付费权益开始-->
    <view class="pay-interest" wx:if="{{rightsPackageBuyRecords.length || isActivation || hasCoupon || isShowCoupon || isShowFeatureService }}">
        <view class="pay-interest-title">ETC服务</view>
        <view class="pay-interest-content">
            <view class="pay-interest_item" catchtap="go" data-url="basic_services" wx:if="{{isActivation}}">
                <view class="pay-interest-item-icon basic_services iconfont"></view>
                <view class="pay-interest_item_text">基础服务</view>
            </view>
            <view class="pay-interest_item" catchtap="go" data-url="the_owner_service" wx:if="{{isActivation}}">
                <view class="pay-interest-item-icon car_service iconfont"></view>
                <view class="pay-interest_item_text">车主服务</view>
            </view>
            <!-- <view class="pay-interest_item" catchtap="go" data-url="life_service" wx:if="{{rightsPackageBuyRecords.length}}">
                <view class="pay-interest-item-icon life_service iconfont"></view>
                <view class="pay-interest_item_text">生活服务</view>
            </view> -->
            <view class="pay-interest_item" catchtap="go" data-url="coupon_redemption_centre" wx:if="{{hasCoupon}}">
                <view class="pay-interest-item-icon coupon_redemption_centre iconfont"></view>
                <view class="pay-interest_item_text">领券中心</view>
            </view>
<!--            <view class="pay-interest_item" catchtap="go" data-url="characteristic_service" wx:if="{{isShowFeatureService}}">-->
<!--                <view class="pay-interest-item-icon characteristic_service iconfont"></view>-->
<!--                <view class="pay-interest_item_text">特色服务</view>-->
<!--            </view>-->
            <view class="pay-interest_item" catchtap="go" data-url="online_customer_service" wx:if="{{isShowFeatureService}}">
                <view class="pay-interest-item-icon server iconfont"></view>
                <view class="pay-interest_item_text">在线客服</view>
            </view>
            <view class="pay-interest_item" catchtap="go" data-url="service_purchase_record" wx:if="{{rightsPackageBuyRecords.length}}">
                <view class="pay-interest-item-icon purchase_records iconfont"></view>
                <view class="pay-interest_item_text">加购记录</view>
            </view>
            <view class="coupon_item pay-interest_item" catchtap="handleCouponMini" wx:if="{{isShowCoupon}}">
                <image src="https://file.cyzl.com/g001/M07/8A/24/oYYBAGK4SDSABl7lAAADWJ2H5X4108.png" mode="widthFix"></image>
                <view class="pay-interest_item_text">通通券权益</view>
            </view>
        </view>
    </view>
    <!--付费权益结束-->
    <!--关注公众号活动开始-->
    <button class="activity_follow" wx:if="{{userInfo && isAttention !== 1}}" catchtap="showDetail">
        <!--活动logo展示-->
        <view class="activity_img iconfont"></view>
        <!--活动文字描述开始-->
        <view class="activity_msg">关注公众号</view>
        <!--活动文字描述结束-->
        <view class="activity_right">去关注</view>
    </button>

    <!--关注公众号活动开始-->
    <button class="activity_follow" wx:if="{{!userInfo && isAttention !== 1 && canIUseGetUserProfile}}" bindtap="getUserProfile">
        <!--活动logo展示-->
        <view class="activity_img iconfont"></view>
        <!--活动文字描述开始-->
        <view class="activity_msg">关注公众号</view>
        <!--活动文字描述结束-->
        <view class="activity_right">去关注</view>
    </button>
    <button class="activity_follow" wx:if="{{!userInfo && isAttention !== 1 && !canIUseGetUserProfile}}" open-type="getUserInfo" bindgetuserinfo="bindGetUserInfo">
        <!--活动logo展示-->
        <view class="activity_img iconfont"></view>
        <!--活动文字描述开始-->
        <view class="activity_msg">关注公众号</view>
        <!--活动文字描述结束-->
        <view class="activity_right">去关注</view>
    </button>

    <button class="activity_follow" wx:if="{{isPrechargeOrder}}" catchtap="go" data-url="help_center">
        <view class="activity_img help-center iconfont"></view>
        <view class="activity_msg {{isShowHelpCenterUpdate ? 'update' : ''}}">帮助中心</view>
    </button>
    <!--	成为众包推广者开始-->
    <view class="{{isAttention === 1 ? '' : ''}}" wx:if="{{crowdSourcingMsg.status === 1}}">

        <!--	众包推广信息开始-->
        <view class="crowdsourcing_msg">
            <!--	我的推广标题开始-->
            <view class="title">我的推广</view>
            <!--	我的推广标题结束-->
            <view catchtap="go" data-url="my_crowdsourcing_gains">
                <view>我的收益</view>
                <view>{{crowdSourcingMsg.redParkCount ? crowdSourcingMsg.redParkCount : 0}}元</view>
            </view>
            <view catchtap="go" data-url="crowdsourcing_user">
                <view>我的推广</view>
                <view>{{crowdSourcingMsg.orderCount ? crowdSourcingMsg.orderCount : 0}}单</view>
            </view>
            <view><button open-type="getUserInfo" bindgetuserinfo="getUserInfo"><image mode="widthFix" src="/pages/personal_center/assets/code.png"></image>邀请办理</button></view>
        </view>
        <!--	众包推广信息结束-->
    </view>
    <view class="auth" bindtap="handleAuth">授权管理</view>
    <!--	成为众包推广者结束开始-->
    <!--	&lt;!&ndash;	未成为众包推广者开始&ndash;&gt;-->
    <!--	<view wx:if="{{crowdSourcingMsg.status === 0}}">-->
    <!--		&lt;!&ndash;	众包推广信息开始&ndash;&gt;-->
    <!--		<view class="invite_friends" catchtap="inviteFriends">-->
    <!--			<image src="/pages/personal_center/assets/invite_friends.png" mode="widthFix"></image>-->
    <!--		</view>-->
    <!--		&lt;!&ndash;	众包推广信息结束&ndash;&gt;-->
    <!--	</view>-->
    <!--	&lt;!&ndash;	未成为众包推广者结束开始&ndash;&gt;-->
    <!--关注公众号活动弹框-->
    <view catchtap="hide" class="detail_mask animated {{showDetailWrapper ? 'fadeIn' : 'fadeOut'}}" wx:if="{{showDetailWrapper}}">
        <view catchtap="close" class="detail_wrapper animated {{showDetailWrapper ? 'zoomInOther' : 'zoomOutOther'}}">
            <view class="public-account-title">关注ETC+公众号</view>
            <image src="/pages/personal_center/assets/{{showPublicAccountType === 1 ? 'privilege' : 'public_account'}}.png"></image>
            <view class="public-account-tips">{{showPublicAccountType === 1 ? '享视频/音频/美食充值优惠' : '最新路况新闻秒GET'}}</view>
            <view class="activity_button">
                <button open-type="contact" catchtap="goPublicAccount">发送“9”关注公众号</button>
            </view>
        </view>
    </view>
    <!--关注公众号活动结束-->
    <crowdsourcing-prompt id="crowdsourcingPrompt" bind:onHandle="onHandle" bind:cancelHandle="cancelHandle"></crowdsourcing-prompt>
</view>
<!--勾选用户协议弹框-->
<view class="agreement-mask animated {{showAgreementWrapper && isNeedShowAgreement ? 'fadeIn' : 'fadeOut'}}" wx:if="{{showAgreementWrapper && isNeedShowAgreement}}">
    <view class="agreement-wrapper animated {{showAgreementWrapper && isNeedShowAgreement ? 'zoomInOther' : 'zoomOutOther'}}">
        <view class="alert-title">协议更新</view>
        <view class="alert-content">尊敬的用户，您好！</view>
        <view class="alert-content">为提供更好的服务，我们更新了用户协议，请您务必仔细阅读最新的<text catchtap="onCLickGoAgreement">《ETC用户协议》</text>。如有疑问，可联系在线客服咨询。</view>
        <view class="alert-agreement {{isAgreement ? 'active' : ''}}" catchtap="onClickChangeAgreement">我已阅读并同意以上协议</view>
        <view class="alert-btn {{isAgreement ? 'active' : ''}}" catchtap="onClickSubmit">确认</view>
    </view>
</view>
<!--勾选用户协议结束-->
<canvas canvas-id="pictorial" class="pictorial"  disable-scroll="true"></canvas>
<popup1 id='dialog1' dialogContent="{{disclaimerDesc}}" bind:onHandle1="popUp"></popup1>
