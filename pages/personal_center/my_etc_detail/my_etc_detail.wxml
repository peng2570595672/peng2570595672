<view class="my-etc-detail animated fadeIn" wx:if="{{orderInfo}}">
	<!--	设备图开始-->
	<image src="/pages/personal_center/assets/etc_device.png"/>
	<!--	设备图结束-->
	<!--	分割线开始-->
	<view class="border"></view>
	<!--	分割线结束-->
	<!--	信息开始-->
	<view class="item">
		<view>车牌号码</view>
		<view>{{orderInfo.vehPlates}}</view>
	</view>
	<view class="item" wx:if="{{orderInfo.receiveMan}}">
		<view>车辆所属人</view>
		<view>{{orderInfo.receiveMan}}</view>
	</view>
	<view class="item">
		<view>订单状态</view>
		<view class="caution" wx:if="{{orderInfo.selfStatus === 1 || orderInfo.selfStatus === 3 && orderInfo.contractStatus !== 2}}">办理中</view>
		<view class="caution" wx:if="{{orderInfo.selfStatus === 2 || orderInfo.contractStatus === 2}}">待签约</view>
		<view class="caution" wx:elif="{{orderInfo.selfStatus === 4 || orderInfo.selfStatus === 7 && orderInfo.contractStatus !== 2}}">审核中</view>
		<view class="caution" wx:elif="{{orderInfo.selfStatus === 6 && orderInfo.contractStatus !== 2}}">待激活</view>
		<view class="failed" wx:elif="{{orderInfo.selfStatus === 5 || orderInfo.selfStatus === 8 && orderInfo.contractStatus !== 2}}">审核失败</view>
		<view wx:elif="{{orderInfo.selfStatus === 9 && orderInfo.contractStatus !== 2}}" class="succeed">正常使用</view>
	</view>
	<view class="des" wx:if="{{orderInfo.selfStatus === 5 || orderInfo.selfStatus === 8 && orderInfo.contractStatus !== 2}}">{{orderInfo.auditRemark}}</view>
	<view class="item {{orderInfo.contractVersion === 'v3' ? 'iconfont' : ''}}">
		<view>扣费方式</view>
		<!--应产品(李小芳)要求,微信套餐,隐藏套餐名   productProcess === 1  微信支付  针对部分套餐名称错误的做处理-->
		<view catchtap="onClickBank">{{orderInfo.productName ? (orderInfo.productProcess === 1 ? '微信支付' : orderInfo.productName) : ''}}</view>
	</view>
	<view class="split"></view>
	<view class="item first" wx:if="{{orderInfo.shopUserTrueName}}">
		<view>业务员姓名</view>
		<view>{{orderInfo.shopUserTrueName}}</view>
	</view>
	<view class="item first" wx:if="{{orderInfo.shopUserTrueName}}">
		<view>业务员手机</view>
		<view>{{orderInfo.shopUserMobilePhone}}</view>
	</view>
	<view class="item" wx:if="{{orderInfo.cardName}}">
		<view>发行卡种</view>
		<view>{{orderInfo.cardName}}</view>
	</view>
	<view class="item" wx:if="{{orderInfo.etcNo}}">
		<view>发行卡号</view>
		<view>{{orderInfo.etcNo}}</view>
	</view>
	<view class="item" wx:if="{{orderInfo.obuNo}}">
		<view>设备（OBU）编号</view>
		<view>{{orderInfo.obuNo}}</view>
	</view>
	<view class="item">
		<view>办理日期</view>
		<view>{{orderInfo.addTime}}</view>
	</view>
	<!--	信息结束-->
	<view class="auto-fill"></view>
	<!--	按钮开始-->
	<view class="btns">
		<button class="btn" wx:if="{{orderInfo.selfStatus === 1 || orderInfo.selfStatus === 3 && orderInfo.contractStatus !== 2}}" catchtap="onClickContinueHandle">继续办理</button>
		<button class="btn" wx:elif="{{orderInfo.selfStatus === 2 || orderInfo.contractStatus === 2}}" catchtap="onClickBackToSign">{{orderInfo.contractStatus === 2? '恢复签约' : '立即签约'}}</button>
		<button class="btn" wx:elif="{{(orderInfo.selfStatus === 4 || orderInfo.selfStatus === 7) && orderInfo.contractStatus !== 2}}" catchtap="onClickViewProcessingProgressHandle">查看进度</button>
		<!--<button class="btn" wx:elif="{{(orderInfo.selfStatus === 4 || orderInfo.selfStatus === 7) && orderInfo.contractStatus !== 2 && orderInfo.orderType !== 11}}" catchtap="onClickCctivate">确认收货并激活</button>-->
		<button class="btn" wx:elif="{{orderInfo.selfStatus === 5 || orderInfo.selfStatus === 8 && orderInfo.contractStatus !== 2}}"  catchtap="onClickModifiedData">修改资料</button>
		<button class="btn" wx:elif="{{orderInfo.selfStatus === 6 && orderInfo.contractStatus !== 2 && orderInfo.logisticsId === 0}}" catchtap="onClickViewProcessingProgressHandle">查看进度</button>
		<button class="btn" wx:elif="{{orderInfo.selfStatus === 6 && orderInfo.contractStatus !== 2 && orderInfo.logisticsId !== 0}}" catchtap="onClickCctivate">确认收货并激活</button>
		<view class="btn-wrapper" wx:if="{{orderInfo.selfStatus !== 4 && orderInfo.selfStatus !== 6 && orderInfo.selfStatus !== 7}}">
			<view catchtap="onClickCancelHandle" wx:if="{{orderInfo.selfStatus !== 4 && orderInfo.selfStatus !== 6 && orderInfo.selfStatus !== 7 && orderInfo.selfStatus !== 9 && orderInfo.contractStatus !== 2 && orderInfo.orderType !== 51}}">取消办理</view>
			<view catchtap="onClickCancelHandle" wx:if="{{orderInfo.contractStatus === 2 && orderInfo.status === 0}}">取消办理</view>
			<!--<view catchtap="onClickCancelHandle">取消办理</view>-->
			<view catchtap="goOnlineServer">客服中心</view>
		</view>
	</view>
	<!--	按钮结束-->
	<!--去车主服务弹窗-->
	<view catchtap="hide" class="owner-service-mask animated {{showDetailWrapper ? 'fadeIn' : 'fadeOut'}}" wx:if="{{showDetailMask}}">
		<view catchtap="close" class="owner-service-wrapper animated {{showDetailWrapper ? 'zoomInOther' : 'zoomOutOther'}}">
			<view class="owner-service-box">
				<view>您即将前往车主服务。</view>
				<view>注意:请勿暂停车主服务，</view>
				<view>扣费失败您的ETC可能被拉入黑名单</view>
				<view>影响高速通行</view>
				<view class="owner-service-button">
					<text catchtap="hide">取消</text>
					<text catchtap="onClickSwitchBank">继续前往</text>
				</view>
			</view>
		</view>
	</view>
	<!--去车主服务弹窗结束-->
</view>
