<view class="my-etc-detail animated fadeIn" wx:if="{{orderInfo}}">
	<!--	设备图开始-->
	<image src="/pages/personal_center/assets/etc_device.png"/>
	<!--	设备图结束-->
	<!--	分割线开始-->
	<view class="border"></view>
	<!--	分割线结束-->
	<!--	信息开始-->
	<view class="item">
		<view>车牌号码</view>
		<view>{{orderInfo.vehPlates}}</view>
	</view>
	<view class="item">
		<view>订单状态</view>
		<view class="caution" wx:if="{{orderInfo.selfStatus === 1 || orderInfo.selfStatus === 3 && orderInfo.contractStatus !== 2}}">办理中</view>
		<view class="caution" wx:if="{{orderInfo.selfStatus === 2 || orderInfo.contractStatus === 2}}">待签约</view>
		<view class="caution" wx:elif="{{orderInfo.selfStatus === 4 || orderInfo.selfStatus === 7 && orderInfo.contractStatus !== 2}}">审核中</view>
		<view class="caution" wx:elif="{{orderInfo.selfStatus === 6 && orderInfo.contractStatus !== 2}}">待激活</view>
		<view class="failed" wx:elif="{{orderInfo.selfStatus === 5 || orderInfo.selfStatus === 8 && orderInfo.contractStatus !== 2}}">申请失败</view>
		<view wx:elif="{{orderInfo.selfStatus === 9 && orderInfo.contractStatus !== 2}}" class="succeed">审核通过</view>
	</view>
	<view class="des" wx:if="{{orderInfo.selfStatus === 5 || orderInfo.selfStatus === 8 && orderInfo.contractStatus !== 2}}">{{orderInfo.auditRemark}}</view>
	<view class="item">
		<view>扣费方式</view>
		<view>{{orderInfo.productName}}</view>
	</view>
	<view class="split"></view>
	<view class="item first">
		<view>所属人</view>
		<view>{{orderInfo.receiveMan}}</view>
	</view>
	<view class="item">
		<view>发行卡种</view>
		<view>{{orderInfo.cardName}}</view>
	</view>
	<view class="item" wx:if="{{orderInfo.etcNo}}">
		<view>发行卡号</view>
		<view>{{orderInfo.etcNo}}</view>
	</view>
	<view class="item" wx:if="{{orderInfo.obuNo}}">
		<view>设备（OBU）编号</view>
		<view>{{orderInfo.obuNo}}</view>
	</view>
	<!--	分割线开始-->
	<view class="split"></view>
	<!--	分割线结束-->
	<view class="item first">
		<view>办理日期</view>
		<view>{{orderInfo.addTime}}</view>
	</view>
	<!--	信息结束-->
	<view class="auto-fill"></view>
	<!--	按钮开始-->
	<view class="btns">
		<view class="btn" wx:if="{{orderInfo.selfStatus === 1 || orderInfo.selfStatus === 3 && orderInfo.contractStatus !== 2}}" catchtap="onClickContinueHandle">继续办理</view>
		<view class="btn" wx:elif="{{orderInfo.selfStatus === 2 || orderInfo.contractStatus === 2}}" catchtap="onClickBackToSign">{{orderInfo.contractStatus === 2 ? '恢复签约' : '立即签约'}}</view>
		<view class="btn" wx:elif="{{orderInfo.selfStatus === 4 || orderInfo.selfStatus === 7 && orderInfo.contractStatus !== 2}}" catchtap="onClickViewProcessingProgressHandle">查看进度</view>
		<view class="btn" wx:elif="{{orderInfo.selfStatus === 5 || orderInfo.selfStatus === 8 && orderInfo.contractStatus !== 2}}"  catchtap="onClickModifiedData">修改资料</view>
		<view class="btn" wx:elif="{{orderInfo.selfStatus === 6 && orderInfo.contractStatus !== 2}}"  catchtap="onClickCctivate">去激活</view>
		<view class="btn-wrapper">
			<view catchtap="onClickCancelHandle">取消办理</view>
			<view catchtap="goOnlineServer">客服中心</view>
		</view>
	</view>
	<!--	按钮结束-->
</view>
