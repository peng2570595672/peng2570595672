<wxs src='../../../utils/util.wxs' module='tools' />
<view class="index">
    <!-- 订单模块 -->
    <view wx:if="{{isShow}}">
        <view class="module moduleOne" wx:key="index" wx:for="{{roadRescueList}}" wx:key="index">
            <view class="upper">
                <view class="attrVal">
                    <view class="attr">{{item.title}}</view>
                    <view class="status iconfont" data-status="{{item.status}}" catchtap="btnLoad">{{item.status}}</view>
                </view>
                <view class="policyInfo">
                    <view class="label">保单编号</view>
                    <view class="value">{{item.policyNumber}}</view>
                </view>
            </view>
            <view class="below">
                <view class="coverageItem" wx:for="{{item.coverages}}" wx:key="key">
                    <view class="label">{{item.label}}</view>
                    <view class="value">{{item.value}}</view>
                </view>
            </view>
        </view>
    </view>
    <view class="iconfont empty" wx:else>当前暂无增值服务</view>
    <!-- 占位 -->
    <view class="split" />
</view>
<privacy_popup></privacy_popup>