@font-face {
  font-family: 'iconfont' /* Project id 1514406 */;
  src: url("//at.alicdn.com/t/font_1514406_r2ex32p9up.woff2?t=1651741878795") format('woff2'), url("//at.alicdn.com/t/font_1514406_r2ex32p9up.woff?t=1651741878795") format('woff'), url("//at.alicdn.com/t/font_1514406_r2ex32p9up.ttf?t=1651741878795") format('truetype');
}
.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.index {
  width: 100vw;
  min-height: 100vh;
  background-color: #ececec;
}
.index::before {
  content: '';
  display: block;
  width: 100%;
  height: 24rpx;
}
.index .module {
  margin: 30rpx 25rpx;
  padding: 20rpx 30rpx;
  border-radius: 16rpx;
  box-shadow: 4rpx 4rpx 46rpx 0rpx rgba(125,132,158,0.1);
}
.index .moduleOne {
  background-color: #fff;
  border-radius: 16rpx;
  font-family: PingFangSC-Medium, PingFang SC;
}
.index .moduleOne .upper {
  border-bottom: 1rpx solid #efefef;
  margin: 5rpx 0rpx;
}
.index .moduleOne .upper .attrVal {
  width: 100%;
  height: 56rpx;
  line-height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.index .moduleOne .upper .attrVal .attr {
  font-size: 36rpx;
  font-weight: 600;
  color: #1e1e1e;
  line-height: 56rpx;
}
.index .moduleOne .upper .attrVal .status {
  position: absolute;
  bottom: 0;
  right: 0;
  font-size: 26rpx;
  font-weight: normal;
  color: #5ebe68;
  line-height: 26rpx;
}
.index .moduleOne .upper .attrVal .status::after {
  display: inline-block;
  content: "\e661";
  color: #00f;
  font-size: 26rpx;
  color: #5ebe68;
  transform: rotateZ(-90deg);
}
.index .moduleOne .upper .policyInfo {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.index .moduleOne .below .coverageItem {
  margin: 10rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  line-height: 60rpx;
  padding-right: 10rpx;
}
.index .moduleOne .below .coverageItem .label {
  color: #666;
}
.index .moduleOne .below .coverageItem .value {
  color: #000;
}
.index .empty {
  font-size: 28rpx;
  font-weight: 400;
  color: #99999d;
  margin-top: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  font-family: PingFangSC-Medium, PingFang SC;
}
.index .empty::before {
  display: block;
  content: "\e649";
  font-size: 120rpx;
  margin-bottom: 30rpx;
  color: #99999d;
}
.index .image {
  width: 100%;
  height: 100%;
}
.index .split {
  width: 100%;
  height: 108rpx;
}
