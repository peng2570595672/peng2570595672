    .module3
        margin-top 40rpx
        .subMod3
            margin-top 20rpx
            mixinsWidthHeight(,400rpx)
            background #FFFFFF
            box-shadow 4rpx 4rpx 46rpx 0rpx rgba(125,132,158,0.1)
            border-radius 16rpx
            .pub1
                height 90rpx
                margin 0rpx 30rpx
                font-size 28rpx
                font-weight 400
                flexModule(center,normal)
                border-bottom 2rpx solid rgba(224, 224, 224, 0.5)
