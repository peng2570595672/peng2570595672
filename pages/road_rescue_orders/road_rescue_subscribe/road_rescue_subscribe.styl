@font-face
    font-family 'iconfont' /* Project id 1514406 */
    src url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff2?t=1651741878795') format('woff2'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff?t=1651741878795') format('woff'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.ttf?t=1651741878795') format('truetype')
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
mixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    height $height
maxMixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    min-height $height
mixinsIconfont($content='',$size=16px,$color=#5EBE68)
    content $content
    font-size $size
    color $color
mixinsPositionPack($position=relative)
    position $position
    left 50%
    transform translateX(-50%)
textLineCenter($height)
    text-align center
    line-height $height
flexModule($align=normal,$justify=normal)
    display flex
    align-items $align
    justify-content $justify
.index
    maxMixinsWidthHeight(100vw,100vh)
    position relative
    &::before
        z-index -1
        position absolute
        content ''
        display block
        mixinsWidthHeight(750rpx,878rpx)
        background linear-gradient(2deg, rgba(255,249,236,0) 0%, #FFE8CC 100%)
    .split1
        height 216rpx
    .module
        margin 20rpx 35rpx 0rpx 35rpx
        border-radius 16rpx
        .name
            height 56rpx
            flexModule(center,normal)
            .square
                mixinsWidthHeight(8rpx,44rpx)
                background: #00B85C
                margin-right 10rpx
            .title
                font-size 32rpx
                font-family PingFangSC-Medium, PingFang SC
                font-weight 500
    .module1
        font-family PingFangSC-Medium, PingFang SC
        .name
            .licensePlate
                font-size 40rpx
                font-weight 500
            .text1
                margin-left 24rpx
                font-size 20rpx
                font-weight 400
                color rgba(30,30,30,0.5)
        .subName
            margin 8rpx 0 0 18rpx
            height 56rpx
            font-size 34rpx
            font-weight 500
            .text2
                color rgba(241, 156, 56, 1)
    .module2
        margin-top 56rpx
        .subMod1
            margin-top 18rpx
            mixinsWidthHeight(,100rpx)
            border-radius 16rpx
            background #FFFFFF
            box-shadow 4rpx 4rpx 46rpx 0rpx rgba(125,132,158,0.1)
            flexModule(center,normal)
            font-family PingFangSC-Regular, PingFang SC
            font-size 28rpx
            font-weight 400
            color #1E1E1E
            .text3
                width 112rpx
                margin-left 30rpx
            .dateTime
                margin-left 60rpx
                width 448rpx
                flexModule(center,space-between)
                .text4
                    color #6F6F6F
                &::after
                    display inline-block
                    content "\e661"
                    font-size 30rpx
                    color rgba(0, 0, 0, 0.5)
                    transform rotateZ(-90deg)
            
    .module3
        margin-top 40rpx
        