@font-face
    font-family 'iconfont' /* Project id 1514406 */
    src url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff2?t=1651741878795') format('woff2'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff?t=1651741878795') format('woff'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.ttf?t=1651741878795') format('truetype')
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
mixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    height $height
maxMixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    min-height $height
mixinsIconfont($content='',$size=16px,$color=#5EBE68)
    content $content
    font-size $size
    color $color
mixinsPositionPack($position=relative)
    position $position
    left 50%
    transform translateX(-50%)
textLineCenter($height)
    text-align center
    line-height $height
flexModule($align=normal,$justify=normal)
    display flex
    align-items $align
    justify-content $justify
.index
    maxMixinsWidthHeight(100vw,100vh)
    background-color #F9F9F9
    &::before
        content ''
        display block
        mixinsWidthHeight(,32rpx)
    .module
        margin 0rpx 50rpx
        border-radius 16rpx
        .title
            mixinsWidthHeight(,56rpx)
            font-size 40rpx
            font-family PingFangSC-Regular, PingFang SC
            font-weight 400
            color #33333D
            flexModule(center,normal)
            &::before
                display block
                mixinsWidthHeight(22rpx,22rpx)
                mixinsIconfont('\e604',25rpx,#F9F9F9)
                flexModule(center,center)
                margin-right 10rpx
                border-radius 50%
                background linear-gradient(48deg, #14AC69 0%, #84D157 100%)
        .fontWeight
            font-weight 500
        .subModule
            background #FFFFFF
            box-shadow 4rpx 12rpx 56rpx 0rpx rgba(102,102,109,0.19)
            border-radius 12rpx
    .top
        .text1
            margin 10rpx 0rpx 0rpx 40rpx
            display block
            height 80rpx
            font-size 24rpx
            font-family PingFangSC-Regular, PingFang SC
            font-weight 400
            color #66666D
            line-height 40rpx
    .module1
        margin-top 40rpx
        .subModule1
            margin-top 10rpx
            height auto
            padding-bottom  40rpx
            font-weight 400
            font-family PingFangSC-Medium, PingFang SC
            .mode
                margin 0rpx 30rpx
                padding-top 40rpx
                .head
                    height 48rpx
                    font-size 32rpx
                    font-weight 500
                    flexModule(center,normal)
                    position relative
                .showDashed
                    &::after
                        content ''
                        height 120rpx
                        display block
                        position absolute
                        top 34rpx
                        left 6rpx
                        border-left 4rpx dashed #99999D
                        z-index 1
                .success
                    color #15AD69
                    &::before
                        display block
                        content ''
                        mixinsWidthHeight(16rpx,16rpx)
                        border-radius 50%
                        background #20B068
                        margin-right 20rpx
                        z-index 100
                .fail
                    color #51515A
                    &::before
                        display block
                        content ''
                        mixinsWidthHeight(16rpx,16rpx)
                        border-radius 50%
                        background #51515A
                        margin-right 20rpx
                        z-index 100
                .text2
                    margin 10rpx 30rpx 0rpx 66rpx
                    font-size 24rpx
                    color #66666D
                    text-align justify
                .btn
                    margin-top 24rpx
                    mixinsWidthHeight(144rpx,44rpx)
                    textLineCenter(44rpx)
                    background #00A9FF
                    box-shadow 4rpx 4rpx 46rpx 0rpx rgba(125,132,158,0.1)
                    border-radius 32rpx
                    font-size 24rpx
                    color #FFFFFF

    .module2
        margin-top 40rpx
        .subModule2
             margin-top 10rpx
             padding 4rpx 0rpx 40rpx 0rpx
             .attrVal
                margin 36rpx 40rpx 0rpx 40rpx
                height 34rpx
                font-size 24rpx
                font-family PingFangSC-Regular, PingFang SC
                font-weight 400
                color #1E1E1E
                line-height 34rpx
                flexModule(center,normal)
                .attr
                    width 150rpx
                .val
                    margin-left 50rpx
                    font-size 24rpx
                    font-family PingFangSC-Regular, PingFang SC
                    font-weight 400
                    color #6F6F6F
    .bottomBtn
        mixinsWidthHeight(,40rpx)
        font-size 24rpx
        font-family PingFangSC-Regular, PingFang SC
        font-weight 400
        color #039EEB
        textLineCenter(40rpx)
        padding 100rpx 0
