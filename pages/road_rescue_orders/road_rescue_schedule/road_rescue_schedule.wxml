<wxs src='../../../utils/util.wxs' module='tools' />
<view class="index">
    <view class="module top">
        <view class="iconfont title">{{'贵ZQ0104'}}</view>
        <text class="text1">申请补贴流水号：{{'366475868688797989'}}\n服务领取流水号：{{'1637488589979797899'}}</text>
    </view>
    <view class="module module1">
        <view class="iconfont title fontWeight">申请补贴进度</view>
        <view class="subModule subModule1">
            <view class="mode">
                <view class="head success {{true ? 'showDashed' : ''}}">申请成功</view>
                <view class="text2">{{'2023-07-02 12:00:00'}}</view>
            </view>
            <view class="mode">
                <view class="head success {{true ? 'showDashed' : ''}}">审核中</view>
                <view class="text2">{{'预计3个工作日完成审核'}}</view>
            </view>
            <view class="mode" wx:if="{{false}}">
                <view class="head success {{true ? 'showDashed' : ''}}">完成</view>
                <view class="text2">{{'申请发放至指定银行卡'}}</view>
            </view>
            <view class="mode">
                <view class="head fail">审核未通过</view>
                <view class="text2">{{'2023-07-02 12:00:00'}}</view>
                <view class="text2">{{'未通过原因未通过原因未通过原因未通过原因未通过原因未通过原因'}}</view>
                <view class="text2 btn" bind:tap="goReceive">重新申请</view>
            </view>
        </view>
    </view>
    <view class="module module2">
        <view class="iconfont title fontWeight">申请信息</view>
        <view class="subModule subModule2">
            <view class="attrVal">
                <view class="attr">呼叫救援时间</view>
                <view class="val">{{'2021-12-02 16:24:33'}}</view>
            </view>
            <view class="attrVal">
                <view class="attr">收款人姓名</view>
                <view class="val">{{'雨林'}}</view>
            </view>
            <view class="attrVal">
                <view class="attr">银行卡号</view>
                <view class="val">{{'523132199103268356'}}</view>
            </view>
            <view class="attrVal">
                <view class="attr">收款金额</view>
                <view class="val">￥{{500}}</view>
            </view>
        </view>
    </view>
    <view class="bottomBtn" bind:tap="goReceive">道路救援申请请页</view>
</view>