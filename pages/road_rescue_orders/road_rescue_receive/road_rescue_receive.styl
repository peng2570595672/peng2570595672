@font-face
    font-family 'iconfont' /* Project id 1514406 */
    src url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff2?t=1651741878795') format('woff2'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff?t=1651741878795') format('woff'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.ttf?t=1651741878795') format('truetype')
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
mixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    height $height
maxMixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    min-height $height
mixinsIconfont($content='',$size=16px,$color=#5EBE68)
    content $content
    font-size $size
    color $color
mixinsPositionPack($position=relative)
    position $position
    left 50%
    transform translateX(-50%)
textLineCenter($height)
    text-align center
    line-height $height
flexModule($align=normal,$justify=normal)
    display flex
    align-items $align
    justify-content $justify
.index
    maxMixinsWidthHeight(100vw,100vh)
    background-color rgba(255, 249, 236, 1)
    &::before
        content ''
        display block
        mixinsWidthHeight(,428rpx)
    .module
        margin 0rpx 28rpx
        border-radius 16rpx
        background #FFFFFF
        box-shadow 0rpx 6rpx 24rpx 0rpx rgba(0,0,0,0.16)
    .module1
        height auto
        padding-bottom 30rpx
        .top
            mixinsWidthHeight(,178rpx)
            background linear-gradient(90deg, #FFF9EC 0%, #FFE8CC 100%)
            border-radius 16rpx 16rpx 0rpx 0rpx
            position relative
            overflow hidden
            .name
                margin 28rpx 0 0 30rpx
                mixinsWidthHeight(634rpx,48rpx)
                font-size 34rpx
                font-family HelveticaNeue-Bold, HelveticaNeue
                font-weight bold
                color #7E5219
                line-height 42rpx
            .text1
                margin 10rpx 0 0 30rpx
                display block
                mixinsWidthHeight(634rpx,64rpx)
                font-size 22rpx
                font-family PingFangSC-Regular, PingFang SC
                font-weight 400
                color #7E5219
                line-height 32rpx
        .bottom
            margin 50rpx 36rpx 0rpx 36rpx
            .vehPlates
                height 44rpx
                font-size 32rpx
                font-family PingFangSC-Semibold, PingFang SC
                font-weight 600
                color #1E1E1E
                flexModule(center,normal)
                .text2
                    margin-right 20rpx
                .pub1
                    mixinsWidthHeight(96rpx,36rpx)
                    border-radius 4rpx
                    font-size 22rpx
                    font-family PingFangSC-Regular, PingFang SC
                    font-weight 400
                    color #FFFFFF
                    textLineCenter(36rpx)
                .blueCard1
                    background #276AD9
                .greenCard1
                    background #5EBE68
            .input
                height 80rpx
                display flex
                color #1E1E1E
                margin 18rpx 0rpx 24rpx 0rpx
                justify-content space-between
                .inputSub
                    mixinsWidthHeight(64rpx,76rpx)
                    font-size 44rpx
                    font-weight 400
                    color #1E1E1E
                    text-align center
                    line-height 72rpx
                    border-radius 16rpx
                    background #ffffff
                    border 2rpx solid #F7F7F7
                    font-family PingFangSC-Regular, PingFang SC
                    .newEnergy
                        font-size 12rpx
                        font-weight 600
                        color #5EBE68
                        font-family PingFangSC-Semibold, PingFang SC
                        .ico
                            &::before
                                display block
                                font-size 24rpx
                                content '\e63d'
                                color #F19C38
                                font-weight bold
                                margin-top -10rpx
                                transform rotateZ(45deg)
                        .text
                            margin-top -40rpx
                .special
                    background-color #fff
                    border 2rpx dashed #F19C38
                .blueCard
                    background #ECF7FF
                .greenCard
                    background #ECFFF4
                    border 2rpx solid #F7F7F7
            .attrVal
                height 88rpx
                border-bottom 2rpx solid #E0E0E0
                flexModule(center,normal)
                font-size 28rpx
                font-family PingFangSC-Regular, PingFang SC
                font-weight 400
                color #1E1E1E
                .attr
                    width 92rpx
                    margin-right 20rpx
                &:last-child
                    flexModule(center,space-between)
                    .val
                        flexModule(center,normal)
                        .pub2
                            mixinsWidthHeight(104rpx,46rpx)
                            textLineCenter(46rpx)
                            background #FFFFFF
                            border-radius 24rpx
                            border 1rpx solid #C8C8C8
                            &:first-child
                                margin-right 20rpx
                        .select
                            border 1rpx solid #5EBE68
                            background #5EBE68
                            color #fff

            
    // 悬浮按钮
    .btn
        z-index 100
        mixinsWidthHeight(,302rpx)
        background #FBFBFB
        box-shadow 0rpx -8rpx 30rpx 0rpx rgba(223,223,223,0.36)
        font-family PingFangSC-Semibold, PingFang SC
        border-radius 16rpx 16rpx 0rpx 0rpx
        position fixed
        bottom 0rpx
        .agremment
            height 56rpx
            margin 30rpx 32rpx 12rpx 32rpx
            flexModule(center,normal)
            .icon
                mixinsWidthHeight(44rpx,)
                margin-right 16rpx
                flexModule(center,center)
            .yes
                &::before
                        display block
                        mixinsWidthHeight(44rpx,44rpx)
                        mixinsIconfont('\e669',22rpx,#FFFFFF)
                        text-align center
                        line-height 44rpx
                        border-radius 50%
                        background-color #39812B
            .no
                &::before
                        display block
                        mixinsWidthHeight(44rpx,44rpx)
                        mixinsIconfont('',22rpx,#FFFFFF)
                        text-align center
                        line-height 44rpx
                        border-radius 50%
                        background-color #39812B
            .text4
                font-size 20rpx
                font-family PingFangSC-Regular, PingFang SC
                font-weight 400
                color #000000
                line-height 28rpx
                text-align justify
                .text5
                    color #39812B 
        .btnInner
            margin 20rpx 44rpx 0rpx 44rpx
            height 110rpx
            background rgba(42, 79, 68, 1)
            box-shadow 0rpx -8rpx 30rpx 0rpx rgba(223,223,223,0.36)
            border-radius 64rpx
            textLineCenter(110rpx)
            font-size 36rpx
            font-weight 600
            color #FFFFFF
            text-shadow 0px -8px 30px rgba(223,223,223,0.36)
    .split2
        mixinsWidthHeight(,342rpx)