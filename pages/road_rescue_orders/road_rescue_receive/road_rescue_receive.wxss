.index {
  width: 100vw;
  min-height: 100vh;
  position: relative;
  z-index: 1;
}
.index .img1 {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  z-index: -1;
}
.index .split1 {
  width: 100%;
  height: 428rpx;
}
.index .module {
  margin: 0rpx 28rpx;
  border-radius: 16rpx;
  background: #fff;
  box-shadow: 0rpx 6rpx 24rpx 0rpx rgba(0,0,0,0.16);
}
.index .module1 {
  height: auto;
  padding-bottom: 30rpx;
}
.index .module1 .top {
  width: 100%;
  height: 178rpx;
  background: linear-gradient(90deg, #fff9ec 0%, #ffe8cc 100%);
  border-radius: 16rpx 16rpx 0rpx 0rpx;
  position: relative;
  overflow: hidden;
}
.index .module1 .top .name {
  margin: 28rpx 0 0 30rpx;
  width: 634rpx;
  height: 48rpx;
  font-size: 34rpx;
  font-family: HelveticaNeue-Bold, HelveticaNeue;
  font-weight: bold;
  color: #7e5219;
  line-height: 42rpx;
}
.index .module1 .top .text1 {
  margin: 10rpx 0 0 30rpx;
  display: block;
  width: 634rpx;
  height: 64rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #7e5219;
  line-height: 32rpx;
}
.index .module1 .top .img2 {
  width: 196rpx;
  height: 158rpx;
  position: absolute;
  top: 14rpx;
  right: 0;
}
.index .module1 .bottom {
  margin: 50rpx 36rpx 0rpx 36rpx;
}
.index .module1 .bottom .vehPlates {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #1e1e1e;
