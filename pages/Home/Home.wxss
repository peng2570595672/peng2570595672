::-webkit-scrollbar {
	width: 12rpx;
	height: 672rpx;
	border-radius: 6rpx;
	background: rgba(240, 240, 240, 1);
}

/*定义滚动条轨道 内阴影+圆角*/
::-webkit-scrollbar-track {
	border-radius: 12rpx;
}

/*定义滑块 内阴影+圆角*/
::-webkit-scrollbar-thumb {
	border-radius: 12rpx;
	background-color: rgba(216, 216, 216, 1);
}

