}
.home .topSwiper {
  width: 100%;
  height: auto;
  font-family: PingFangSC-Regular, PingFang SC;
}
.home .topSwiper .swiperBox {
  width: 100%;
  height: 100%;
}
.home .topSwiper .swiperBox .swiper-item {
  width: 100%;
  height: 100%;
  z-index: -1;
}
.home .topSwiper .swiperBox .swiper-item .topImg {
  z-index: -1;
  width: 100%;
  height: 100%;
}
.home .topSwiper .regular {
  pointer-eventsnone: none;
}
.home .topSwiper .regular .regular-item {
  pointer-eventsauto: auto;
}
.home .module {
  padding: 0rpx 28rpx;
  border-radius: 16rpx;
  background: #fff;
  margin: 32rpx 28rpx 0rpx 28rpx;
  position: relative;
  top: -100rpx;
}
.home .immediately {
  display: flex;
  overflow: hidden;
  align-items: center;
  padding: 0rpx 28rpx;
}
.home .immediately .vehicle-info {
  flex: 1;
  position: relative;
}
.home .immediately .vehicle-info .vehicle-info-box {
  flex: 1 /* height 98rpx */;
  font-size: 36rpx;
  margin: 32rpx 0rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #1e1e1e;
}
.home .immediately .vehicle-info .vehicle-info-box .sub-title {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #1e1e1e;
  line-height: 34rpx;
  padding-top: 20rpx;
}
.home .immediately .vehicle-info .vehicle-info-box.vehicle-info-trucks-box {
  position: absolute;
  top: 150rpx;
}
.home .immediately .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0rpx 36rpx;
  height: 76rpx;
  background: #2a4f44;
  border-radius: 16rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 400;
  color: #fff;
}
.home .immediately .bill-info {
  text-align: center;
}
.home .immediately .bill-info .bill {
  height: 56rpx;
  display: flex;
  align-items: center;
  font-size: 48rpx;
  font-weight: bold;
  color: #00b85c;
}
.home .immediately .bill-info .bill::after {
  content: '\e63e';
  font-size: 20rpx;
  color: #ffec9a;
  margin-left: 12rpx;
}
.home .immediately .bill-info .bill.failure {
  color: #ff3c48;
}
.home .immediately .bill-info .bill-status {
  display: flex;
  height: 34rpx;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #33333e;
  line-height: 34rpx;
  padding-top: 2rpx;
}
.home .immediately .bill-info .bill-status.failure::before {
  color: #ff3c48;
  content: '\e617';
  margin-right: 8rpx;
}
.home .moduleOne {
  display: flex;
  min-height: 302rpx;
  align-items: center;
  justify-content: space-around;
}
.home .moduleOne .moduleOneBox {
  width: 186rpx;
  height: 238rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.home .moduleOne .moduleOneBox .imgTitle {
  width: 118rpx;
  height: 118rpx;
  margin: 0 34rpx;
}
.home .moduleOne .moduleOneBox .imgTitle image {
  width: 100%;
  height: 100%;
}
.home .moduleOne .moduleOneBox .title {
  width: 100%;
  height: 40rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #000;
  margin-top: 14rpx;
  line-height: 40rpx;
  text-align: center;
}
.home .moduleOne .moduleOneBox .btn_two {
  width: 100%;
  height: 50rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #d2ba78;
  margin-top: 16rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 26rpx;
  background: #f8f1df;
  letter-spacing: 3rpx;
  box-shadow: inset 0rpx -4rpx 6rpx 0rpx rgba(235,212,151,0.34);
}
.home .head {
  font-size: 30rpx;
  font-weight: 500;
  color: #000;
  margin-bottom: 16rpx;
  font-family: PingFangSC-Medium, PingFang SC;
}
.home .moduleTwo {
  margin: 32rpx 28rpx 0rpx 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  position: relative;
  top: -100rpx;
  height: auto;
}
.home .moduleTwo .moduleTwoBox {
  width: 100%;
  height: 154rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.home .moduleTwo .moduleTwoBox .integral {
  height: 100%;
  width: 334rpx;
  display: flex;
  overflow: hidden;
  margin-bottom: 28rpx;
  align-items: center;
  border-radius: 16rpx;
  box-shadow: 0rpx 4rpx 8rpx 0rpx rgba(240,240,240,0.5);
}
.home .moduleTwo .moduleTwoBox .integral image {
  width: 100%;
  height: 100%;
}
.home .moduleThree {
  height: auto;
  margin: 32rpx 28rpx 0rpx 28rpx;
  background-color: transparent;
  position: relative;
  top: -100rpx;
}
.home .moduleThree .moduleThreeBox {
  width: 100%;
  overflow: hidden;
  min-height: 256rpx;
  background: #fff;
  border-radius: 16rpx;
}
.home .moduleThree .moduleThreeBox .directSeeding {
  margin: 28rpx;
  width: 638rpx;
  display: flex;
  height: 200rpx;
  align-items: center;
  justify-content: space-between;
}
.home .moduleThree .moduleThreeBox .directSeeding .leftImg {
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  margin-right: 28rpx;
  border-radius: 16rpx;
  background: #efefef;
}
.home .moduleThree .moduleThreeBox .directSeeding .leftImg image {
  width: 100%;
  height: 100%;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView {
  width: 410rpx;
  min-height: 128rpx;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .directSeedingTitle {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .directSeedingTitle view:first-child {
  height: auto;
  width: 290rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #000;
  overflow: hidden;
  line-height: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .directSeedingTitle .directSeedingImg {
  height: 40rpx;
  display: flex;
  font-size: 16rpx;
  font-weight: 400;
  color: #000;
  line-height: 40rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .directSeedingTitle .directSeedingImg .loginImg {
  width: 26rpx;
  height: 28rpx;
  overflow: hidden;
  margin-right: 2rpx;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .directSeedingTitle .directSeedingImg .loginImg image {
  width: 100%;
  height: 100%;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .lsBack {
  width: 100%;
  height: 34rpx;
  margin-top: 8rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #000;
  line-height: 34rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .viewText {
  width: 100%;
  height: 24rpx;
  display: flex;
  margin-top: 8rpx;
  align-items: center;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .viewText view:first-child {
  width: 24rpx;
  height: 24rpx;
  overflow: hidden;
  margin-right: 8rpx;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView .viewText view:first-child image {
  width: 100%;
  height: 100%;
}
.home .moduleThree .moduleThreeBox .directSeeding .rightView text {
  display: block;
}
.home .split {
  width: 100%;
  overflow: hidden;
}
