@import "../../../style/base.styl";
.index
	display flex
	flex-direction column
	padding 32rpx
	box-sizing border-box
	min-height 100vh
	.pre-recharge
		margin-bottom 32rpx
		.pre-recharge-info
			height: 278rpx;
			background: #00B85C;
			padding 32rpx
			box-sizing border-box
			box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(0,0,0,0.1);
			border-radius: 20rpx 20rpx 0rpx 0rpx;
			.type
				display flex
				height 40rpx
				align-items center
				color #FFFFFF
				padding-bottom 12rpx
				:first-child
					font-size: 28rpx;
					flex 1
				:last-child
					font-size: 24rpx;
					font-family: PingFangSC-Medium, PingFang SC;
					font-weight: 500;
					color: rgba(255, 255, 255, 0.5);
			.account-info
				display flex
				align-items center
				height 84rpx
				font-size 60rpx
				font-family PingFangSC-Semibold, PingFang SC
				font-weight 600
				color #FFFFFF
				margin-bottom 10rpx
				.info
					flex 1
					text
						font-size: 24rpx;
						font-family: PingFangSC-Regular, PingFang SC;
						font-weight: 400;
						color: rgba(255, 255, 255, 0.9);
						padding-left 8rpx
				.recharge
					display flex
					align-items center
					justify-content center
					width: 114rpx;
					height: 52rpx;
					background: #FFFFFF;
					border-radius: 4rpx;
					font-size: 24rpx;
					font-family: PingFangSC-Regular, PingFang SC;
					font-weight: 400;
					color: #00B85C;
			.prompt
				font-size: 24rpx;
				font-family: PingFangSC-Medium, PingFang SC;
				font-weight: 500;
				color: #FFC000;
				line-height: 34rpx;
		.vehicle
			height 70rpx
			display flex
			align-items center
			background: #FCFFFD;
			box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(0,0,0,0.1);
			border-radius: 0rpx 0rpx 20rpx 20rpx;
			font-size: 24rpx;
			font-family: PingFangSC-Regular, PingFang SC;
			font-weight: 400;
			color: rgba(0,0,0,.39)
			padding-left 32rpx
	.card
		position relative
		height 325rpx
		background-image url("data:image/png;base64,")
		background-size 322rpx
		background-repeat no-repeat
		background-position 110% -60rpx
		background-color #C25255
		color #fff
		padding 44rpx 24rpx 32rpx
		box-sizing border-box
		box-shadow 0rpx 6rpx 16rpx 0rpx rgba(0, 0, 0, 0.1)
		font-family PingFangSC-Medium, PingFang SC
		border-radius 20rpx
		z-index 1
		.card-info
			display flex
			.left
				flex 1
				color rgba(255, 255, 255, 0.9)
				& > view:first-child
					font-size 28rpx
					line-height 39rpx
				& > view:nth-child(2)
					color rgba(255, 255, 255, 1)
					font-size 60rpx
					font-family PingFangSC-Semibold, PingFang SC
					padding-top 8rpx
					line-height 84rpx
				& > view:last-child
				.account-info
					display flex
					font-size 24rpx
					line-height 39rpx
					.name
						flex 1
					.account
						font-size 30rpx
			.right
				display flex
				height 60rpx
				align-items center
				margin-right 40rpx
				&::before
					content '\e66f'
					font-size 60rpx
					margin-right 12rpx
				& > view
					color #ffffff
					opacity .5
					font-family PingFangSC-Medium, PingFang SC
					font-size 24rpx
					line-height 34rpx
					& > view:last-child
						font-size 18rpx
						line-height 26rpx
		.card-no
			display flex
			padding-top 34rpx
			font-size 24rpx
			font-family PingFangSC-Medium, PingFang SC
			height 42rpx
			align-items center
			& > view
				margin-right 56rpx
				&::before
					content ''
					font-size 30rpx
					margin-right 8rpx
			.recharge
				&::before
					content '\e6ad'
			.deposit
				&::before
					content '\e6ae'
			.tied-card
				&::before
					content '\e6ac'
		&.bocom
			background-image url("data:image/png;base64,")
			background-size cover
			background-repeat no-repeat
			background-position 0
			background-color #003A90
	.bocom-info
		margin-bottom 20rpx
		.vehicle
			position relative
			z-index 0
			height 176rpx
			background #FCFFFD
			box-shadow 0rpx 6rpx 16rpx 0rpx rgba(0,0,0,0.1)
			border-radius 20rpx
			padding-top 120rpx
			padding-left 32rpx
			box-sizing border-box
			font-size 24rpx
			font-family PingFangSC-Regular, PingFang SC
			font-weight 400
			color #000000
			line-height 39rpx
			margin-top -108rpx
	// @cyl
	.margin
		background none
		background-color #00B85C
		box-shadow 0px 3px 8px 0px rgba(0, 0, 0, 0.1)
		.card-info
			.left
				view
					&:nth-of-type(2)
						font-family PingFangSC-Semibold, PingFang SC
						margin-top 10rpx
						line-height 84rpx
						.money
							font-size 60rpx
						.t2
							height: 84rpx
							font-size 28rpx
							line-height 30rpx
							margin-left 25rpx
							opacity 0.9
						.t3
							width 114rpx
							height 52rpx
							line-height 52rpx
							text-align center
							background #FFFFFF
							border-radius 2px
							color #00B85C
							font-size 28rpx
							font-weight 400
							position absolute
							top 115rpx
							right 48rpx
							z-index 100
			.right1
				opacity 0.5
				font-size 24rpx
				margin-right 28rpx
		.accont
			view
				height 34rpx
				background url('https://file.cyzl.com/g001/M00/98/0A/oYYBAGMa6G6AKdv9AAACKC1YOLE589.png') no-repeat
				background-size 32rpx 24rpx
				background-position 0rpx 10rpx
				padding-left 50rpx
		.tip
			width 414rpx
			height 68rpx
			font-size 24rpx
			font-family PingFangSC-Medium, PingFang SC
			font-weight 500
			color #FFC000
			line-height 34rpx
		.card-no
			width 100%
			height 40rpx
			line-height 40rpx
			color #000
			background-color #FCFFFD
			border-radius 0 0 20rpx 20rpx
			position absolute
			bottom -1rpx
			left 50%
			transform translateX(-50%)
			.accountBind
				width 100%
				height 60rpx
				line-height 60rpx
				font-size 24rpx
				font-family PingFangSC-Regular, PingFang SC
				font-weight 400
				padding-left 20rpx
				color rgba(0, 0, 0, 0.4)
				position absolute
				top 0