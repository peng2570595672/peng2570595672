@import "../../../style/base.styl";
.index
	display flex
	flex-direction column
	padding 32rpx
	box-sizing border-box
	min-height 100vh
	.pre-recharge
		margin-bottom 32rpx
		.pre-recharge-info
			height: 278rpx;
			background: #00B85C;
			padding 32rpx
			box-sizing border-box
			box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(0,0,0,0.1);
			border-radius: 20rpx 20rpx 0rpx 0rpx;
			.type
				display flex
				height 40rpx
				align-items center
				color #FFFFFF
				padding-bottom 12rpx
				:first-child
					font-size: 28rpx;
					flex 1
				:last-child
					font-size: 24rpx;
					font-family: PingFangSC-Medium, PingFang SC;
					font-weight: 500;
					color: rgba(255, 255, 255, 0.5);
			.account-info
				display flex
				align-items center
				height 84rpx
				font-size 60rpx
				font-family PingFangSC-Semibold, PingFang SC
				font-weight 600
				color #FFFFFF
				margin-bottom 10rpx
				.info
					flex 1
					text
						font-size: 24rpx;
						font-family: PingFangSC-Regular, PingFang SC;
						font-weight: 400;
						color: rgba(255, 255, 255, 0.9);
						padding-left 8rpx
				.recharge
					display flex
					align-items center
					justify-content center
					width: 114rpx;
					height: 52rpx;
					background: #FFFFFF;
					border-radius: 4rpx;
					font-size: 24rpx;
					font-family: PingFangSC-Regular, PingFang SC;
					font-weight: 400;
					color: #00B85C;
			.prompt
				font-size: 24rpx;
				font-family: PingFangSC-Medium, PingFang SC;
				font-weight: 500;
				color: #FFC000;
				line-height: 34rpx;
		.vehicle
			display flex
			align-items center
			height 70rpx
			background: #FCFFFD;
			box-shadow: 0rpx 6rpx 16rpx 0rpx rgba(0,0,0,0.1);
			border-radius: 0rpx 0rpx 20rpx 20rpx;
			font-size: 24rpx;
			font-family: PingFangSC-Regular, PingFang SC;
			font-weight: 400;
			color: rgba(0,0,0,.39)
			padding-left 32rpx
	.card
		height 325rpx
		background-image url("data:image/png;base64,")
		background-size 322rpx
		background-repeat no-repeat
		background-position 110% -60rpx
		background-color #C25255
		color #fff
		padding 44rpx 24rpx 32rpx
		box-sizing border-box
		box-shadow 0rpx 6rpx 16rpx 0rpx rgba(0, 0, 0, 0.1)
		font-family PingFangSC-Medium, PingFang SC
		border-radius 20rpx
		.card-info
			display flex
			.left
				flex 1
				color rgba(255, 255, 255, 0.9)
				& > view:first-child
					font-size 28rpx
					line-height 39rpx
				& > view:nth-child(2)
					color rgba(255, 255, 255, 1)
					font-size 60rpx
					font-family PingFangSC-Semibold, PingFang SC
					padding-top 8rpx
					line-height 84rpx
				& > view:last-child
					font-size 24rpx
					line-height 39rpx
			.right
				display flex
				height 60rpx
				align-items center
				margin-right 40rpx
				&::before
					content '\e66f'
					font-size 60rpx
					margin-right 12rpx
				& > view
					color #ffffff
					opacity .5
					font-family PingFangSC-Medium, PingFang SC
					font-size 24rpx
					line-height 34rpx
					& > view:last-child
						font-size 18rpx
						line-height 26rpx
		.card-no
			display flex
			padding-top 34rpx
			font-size 30rpx
			font-family PingFangSC-Medium, PingFang SC
			line-height 42rpx
			& > view:first-child
				flex 1
				font-size 24rpx
				font-family PingFangSC-Regular, PingFang SC
				color rgba(255, 255, 255, 0.9)
