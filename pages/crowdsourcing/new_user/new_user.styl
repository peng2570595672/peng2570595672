.page
	width 100vw
	height 100vh
	background-color #7D2360
	overflow hidden
	.bg-container
		position relative
		display block
		width 100vw
		height 100vh
		background-color #7D2360
		background-image url("data:image/png;base64,")
		background-size 100% 100%
		.user_msg
			display flex
			padding 65rpx 45rpx 30rpx
			line-height 83rpx
			font-size 24rpx
			font-family PingFangSC-Semibold, PingFang SC
			font-weight 600
			color #FFFFFF
			.user_head
				width 83rpx
				height 83rpx
				border 1px solid rgba(151, 151, 151, 0.2)
				border-radius 50%
				overflow hidden
				margin-right 20rpx
		.prompt-char
			text-align center
			font-size 36rpx
			font-family PingFangSC-Semibold,PingFang SC
			font-weight 600rpx
			color #fff
		.btns
			width 100vw
			text-align center
			margin-top -10rpx
			button
				font-weight 500
				width 567rpx
				height 90rpx
				background #FDE27C
				box-shadow 0rpx 5rpx 9rpx 0rpx rgba(59,24,24,0.28)
				border-radius 45rpx
				font-size 34rpx
				font-family PingFangSC-Medium, PingFang SC
				color #B25B25
				line-height 90rpx
			button.go
				background-color transparent
				border 2rpx solid #FDE27C
				font-size 24rpx
				font-family PingFangSC-Semibold, PingFang SC
				font-weight 600
				color #FDE27C
				margin-top 36rpx
		.content-container
			position relative
			width 100%
			text-align center
			min-height 628rpx
			margin-top -10rpx
			image
				width 100%
			.content
				position absolute
				width 100%
				top 130rpx
				font-size 40rpx
				font-family PingFangSC-Semibold, PingFang SC
				font-weight 600
				color #FFFEFC
				line-height 40rpx
				text-shadow 0rpx 2rpx 4rpx rgba(0, 0, 0, 0.11)
				background linear-gradient(235deg, #F3CD70 0%, #FADB84 47%, #EBB137 100%)
				-webkit-background-clip text
				-webkit-text-fill-color transparent
			.deal_with
				position absolute
				top 5%
				left 5%
				width 90%
				height 90%
				z-index 10
				background-color transparent
	.bind-mobile-mask
		position fixed
		left 0
		right 0
		top 0
		bottom 0
		background: rgba(0, 0, 0, .6)
		z-index 11
		.bind-mobile-wrapper
			position fixed
			left 0
			right 0
			bottom 0
			padding 66rpx
			background: #fff
			border-top-left-radius 20rpx
			border-top-right-radius 20rpx
			.title
				display flex
				width 100%
				align-items center
				font-size 40rpx
				&::before,
				&::after
					flex 1
					content ''
					height 2rpx
				&::before
					margin-right 30rpx
					background: linear-gradient(to right, #fff, #D9D9D9)
				&::after
					margin-left 30rpx
					background: linear-gradient(to right, #D9D9D9, #fff)
			.mobile
				display flex
				& > button
					margin-top 71rpx
					flex 1
					height 90rpx
					display flex
					align-items center
					justify-content center
					font-size 32rpx
					color #fff
					background-color #f23955
					border-radius 45rpx
