.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.index {
  margin: 0;
  padding: 0;
  width: 100vw;
  min-height: 100vh;
  overflow: hidden;
  background-color: #fff;
}
.changeBg {
  background-color: #ececec;
}
.index .topSplit {
  width: 100%;
  height: 60rpx;
}
.index .module {
  width: 694rpx;
  border-radius: 16rpx;
  background-color: #fff;
  margin: 32rpx 0 0 0;
  font-family: PingFangSC-Regular, PingFang SC;
  position: relative;
  left: 50%;
  transform: translateX(-50%);
}
.index .module .titles {
  height: 48rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #000;
  line-height: 48rpx;
  padding-left: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
}
.index .boxS {
  box-shadow: 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.index .moduleOne {
  min-height: 400rpx;
  padding: 40rpx 0 20rpx 0;
}
.index .moduleOne .photo-wrapper {
  display: flex;
  height: 330rpx;
  justify-content: space-between;
}
.index .moduleOne .photo-wrapper .photo {
  width: 326rpx;
  height: 330rpx;
  text-align: center;
  border-radius: 16rpx;
  background: #f6f6f6;
}
.index .moduleOne .photo-wrapper .photo .upload {
  width: 90%;
  height: 192rpx;
  overflow: hidden;
  margin: 22rpx 5%;
  border-radius: 8rpx;
  background: #fafafa;
}
.index .moduleOne .photo-wrapper .photo .upload .img-box {
  width: 100%;
  height: 100%;
  position: relative;
}
.index .moduleOne .photo-wrapper .photo .upload .img-box .ico {
  width: 56rpx;
  height: 49rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.index .moduleOne .photo-wrapper .photo .upload .success {
  width: 100%;
  height: 100%;
}
.index .moduleOne .photo-wrapper .photo .changeThree {
  width: 100%;
  margin: 22rpx 0;
}
.index .moduleOne .photo-wrapper .photo:nth-of-type(1) .upload .img-box {
  background: url("https://file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png") center no-repeat;
  background-size: 80% 80%;
}
.index .moduleOne .photo-wrapper .photo:nth-of-type(2) .upload .img-box {
  background: url("https://file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png") center no-repeat;
  background-size: 80% 80%;
}
.index .moduleOne .photo-wrapper .photo .cardText {
  margin-top: 40rpx;
}
.index .moduleOne .photo-wrapper .changeTwo {
  width: 300rpx;
  background-color: #fff;
}
.index .moduleOne .changeOne {
  margin: 20rpx 36rpx 0 36rpx;
}
.index .moduleTwo {
  padding: 40rpx 0 20rpx 0;
  color: #666;
}
.index .moduleTwo .tips {
  font-size: 25rpx;
  font-weight: 400;
  color: #666;
  margin: 10rpx 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .moduleTwo .keyValue {
  height: 86rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  margin: 0 36rpx 20rpx 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .moduleTwo .keyValue .left {
  width: 120rpx;
  padding: 2rpx;
}
.index .moduleTwo .keyValue .attrName {
  font-size: 28rpx;
  font-weight: 550;
  text-align: right;
  color: #1e1e1e;
}
.index .moduleTwo .keyValue .attrValue {
  width: 70%;
  border-bottom: 1px solid #e0e0e0;
  height: 64rpx;
  display: flex;
  align-items: center;
}
.index .moduleTwo .keyValue .attrValue .input {
  display: block;
  max-width: 400rpx;
  font-size: 26rpx;
  color: #333;
  text-align: left;
  word-wrap: break-word;
  font-family: PingFangSC-Semibold, PingFang SC;
  opacity: 0.9;
}
.index .moduleTwo .keyValue .attrValue .textarea {
  display: block;
  max-width: 400rpx;
  height: 100%;
  font-size: 28rpx;
  font-weight: 600;
  color: #262626;
  text-align: right;
  font-family: PingFangSC-Semibold, PingFang SC;
}
.index .moduleTwo .keyValue .input-placeholder {
  color: rgba(212,212,212,0.7);
}
.index .split {
  width: 100%;
  height: 280rpx;
  overflow: hidden;
}
.index .bottomBtn {
  display: flex;
  width: 750rpx;
  height: 244rpx;
  align-items: center;
  background: #fff;
  justify-content: center;
  box-shadow: 0 -8rpx 30rpx 0 rgba(223,223,223,0.36);
  border-radius: 16rpx 16rpx 0 0;
  position: fixed;
  bottom: 0;
}
.index .bottomBtn .btnClose {
  width: 662rpx;
  height: 110rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #fff;
  text-align: center;
  line-height: 110rpx;
  border-radius: 64rpx;
  background: #c0c0c0;
  font-family: PingFangSC-Semibold, PingFang SC;
  box-shadow: 0 -8rpx 30rpx 0 rgba(223,223,223,0.36), 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.index .bottomBtn .available {
  background: #2a4f44;
  animation: btnFadeIn 0.3s;
  animation-fill-mode: forwards;
}
.index .bottomBtn .unavailable {
  animation: btnFadeOut 0.3s;
  animation-fill-mode: forwards;
}
