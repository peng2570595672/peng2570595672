        <!-- 占位 -->
        <view class="module moduleTwo" wx:if="{{drivingLicenseFace.fileUrl || drivingLicenseBack.fileUrl}}">
            <view class="titles">详细信息</view>
            <view class="keyValue">
                <view style="width: 150rpx;font-weight: 550;">尺寸(mm)</view>
                <view>
                    <input class="input"
                        style="width: 110rpx;height: 50rpx;text-align: center; border: 1px solid #F0F0F0; padding: 2rpx 5rpx;"
                        type="text" bindinput="onInputChangedHandle" data-key="size" data-type="4"
                        value="{{drivingLicenseBack.ocrObject.vehicleLength}}">
                    </input>
                    <view style="margin-left: 40rpx;padding: 10rpx 0;">长</view>
                </view>
                <view>
                    <input class="input"
                        style="width: 110rpx;height: 50rpx;text-align: center; border: 1px solid #F0F0F0; padding: 2rpx 5rpx;"
                        type="text" bindinput="onInputChangedHandle" data-key="size" data-type="4"
                        value="{{drivingLicenseBack.ocrObject.vehicleLength}}">
                    </input>
                    <view style="margin-left: 40rpx;padding: 10rpx 0;">宽</view>
                </view>
                <view>
                    <input class="input"
                        style="width: 110rpx;height: 50rpx;text-align: center; border: 1px solid #F0F0F0; padding: 2rpx 5rpx;"
                        type="text" bindinput="onInputChangedHandle" data-key="size" data-type="4"
                        value="{{drivingLicenseBack.ocrObject.vehicleLength}}">
                    </input>
                    <view style="margin-left: 40rpx;padding: 10rpx 0;">高</view>
                </view>
            </view>
            <view class="fill" style="border-top: 1px solid #E0E0E0; width: 60%;margin-left: 32%;margin-bottom: 30rpx;">
            </view>
            <view class="keyValue">
                <view style="width: 150rpx;font-weight: 550;">质量(kg)</view>
                <view>
                    <input class="input"
                        style="width: 110rpx;height: 50rpx;text-align: center; border: 1px solid #F0F0F0; padding: 2rpx 5rpx;"
                        type="text" bindinput="onInputChangedHandle" data-key="size" data-type="4"
                        value="{{drivingLicenseBack.ocrObject.totalMass}}">
                    </input>
                    <view style="margin-left: 10rpx;padding: 10rpx 0;">总重量</view>
                </view>
                <view>
                    <input class="input"
                        style="width: 110rpx;height: 50rpx;text-align: center; border: 1px solid #F0F0F0; padding: 2rpx 5rpx;"
                        type="text" bindinput="onInputChangedHandle" data-key="size" data-type="4"
                        value="{{drivingLicenseBack.ocrObject.tractionMass}}">
                    </input>
                    <view style="margin-left: 10rpx;padding: 10rpx 0;">机身重量</view>
                </view>
                <view>
                    <input class="input"
                        style="width: 110rpx;height: 50rpx;text-align: center; border: 1px solid #F0F0F0; padding: 2rpx 5rpx;"
                        type="text" bindinput="onInputChangedHandle" data-key="size" data-type="4"
                        value="{{drivingLicenseBack.ocrObject.vehicleLength}}">
                    </input>
                    <view style="margin-left: 10rpx;padding: 10rpx 0;">货物重量</view>
                </view>
            </view>
        </view>
        <view class="split"></view>
        <!-- 底部悬浮按钮 -->
        <view class="bottomBtn" wx:if="{{faceStatus === 4 && backStatus === 4}}">
            <view class="btnClose {{available ? 'available' : 'unavailable'}}" catchtap="onClickConfirmHandle">
                确认资料并保存
            </view>
        </view>
    </view>
    <!-- 子容器覆盖 -->
    <page-container show="{{show}}" round="{{round}}" overlay="{{overlay}}" duration="{{duration}}"
        position="{{position}}" custom-style="{{customStyle}}" overlay-style="{{overlayStyle}}"
        close-on-slide-down="{{false}}" bindbeforeleave="onBeforeLeave">
    </page-container>
    <information-validation-prompt id="notFinishedOrder" promptObject="{{promptObject}}">
    </information-validation-prompt>
    <!-- 组件 提示 -->
    <popTipComp id="popTipComp" tipObj="{{tipObj}}"></popTipComp>
</view>
<privacy_popup></privacy_popup>