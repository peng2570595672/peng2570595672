mixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    height $height
mixinsPositionPack($position = relative)
    position $position
    left 50%
    transform translateX(-50%)
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
.index
    margin 0
    padding 0
    width 100vw
    min-height 100vh
    overflow hidden
    background-color #fff
.changeBg
    background-color #ECECEC
.index
    /* 顶部组件占位 */
    .topSplit
        mixinsWidthHeight(,60rpx)
    /* 公共模块 */
    .module
        width 694rpx
        border-radius 16rpx
        background-color #fff
        margin 32rpx 0 0 0
        font-family PingFangSC-Regular, PingFang SC
        position relative
        left 50%
        transform translateX(-50%)
        .titles
            height 48rpx
            font-size 32rpx
            font-weight 600
            color #000000
            line-height 48rpx
            padding-left 36rpx
            font-family PingFangSC-Semibold, PingFang SC
    .boxS
        box-shadow 0 4rpx 2rpx 0 rgba(212, 212, 212, 0.5)
    /* 模块一 */
    .moduleOne
        min-height 400rpx
        padding 40rpx 0 20rpx 0
        .photo-wrapper
            display flex
            height 330rpx
            justify-content space-between
            .photo
                mixinsWidthHeight(326rpx, 330rpx)
                text-align center
                border-radius 16rpx
                background #F6F6F6
                .upload
                    mixinsWidthHeight(90%, 192rpx)
                    overflow hidden
                    margin 22rpx 5%
                    border-radius 8rpx
                    background #FAFAFA
                    .img-box
                        mixinsWidthHeight()
                        position relative
                        .ico
                            mixinsWidthHeight(56rpx, 49rpx)
                            position absolute
                            top 50%
                            left 50%
                            transform translate(-50%, -50%)
                    .success
                        mixinsWidthHeight()
                .changeThree
                    width 100%
                    margin 22rpx 0
                &:nth-of-type(1)
                    .upload
                        .img-box
                            background url('https://file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5VGAN4KUAAAf_5IXjB0976.png') center no-repeat
                            background-size 80% 80%
                &:nth-of-type(2)
                    .upload
                        .img-box
                            background url('https://file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat // file.cyzl.com/g001/M01/C7/A2/oYYBAGPs5YWAdC_FAAAXSu-w0ZM580.png') center no-repeat
                            background-size 80% 80%
                .cardText
                    margin-top 40rpx
            .changeTwo
                width 300rpx
                background-color #ffffff
        .changeOne
            margin 20rpx 36rpx 0 36rpx
    .fill
        border-top: 1px solid #E0E0E0;
        width: 60%;
        margin-left: 32%;
        margin-bottom: 30rpx;
    /* 模块二 信息显示 */
    .moduleTwo
        padding 40rpx 0 20rpx 0
        color #666
        .tips
            font-size 25rpx
            font-weight 400
            color #666
            margin 10rpx 36rpx
            font-family PingFangSC-Regular, PingFang SC
        .keyValue
            height 86rpx
            display flex
            align-items center
            justify-content space-around
            margin 0 36rpx 20rpx 36rpx
            font-family PingFangSC-Regular, PingFang SC
            position relative
            .label
                width: 150rpx;
                font-weight: 550;
            .input-info
                flex 1
                display flex
                align-items center
                text-align center
                justify-content center
                & > view
                    flex 1
                    text-align center
                    .input
                        width: 110rpx;
                        height: 50rpx;
                        text-align: center;
                        border: 1px solid #F0F0F0;
                        padding: 2rpx 5rpx;
                        margin auto
                    .label-sub
                        padding: 10rpx 0;
            .left
                width 130rpx
                padding 2rpx
            .attrName
                font-size 28rpx
                font-weight 550
                text-align right
                color #1E1E1E
            .upload-vin
                position absolute
                right 0
                width: 86rpx;
                height: 46rpx;
                border-radius: 4rpx;
                background: #2A4F44;
                font-size: 24rpx;
                color #fff
                font-weight: normal;
                display: flex;
                align-items: center;
                justify-content center
            .errTips
                position absolute
                bottom -30rpx
                left 140rpx
                font-size: 24rpx;
                color #FF563C
            .attrValue
                flex 1
                border-bottom 1px solid #E0E0E0
                height 64rpx
                display flex
                padding-left 24rpx
                align-items center
                .input
                    display block
                    max-width 400rpx
                    min-width 300rpx
                    font-size 26rpx
                    color #333
                    text-align left
                    word-wrap break-word
                    font-family PingFangSC-Semibold, PingFang SC
                    opacity 0.9
                .textarea
                    display block
                    max-width 400rpx
                    height 100%
                    font-size 28rpx
                    font-weight 600
                    color #262626
                    text-align right
                    font-family PingFangSC-Semibold, PingFang SC
            .input-placeholder
                color rgba(212, 212, 212, 0.7)
    /* 占位 */
    .split
        width 100%
        height 280rpx
        overflow hidden
    /* 底部悬浮按钮 */
    .bottomBtn
        display flex
        mixinsWidthHeight(750rpx, 244rpx)
        align-items center
        background #FFFFFF
        justify-content center
        box-shadow 0 -8rpx 30rpx 0 rgba(223, 223, 223, 0.36)
        border-radius 16rpx 16rpx 0 0
        position fixed
        bottom 0
        .btnClose
            mixinsWidthHeight(662rpx, 110rpx)
            font-size 36rpx
            font-weight 600
            color #FFFFFF
            text-align center
            line-height 110rpx
            border-radius 64rpx
            background #C0C0C0
            font-family PingFangSC-Semibold, PingFang SC
            box-shadow 0 -8rpx 30rpx 0 rgba(223, 223, 223, 0.36), 0 4rpx 2rpx 0 rgba(212, 212, 212, 0.5)
        .available
            background: #2A4F44;
            animation btnFadeIn 0.3s
            animation-fill-mode forwards
        .unavailable
            animation btnFadeOut 0.3s
            animation-fill-mode forwards
