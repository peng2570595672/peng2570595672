.index {
  width: 100%;
  height: 100vh;
  background: #f6f6f6;
  margin: 0;
}
.index .top {
  width: 100%;
  padding: 40rpx;
  background: linear-gradient(180deg, #73cb5a 0%, rgba(132,209,87,0) 100%);
}
.index .top .bg-image {
  height: 45vh;
  background-image: url("https://file.cyzl.com/g001/M07/20/C1/oYYBAGBXGReAAL9iAAFY0ECq4wI874.png");
  background-size: cover;
}
.index .btn-content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  z-index: 111;
  width: 682rpx;
  border-radius: 20rpx;
  margin: -130rpx 30rpx 0rpx;
  background: #fff;
}
.index .btn-content .interior {
  margin-top: 30rpx;
}
.index .btn-content .interior image {
  margin-top: 20epx;
  width: 85rpx;
  height: 48rpx;
  padding-left: 300rpx;
}
.index .btn-content .online-tips {
  color: #33333d;
  font-size: 48rpx;
  margin-top: -16rpx;
}
.index .btn-content .memo-content {
  margin-top: 10rpx;
  margin-bottom: 40rpx;
  color: #66666d;
  font-size: 28rpx;
}
.index .btn-content .memo-content>text:nth-child(2) {
  color: #f7b500;
}
.index .btn-content .btn {
  display: flex;
  margin: 0rpx 30rpx;
  height: 100rpx;
  align-items: center;
  justify-content: center;
  background: linear-gradient(45deg, #14ac69 0%, #84d157 100%);
  box-shadow: 0rpx 4rpx 5rpx 0rpx rgba(132,209,87,0.2);
  border-radius: 50rpx;
  font-size: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #fff;
}
.index .btn-content .discount-tips {
  margin-top: 10rpx;
  margin-bottom: 30rpx;
  color: #404040;
  font-size: 28rpx;
}
.index .right-interest {
  background: #fff;
  box-shadow: 2rpx 2rpx 23rpx 0rpx rgba(126,132,156,0.1);
  border-radius: 32rpx;
  margin: 30rpx 30rpx 0rpx 30rpx;
  padding: 36rpx 30rpx;
}
.index .right-interest .interest-title {
  display: flex;
  align-items: center;
  font-size: 40rpx;
  height: 50rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #33333d;
}
.index .right-interest .interest-title::before {
  margin-right: 10rpx;
  content: '';
  width: 10rpx;
  height: 10rpx;
  border: 6rpx solid #14ac69;
  border-radius: 50%;
}
.index .right-interest .interest-title > text {
  margin-left: 12rpx;
  font-size: 24rpx;
  color: #7a7f83;
  transform: translateY(8rpx);
}
.index .right-interest .interests-list {
  display: flex;
  flex-wrap: wrap;
  margin-top: 40rpx;
}
.index .right-interest .interests-list .charge_item {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 290rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin-bottom: 24rpx;
  border: 1rpx solid rgba(217,236,209,0.35);
}
.index .right-interest .interests-list .charge_item .btn-charge-detail {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 80rpx;
  width: 50rpx;
  height: 22rpx;
  background: #1cae67;
  border-radius: 12rpx;
  opacity: 0.25;
  color: #fff;
  font-size: 14rpx;
}
.index .right-interest .interests-list .charge_item .charge-text {
  color: #404040;
  font-size: 24rpx;
}
.index .right-interest .interests-list .charge_item .charge-text.two {
  padding-bottom: 24rpx;
}
.index .right-interest .interests-list .charge_item .label {
  margin-right: 18rpx;
  font-size: 46rpx;
  color: #404040;
}
.index .right-interest .interests-list .charge_item .label.one::before {
  color: #000;
  display: block;
  content: '\e663';
}
.index .right-interest .interests-list .charge_item .label.two::before {
  color: #000;
  display: block;
  content: '\e664';
}
.index .right-interest .interests-list .charge_item .label.three::before {
  display: block;
  content: '\e666';
}
.index .right-interest .interests-list .charge_item .label.four::before {
  display: block;
  content: '\e667';
}
.index .right-interest .interests-list .charge_item:nth-child(odd) {
  margin-right: 20rpx;
}
.index .handle-and-process {
  margin: 30rpx 30rpx 0;
  background: #fff;
  box-shadow: 2rpx 2rpx 23rpx 0rpx rgba(126,132,156,0.1);
  border-radius: 32rpx;
  padding: 30rpx;
}
.index .handle-and-process .process-title {
  display: flex;
  align-items: center;
  font-size: 40rpx;
  height: 50rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #33333d;
}
.index .handle-and-process .process-title .sign {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-image: linear-gradient(48deg, #14ac69 0%, #84d157 100%);
  margin-right: 10rpx;
}
.index .handle-and-process .process-title .sign > text {
  display: block;
  width: 10rpx;
  height: 10rpx;
  background-color: #fff;
  border-radius: 50%;
}
.index .handle-and-process .material {
  margin-top: 40rpx;
  margin-left: 28rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #15ad69;
}
.index .handle-and-process .photo {
  display: flex;
  margin-top: 20rpx;
  background: #f3fff9;
  border-radius: 20rpx;
  padding: 40rpx 30rpx;
}
.index .handle-and-process .photo .photo-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex: 1;
}
.index .handle-and-process .photo .photo-item .img-box {
  margin-top: 40rpx;
  position: relative;
  padding-bottom: 10rpx;
}
.index .handle-and-process .photo .photo-item .img-box .img {
  width: 164rpx;
  height: 68rpx;
}
.index .handle-and-process .photo .photo-item .img-box .ico {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 36rpx;
  height: 30rpx;
}
.index .handle-and-process .photo .photo-item .image {
  width: 150rpx;
  height: 90rpx;
}
.index .handle-and-process .photo .photo-item > text {
  text-align: center;
  margin-top: 10rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #66666d;
}
.index .handle-and-process .photo .photo-item .memo {
  color: rgba(102,102,109,0.85);
  font-size: 12rpx;
}
.index .handle-and-process .progress_wrapper {
  margin-top: 60rpx;
}
.index .handle-and-process .progress_wrapper .item {
  position: relative;
  margin-left: 36rpx;
  padding-bottom: 40rpx;
  border-left: 1rpx dashed #a7a7a7;
}
.index .handle-and-process .progress_wrapper .item::before {
  position: absolute;
  top: -8rpx;
  left: -9rpx;
  content: '';
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #20b068;
}
.index .handle-and-process .progress_wrapper .item:last-of-type {
  border-left: 0;
}
.index .handle-and-process .progress_wrapper .item:last-of-type::before {
  left: -8rpx;
}
.index .handle-and-process .progress_wrapper .item .content-title {
  position: relative;
  padding-left: 26rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #15ad69;
  transform: translateY(-24rpx);
}
.index .handle-and-process .progress_wrapper .item .content-des {
  display: flex;
  margin-top: 10rpx;
  padding-left: 26rpx;
  padding-right: 80rpx;
  line-height: 40rpx;
  color: rgba(126,132,156,0.85);
  font-size: 24rpx;
  transform: translateY(-24rpx);
}
.index .handle-and-process .progress_wrapper .item .tab {
  display: flex;
  width: 360rpx;
  height: 120rpx;
  margin-left: 26rpx;
  border: 1rpx solid rgba(151,151,151,0.34);
}
.index .handle-and-process .progress_wrapper .item .tab .tab-item {
  display: flex;
  flex-direction: column;
  flex: 1;
  color: rgba(102,102,109,0.85);
  font-size: 20rpx;
  border-right: 1rpx solid #f3f3f3;
}
.index .handle-and-process .progress_wrapper .item .tab .tab-item:nth-child(3) {
  border-right: none !important;
}
.index .handle-and-process .progress_wrapper .item .tab .tab-item > view {
  text-align: center;
  margin-top: 14rpx;
}
.index .handle-and-process .progress_wrapper .item .tab .tab-item .first {
  height: 50rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  border-bottom: 1rpx solid #f3f3f3;
}
.index .handle-and-process .progress_wrapper .last::after {
  display: none !important;
}
.index .bottom {
  margin-top: 60rpx;
  margin-bottom: 160rpx;
  height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  color: rgba(3,158,235,0.85);
}
