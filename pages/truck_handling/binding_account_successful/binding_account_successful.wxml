<view class="binding_account">
    <view class="title iconfont">你已成功开通II账户</view>
    <view class="card">
        <view class="name iconfont">
            <text>中国工商银行信用卡</text>
        </view>
        <view class="card-no">{{bankCardInfo.accountNo}}</view>
    </view>
	<!--	按钮开始-->
	<view class="btn" catchtap="onclickSign"><view class="animate" wx:if="{{isRequest}}"></view>添加微信卡包并签约</view>
	<!--	按钮结束-->
    <view class="tips">需添加II类户至微信钱包方可预充保证金及继续办理</view>
</view>
