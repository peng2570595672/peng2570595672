.index {
  margin: 0;
  padding: 0;
  width: 100vw;
  min-height: 100vh;
  background-color: #fff;
}
.index::after {
  display: block;
  content: '';
  width: 100%;
  height: 260rpx;
}
.changeBg {
  background-color: #ececec;
}
.index .topSplit {
  width: 100%;
  height: 60rpx;
}
.index .module {
  width: 694rpx;
  border-radius: 16rpx;
  background-color: #fff;
  margin: 32rpx 0 0 0;
  font-family: PingFangSC-Regular, PingFang SC;
  position: relative;
  left: 50%;
  top: 0;
  transform: translate(-50%, 0);
}
.index .module .titles {
  height: 48rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #000;
  line-height: 48rpx;
  padding-left: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
}
.index .module .title2 {
  margin-bottom: 20rpx;
}
.index .boxS {
  box-shadow: 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.index .moduleOne {
  min-height: 100rpx;
  padding: 40rpx 0 20rpx 0;
}
.index .moduleOne.bg3 {
  background-color: #ececec;
}
.index .moduleOne .photo-wrapper {
  display: flex;
  height: 330rpx;
  justify-content: space-between;
}
.index .moduleOne .photo-wrapper .photo {
  width: 326rpx;
  height: 330rpx;
  text-align: center;
  border-radius: 16rpx;
  background: #f6f6f6;
}
.index .moduleOne .photo-wrapper .photo .upload {
  width: 90%;
  height: 192rpx;
  overflow: hidden;
  margin: 22rpx 5%;
  border-radius: 8rpx;
  background: #fafafa;
}
.index .moduleOne .photo-wrapper .photo .upload .img-box {
  width: 100%;
  height: 100%;
  position: relative;
}
.index .moduleOne .photo-wrapper .photo .upload .img-box .ico {
  width: 56rpx;
  height: 49rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.index .moduleOne .photo-wrapper .photo .upload .success {
  width: 100%;
  height: 100%;
}
.index .moduleOne .photo-wrapper .photo .changeThree {
  width: 100%;
  margin: 22rpx 0;
}
.index .moduleOne .photo-wrapper .photo:nth-of-type(1) .upload .img-box {
  background: url("https://file.cyzl.com/g001/M01/CA/4F/oYYBAGP8tVuAGQNnAAANbmC6Sdo207.png") center no-repeat;
  background-size: 80% 80%;
}
.index .moduleOne .photo-wrapper .photo:nth-of-type(2) .upload .img-box {
  background: url("https://file.cyzl.com/g001/M01/CA/4F/oYYBAGP8tYGAc0C7AAAL0oJkeVs625.png") center no-repeat;
  background-size: 80% 80%;
}
.index .moduleOne .photo-wrapper .photo .cardText {
  margin-top: 40rpx;
}
.index .moduleOne .photo-wrapper .changeTwo {
  width: 300rpx;
  background-color: #fff;
}
.index .moduleOne .changeOne {
  margin: 20rpx 36rpx 0 36rpx;
}
.index .moduleOne .titleBtn {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.index .moduleOne .titleBtn .radio {
  display: block;
  padding-right: 36rpx;
  display: flex;
  align-items: center;
  justify-content: normal;
  font-size: 22rpx;
  font-weight: normal;
  text-align: justify;
  letter-spacing: 0.4rpx;
  color: #222;
}
.index .moduleOne .titleBtn .radio .confirm {
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  border: 2rpx solid #ccc;
  margin-right: 8rpx;
}
.index .moduleOne .titleBtn .radio .tick::before {
  display: block;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 28rpx;
  height: 28rpx;
  content: '\e669';
  font-size: 16rpx;
  color: #fff;
  border-radius: 50%;
  background-color: #2a4f44;
}
.index .moduleOne .text1 {
  display: block;
  color: #ccc;
  font-size: 24rpx;
  padding: 20rpx 36rpx;
}
.index .moduleTwo {
  padding: 40rpx 0 20rpx 0;
}
.index .moduleTwo .keyValue {
  height: 66rpx;
  display: flex;
  margin-bottom: 28rpx;
  align-items: center;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .moduleTwo .keyValue .attrName {
  width: 124rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #1e1e1e;
  margin-left: 36rpx;
}
.index .moduleTwo .keyValue .attrValue {
  width: 508rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #e0e0e0;
}
.index .moduleTwo .keyValue .attrValue .input {
  width: 100%;
  padding-left: 24rpx;
  font-size: 28Rpx;
  font-weight: 600;
  color: #262626;
  opacity: 0.9;
  font-family: PingFangSC-Semibold, PingFang SC;
}
.index .moduleTwo .keyValue .attrValue .input-placeholder {
  color: rgba(212,212,212,0.7);
}
.index .module3 {
  height: 80rpx;
  background-color: #ccc;
}
.index .bottomBtn {
  display: flex;
  width: 750rpx;
  height: 244rpx;
  align-items: center;
  background: #fff;
  justify-content: center;
  box-shadow: 0 -8rpx 30rpx 0 rgba(223,223,223,0.36);
  border-radius: 16rpx 16rpx 0 0;
  position: fixed;
  bottom: 0;
}
.index .bottomBtn .btnClose {
  width: 662rpx;
  height: 110rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #fff;
  text-align: center;
  line-height: 110rpx;
  border-radius: 64rpx;
  background: #c0c0c0;
  font-family: PingFangSC-Semibold, PingFang SC;
  box-shadow: 0 -8rpx 30rpx 0 rgba(223,223,223,0.36), 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.index .bottomBtn .available {
  background: #2a4f44;
  -webkit-animation: btnFadeIn 0.3s;
          animation: btnFadeIn 0.3s;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
.index .bottomBtn .unavailable {
  -webkit-animation: btnFadeOut 0.3s;
          animation: btnFadeOut 0.3s;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
