/* pages/moving_integral/confirm_exchange/confirm_exchange.wxss */
.confirm_exchange {
  position: relative;
  
}

.main {
  height: 574rpx;
  /* 背景是一张图片 */
  background: linear-gradient(#00B85C, #F6F6F6);
  /* background: url(https://lanhu.oss-cn-beijing.aliyuncs.com/SketchPng0edfb641d2f7bcd8eb901a733ddc55247e76b2b538b2757fbb55d2c5cce99334); */
}

.main .progress_bar {
  width: 100%;
  height: 62rpx;
  display: flex;
  align-items: center;
  position: relative;
  top: 86rpx;
}

.bar_1,
.bar_2 {
  height: 12rpx;
  display: block;
  border-radius: 6rpx;
}

.main .progress_bar .bar_1 {
  width: 624rpx;
  margin: 0 58rpx;
  background-color: rgba(255, 255, 255, 0.6);
}

.main .progress_bar .bar_1 .bar_2 {
  width: 320rpx;
  background-color: #FFB33B;
}

.main .progress_bar .exchange_tip,
.main .progress_bar .picure_1 {
  position: absolute;
  text-align: center;
  font-size: 20rpx;
}

.main .progress_bar .picure_1 {
  width: 72rpx;
  height: 62rpx;
  background-color: pink;
}

.main .progress_bar .picure_1:nth-of-type(1) {
  left: 90rpx;
}

.main .progress_bar .picure_1:nth-of-type(3) {
  left: 334rpx;
}

.main .progress_bar .picure_1:nth-of-type(5) {
  left: 580rpx;
}

.main .progress_bar .exchange_tip {
  width: 82rpx;
  height: 28rpx;
  color: #fff;
  border-radius: 12rpx;
  border: 4rpx solid #0DBB64;
}

.main .progress_bar .exchange_tip:nth-of-type(2) {
  background-color: #FFB33B;
  left: 206rpx;
}

.main .progress_bar .exchange_tip:nth-of-type(4) {
  background-color: #2AC376;
  left: 452rpx;
}


.main .text_1 {
  width: 100%;
  height: 32rpx;
  position: relative;
  top: 104rpx;
}

.main .text_1 text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  position: absolute;
}

.main .text_1 text:nth-of-type(1) {
  left: 82rpx;
}

.main .text_1 text:nth-of-type(2) {
  left: 348rpx;
}

.main .text_1 text:nth-of-type(3) {
  right: 84rpx;
}

.main .text_1 text:nth-of-type(4) {
  top: 36rpx;
  right: 320rpx;
}

.main .pass {
  width: 686rpx;
  height: 174rpx;
  display: flex;
  background-color: #fff;
  position: relative;
  top: 200rpx;
  left: 32rpx;
}

.main .pass view:nth-of-type(1) text {
  position: absolute;
  left: 44rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}

.main .pass view:nth-of-type(1) text:nth-of-type(1) {
  top: 24rpx;
  color: #FFB33B;
  font-size: 28rpx;
  font-weight: 400;
}

.main .pass view:nth-of-type(1) text:nth-of-type(2) {
  top: 56rpx;
  color: #00B85C;
  font-size: 68rpx;
  font-weight: 600;
}

.main .pass view:nth-of-type(2) {
  width: 270rpx;
  height: 70rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #7B7B7B;
  line-height: 35rpx;
  position: absolute;
  top: 54rpx;
  right: 122rpx;
}


.main_1 view:nth-of-type(1),
.main_1 view:nth-of-type(2) {
  width: 686rpx;
  height: 86rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.5000);
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
  position: relative;
  left: 32rpx;
}

.main_1 view:nth-of-type(1) text,
.main_1 view:nth-of-type(2) text {
  position: absolute;
  top: 32rpx;
}

.main_1 view:nth-of-type(1) text:nth-of-type(2),
.main_1 view:nth-of-type(2) text:nth-of-type(2) {
  left: 144rpx;
}

.main_1 view:nth-of-type(2) button {
  width: 114rpx;
  height: 52rpx;
  font-size: 24rpx;
  font-weight: 400;
  line-height: 52rpx;
  color: #FFFFFF;
  background-color: #FFB33B;
  border-radius: 4rpx;
  position: absolute;
  top: 12rpx;
  right: 74rpx;
}

.main_1 view:nth-of-type(3) {
  width: 686rpx;
  height: 114rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 192, 0, 1);
  line-height: 38rpx;
  position: relative;
  top: 32rpx;
  left: 32rpx;
}

.confirm_exchange>.ok {
  width: 610rpx;
  height: 98rpx;
  background: linear-gradient(to right, #00AF62, #68D33F);
  box-shadow: 0px 8rpx 10rpx 0px rgba(104, 211, 63, 0.2000);
  border-radius: 50rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 98rpx;
  position: fixed;
  bottom: 100rpx;
  left: 70rpx;
}
