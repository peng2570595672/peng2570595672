@charset "UTF-8"
// 以下配置用于stylus编译后不添加前缀 如@-moz-keyframes 小程序会报错
vendors = official
mixinsWidthHeight($width = 100%, $height = 100%)
    width $width
    height $height
mixinsIconfont($content='',$size=16px,$color=#5EBE68)
    content $content
    font-size $size
    color $color
mixinsPositionPack($position=relative)
    position $position
    left 50%
    transform translateX(-50%)
.iconfont
	font-family 'iconfont' !important
	font-size 16px
	font-style normal
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
.index
	margin 0
	padding 30rpx 0 0
	width 100vw
	min-height 100vh
	box-sizing border-box
	background-color #ECECEC
	/* 顶部组件占位 */
	.moduleZero
		mixinsWidthHeight(,60rpx)
	/* 公用模块 */
	.module
		padding 0 37rpx 44rpx 37rpx
		border-radius 16rpx
		background #FFFFFF
		margin 0 28rpx
		box-shadow 0 4rpx 2rpx 0 rgba(212, 212, 212, 0.5)
		.title
			display flex
			padding-top 40rpx
			align-items center
			font-family PingFangSC-Semibold, PingFang SC
			.title-left
				flex 1
				display flex
				align-items center
				.titles
					font-size 32rpx
					font-weight 600
					color #1E1E1E
					line-height 44rpx
				.colorBrand
					mixinsWidthHeight(62rpx,34rpx)
					font-size 24rpx
					font-weight 400
					color #FFFFFF
					line-height 34rpx
					text-align center
					border-radius 8rpx
					margin-left 8rpx
					&.temporaryLicensePlate
						border: 1px solid #56C582;
						color #56C582
			.hasVeh
				display flex
				align-items center
				font-size: 22rpx;
				font-weight: normal;
				color: #222222;
				&::before
					content ''
					width: 28rpx;
					height: 28rpx;
					border-radius: 50%;
					border: 1rpx solid #969696;
					margin-right 8rpx
				&.notVeh
					&::before
						content '\e655'
						font-size 28rpx
						border-radius: 50%;
						color #2A4F44
						border 0
	/* 模块一   办理车牌号 */
	.moduleOne
		.input
