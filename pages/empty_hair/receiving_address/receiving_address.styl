				border 2rpx solid #ECFFF4
		.moduleOneTip
			font-size 24rpx
			font-weight 400
			color #1E1E1E
			line-height 34rpx
			margin-top 26rpx 
			font-family PingFangSC-Regular, PingFang SC
			.tipsOne
				color #F19C38
		
	/* 公共模块 */
	.module
		margin 42rpx
		.tipPhone
