							background-size 40rpx 40rpx
						.icon
							&::before
								mixinsWidthHeight(40rpx,40rpx)
								display block
								mixinsIconfont('\e6a1',24rpx,#E9CA7B)
						.text
							font-size 28rpx
							font-weight 600
							color #E9CA7B
							line-height 39rpx
							margin-left 10rpx
						.greenIcon
							&::before
								mixinsWidthHeight(40rpx,40rpx)
