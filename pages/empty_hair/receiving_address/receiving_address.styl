						top -10rpx
						right 0
					.planSpan
						padding-left 28rpx
				.imgExplain
					height auto
					&::before
						border 0
					image
						width 632rpx
				.subTitle
					height 40rpx
					font-size 28rpx
					font-weight 400
					color #262626
					margin-top 10rpx
					line-height 40rpx
					padding 15rpx 0
					border-bottom 2rpx solid #F6F6F6
				.description
					display flex
					flex-wrap wrap
					font-size 24rpx
					font-weight 400
					color #262626
					justify-content space-between
