						color #5EBE68
			&:nth-of-type(4)
				.publicCoderight
					.input
						width 100%
	.moduleThree
		margin-bottom 300rpx

	// 套餐模块
	.package
		background-color transparent
		padding-bottom  0 !important
		.module1
