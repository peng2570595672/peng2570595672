								mixinsIconfont('\e6a1',24rpx,#54B95C)
					.details
						color #00B85C
						mixinsWidthHeight(60rpx,40rpx)
						font-size 28rpx
						font-weight 400
						padding 10rpx 0rpx
						font-family PingFangSC-Regular, PingFang SC
		.btnHighLight
			border 4rpx solid #E9CA7B
