.container
	min-height 100vh
	padding 62rpx 44rpx
	text-align center
	box-sizing border-box
	background #fff
	.logo
		padding-bottom 30rpx
		image
			width 588rpx
	.title
		font-size: 32rpx;
		font-family: PingFang SC-Regular, PingFang SC;
		font-weight: 400;
		color: #262626;
		line-height: 34rpx;
		&.top-title
			display flex
			align-items center
			padding 0 5rpx
			height 56rpx
			font-size 40rpx
			font-weight 500
			&:before
				margin-right 10rpx
				content ''
				width 10rpx
				height 10rpx
				border 6rpx solid #54B95C
				border-radius 50%
	.sub-title
		font-size: 24rpx;
		font-family: PingFang SC-Regular, PingFang SC;
		font-weight: 400;
		color: #979797;
		line-height: 36rpx;
		margin-top 14rpx
		margin-bottom 80rpx
		&.top-sub-title
			text-align left
			margin-left 30rpx
	.tips
		display flex
		align-items flex-start
		justify-content center
		font-size: 24rpx;
		padding-left 62rpx
		font-family: PingFang SC-Regular, PingFang SC;
		font-weight: 400;
		color: #EC602A;
		line-height: 36rpx;
	.btn
		height: 110rpx;
		display flex
		align-items center
		justify-content center
		background: #2A4F44;
		box-shadow: 0rpx 4rpx 2rpx 0rpx rgba(211.5760973095894,211.5761125087738,211.5761125087738,0.5);
		border-radius: 55rpx 55rpx 55rpx 55rpx;
		font-size: 36rpx;
		font-family: PingFang SC-Semibold, PingFang SC;
		font-weight: 600;
		color: #FFFFFF;
		margin-top 40rpx
.alert-for-not-finished
	position: fixed
	left: 0
	right: 0
	top: 0
	bottom: 0
	background: rgba(0, 0, 0, 0.6)
	z-index: 10000
.alert-for-not-finished-content
	position fixed
	left 0
	right 0
	top 0
	bottom 0
	width 694rpx
	height 752rpx
	text-align justify
	background rgba(255,255,255,1)
	box-shadow 4rpx 12rpx 56rpx 0px rgba(102,102,109,0.19)
	border-radius 16rpx
	margin auto
	z-index 100002
	.dialog-container
		.wx-dialog-title
			text-align center
			font-size: 44rpx;
			font-family: PingFang SC-Medium, PingFang SC;
			font-weight: 500;
			color: #1E1E1E;
			line-height: 52rpx;
			padding 48rpx 0 34rpx
		.wx-dialog-con
			padding 0 48rpx
			text-align center
			font-size: 32rpx;
			font-family: PingFang SC-Regular, PingFang SC;
			font-weight: 400;
			color: #262626;
			line-height: 54rpx;
			image
				width 590rpx
				height 234rpx
				margin-bottom 24rpx
			.step1
				margin-bottom 8rpx
			.step2
				color #979797
		.fill
			flex 1
		.wx-dialog-btn
			display flex
			justify-content center
			padding-top 40rpx
			padding-bottom 48rpx
			.confirm
				width: 318rpx;
				height: 106rpx;
				display flex
				align-items center
				justify-content center
				background: #2A4F44;
				border-radius: 53rpx 53rpx 53rpx 53rpx;
				font-size: 36rpx;
				font-family: PingFang SC-Regular, PingFang SC;
				font-weight: 400;
				color: #FFFFFF;
