<view class="receiving_address">
	<!--	车牌号标题开始-->
	<view class="title">车牌号</view>
	<!--	车牌号标题结束-->
	<!--	车牌号开始-->
	<view class="car-no-wrapper">
		<view class="{{currentIndex === 0 ? 'selected' : ''}}" data-index="0" catchtap="setCurrentCarNo">
			<view>{{carNo[0]}}</view>
			<view></view>
		</view>
		<view class="{{currentIndex === 1 ? 'selected' : ''}}" data-index="1" catchtap="setCurrentCarNo">
			<view>{{carNo[1]}}</view>
			<view></view>
		</view>
		<view class="{{currentIndex === 2 ? 'selected' : ''}}" data-index="2" catchtap="setCurrentCarNo">
			<view>{{carNo[2]}}</view>
			<view></view>
		</view>
		<view class="{{currentIndex === 3 ? 'selected' : ''}}" data-index="3" catchtap="setCurrentCarNo">
			<view>{{carNo[3]}}</view>
			<view></view>
		</view>
		<view class="{{currentIndex === 4 ? 'selected' : ''}}" data-index="4" catchtap="setCurrentCarNo">
			<view>{{carNo[4]}}</view>
			<view></view>
		</view>
		<view class="{{currentIndex === 5 ? 'selected' : ''}}" data-index="5" catchtap="setCurrentCarNo">
			<view>{{carNo[5]}}</view>
			<view></view>
		</view>
		<view class="{{currentIndex === 6 ? 'selected' : ''}}" data-index="6" catchtap="setCurrentCarNo">
			<view>{{carNo[6]}}</view>
			<view></view>
		</view>
		<view wx:if="{{currentIndex === 7 || carNo[7]}}" class="{{currentIndex === 7 ? 'selected' : ''}}" data-index="7" catchtap="setCurrentCarNo">
			<view>{{carNo[7]}}</view>
			<view></view>
		</view>
		<view wx:else class="unselected" data-index="7" catchtap="setCurrentCarNo">
			<view>+</view>
			<view>新能源</view>
		</view>
	</view>
	<!--	车牌号结束-->
	<!--车牌颜色开始	-->
	<view class="choose-car-no-color">
		<view>选择车牌颜色</view>
		<view class="color-wrapper">
			<view class="blue selected">蓝色车牌</view>
			<view class="green">渐变绿车牌</view>
			<view class="yellow">黄色车牌</view></view>
	</view>
	<!--车牌颜色结束	-->
	<!--	分割线开始-->
	<view class="split"></view>
	<!--	分割线结束-->
	<!--	邮寄地址开始-->
	<view class="title">邮寄地址<text>用于接收ETC设备</text></view>
	<!--	邮寄地址结束-->
	<!--	输入开始-->
	<view class="input-wrapper">
		<view class="input-item">
			<view>姓名</view>
			<input type="text" placeholder="请输入收货人姓名" placeholder-class="placeholder"/>
			<text class="auto-fill">自动填写</text>
		</view>
		<view class="input-item">
			<view>收货地区</view>
			<input type="text" placeholder="选择省 市 区" placeholder-class="placeholder"/>
			<text class="iconfont"></text>
		</view>
		<view class="input-item">
			<view>详细地址</view>
			<input type="text" placeholder="填写详细地址信息" placeholder-class="placeholder"/>
		</view>
		<view class="input-item">
			<view>手机号</view>
			<input type="number" placeholder="请输入手机号" placeholder-class="placeholder"/>
		</view>
		<view class="input-item">
			<view>验证码</view>
			<input type="number" placeholder="输入验证码" placeholder-class="placeholder"/>
			<text class="validate-code">获取验证码</text>
		</view>
	</view>
	<!--	输入结束-->
	<view class="fill"></view>
	<!--	协议开始-->
	<view class="agreement iconfont selected">我已阅读并同意<text>《ETC+办理协议》</text></view>
	<!--	协议结束-->
	<!--	按钮开始-->
	<view class="btn" catchtap="next">下一步</view>
	<!--	按钮结束-->
	<!--	键盘-->
	<keyboard id="keyboard" index="{{currentIndex}}" show="{{showKeyboard}}" bind:valueChange="valueChange" myCar="{{carNo}}"></keyboard>
</view>
