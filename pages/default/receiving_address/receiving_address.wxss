.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  .index {
    margin: 0;
    padding: 0;
    width: 100vw;
    min-height: 100vh;
    background-color: #ECECEC;
  }
  
  /* 顶部组件占位 */
  .index .topSplit {
    width: 100%;
    height: 60rpx;
  }
  
  /* 公用模块 */
  .index .module {
    padding: 0rpx 37rpx 44rpx 37rpx;
    border-radius: 16rpx;
    background: #FFFFFF;
    margin: 28rpx 28rpx 0rpx 28rpx;
    box-shadow: 0rpx 4rpx 2rpx 0rpx rgba(212, 212, 212, 0.5);
  }
  
  .index .module .title {
    display: flex;
    padding-top: 40rpx;
    align-items: center;
    font-family: PingFangSC-Semibold, PingFang SC;
  }
  
  .index .module .title .titles {
    font-size: 32rpx;
    font-weight: 600;
    color: #1E1E1E;
    line-height: 44rpx;
  }
  
  .index .module .title .colorBrand {
    width: 96rpx;
    height: 36rpx;
    font-size: 22rpx;
    font-weight: 400;
    color: #FFFFFF;
    line-height: 36rpx;
    text-align: center;
    margin-left: 20rpx;
    border-radius: 4rpx;
  }
  
  /* 模块一   办理车牌号 */
  .index .moduleOne {
    margin-top: 42rpx;
  }
  
  .index .moduleOne .input {
    height: 80rpx;
    display: flex;
    color: #1E1E1E;
    margin-top: 18rpx;
    justify-content: space-between;
  }
  
  .index .moduleOne .input .inputSub {
    width: 64rpx;
    height: 76rpx;
    font-size: 44rpx;
    font-weight: 400;
    color: #1E1E1E;
    text-align: center;
    line-height: 72rpx;
    border-radius: 16rpx;
    background: #F7F7F7;
    border: 2rpx solid #F7F7F7;
    font-family: PingFangSC-Regular, PingFang SC;
  }
  
  .index .moduleOne .input .inputSub .newEnergy {
    font-size: 12rpx;
    font-weight: 600;
    color: #5EBE68;
    font-family: PingFangSC-Semibold, PingFang SC;
  }
  
  .index .moduleOne .input .inputSub .newEnergy .ico::before {
    display: block;
    font-size: 24rpx;
    content: '\e63d';
    color: #F19C38;
    font-weight: bold;
    margin-top: -10rpx;
    transform: rotateZ(45deg);
  }
  
  .index .moduleOne .input .inputSub .newEnergy .text {
    margin-top: -40rpx;
  }
  
  .index .moduleOne .input .special {
    background-color: #fff;
    border: 2rpx dashed #F19C38;
  }
  
  .index .moduleOne .input .select {
    border: 2rpx solid #5EBE68 !important;
  }
  
  .index .moduleOne .input .blueCard {
    background: #ECF7FF;
  }
  
  .index .moduleOne .input .greenCard {
    background: #ECFFF4;
  }
  
  .index .moduleOne .input .noBorder {
    border: 2rpx solid #ECFFF4;
  }
  
  .index .moduleOne .moduleOneTip {
    font-size: 24rpx;
    font-weight: 400;
    color: #1E1E1E;
    line-height: 34rpx;
    margin-top: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
  }
  
  .index .moduleOne .moduleOneTip .tipsOne {
    color: #F19C38;
  }
  
  /* 公共模块 */
  .index .module .publicCode {
    height: 75rpx;
    display: flex;
    margin-top: 18rpx;
    align-items: center;
    justify-content: space-between;
    font-family: PingFangSC-Regular, PingFang SC;
  }
  
  .index .module .publicCode .publicCodeLeft {
    width: 140rpx;
    font-size: 32rpx;
    font-weight: 400;
    color: #1E1E1E;
    text-align: right;
  }
  
  .index .module .publicCode .publicCoderight {
    width: 520rpx;
    height: 66rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 2rpx solid #E0E0E0;
    position: relative;
  }
  
  .index .module .publicCode .publicCoderight .input {
    margin-left: 20rpx;
    color: #1E1E1E;
    font-size: 32rpx;
    font-weight: 400;
  }
  
  .index .moduleThree .publicCode:nth-of-type(4) .publicCoderight .input {
    width: 100%;
  }
  
  .index .module .publicCode .publicCoderight .input-placeholder {
    font-size: 32rpx;
    font-weight: 400;
    color: rgba(0,0,0,0.5);
    letter-spacing: 3rpx;
    font-family: PingFangSC-Regular, PingFang SC;
  }
  
  .index .module .publicCode .publicCoderight .btns {
    height: 45rpx;
    color: #fff;
    font-size: 16rpx;
    font-weight: 400;
    text-align: center;
    line-height: 45rpx;
    padding: 0rpx 20rpx;
    border-radius: 24rpx;
    background: #5EBE68;
    display: flex;
  }
  
  .index .module .publicCode .publicCoderight .errTip {
    font-size: 18rpx;
    font-weight: 400;
    color: #F19C38;
    line-height: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    position: absolute;
    left: 0rpx;
    bottom: -30rpx;
  }
  
  .index .module .publicCode .publicCoderight .address {
    width: 45rpx;
    height: 45rpx;
    padding: 0rpx;
    background-color: #FFFFFF;
  }
  
  .index .module .publicCode .publicCoderight .address::before {
    display: inline-block;
    content: '\e64d';
    font-size: 30rpx;
    color: #5EBE68;
  }
  
  .index .module .publicCode .publicCoderight .btns .text {
    min-width: 100rpx;
    display: block;
  }
  
  .index .module .publicCode .publicCoderight .btns .wChat::before {
    display: block;
    content: '\e6b1';
    font-size: 25rpx;
    color: #f0f4f1;
  }
  
  /* 占位 */
  .index .split {
    width: 100%;
    height: 280rpx;
  }
  
  /* 底部悬浮按钮 */
  .index .floatButton {
    width: 100%;
    opacity: 1;
    display: flex;
    height: 244rpx;
    align-items: center;
    background: #ffffff;
    justify-content: center;
    border-radius: 16px 16px 0px 0px;
    box-shadow: 0px -8px 30px 0px rgba(223, 223, 223, 0.36);
    position: fixed;
    bottom: 0rpx;
    z-index: 100;
  }
  
  .index .floatButton .handleImmediately {
    width: 662rpx;
    height: 110rpx;
    font-size: 36rpx;
    font-weight: 600;
    color: #FFFFFF;
    text-align: center;
    line-height: 110rpx;
    background: #C0C0C0;
    border-radius: 64rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    text-shadow: 0px -8rpx 30rpx rgba(223, 223, 223, 0.36);
    box-shadow: 0px -8rpx 30rpx 0rpx rgba(223, 223, 223, 0.36), 0rpx 4rpx 2rpx 0prx rgba(212, 212, 212, 0);
  }
  
  .index .floatButton .btn {
    background: #2A4F44;
  }