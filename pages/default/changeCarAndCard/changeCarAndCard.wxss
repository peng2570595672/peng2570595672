.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.index {
  margin: 0;
  padding: 0;
  width: 100vw;
  min-height: 100vh;
  background-color: #ececec;
  padding-top: 30rpx;
/* 公用模块 */
/* 模块一   办理车牌号 */
/* 公共模块 */
/* 占位 */
/* 底部悬浮按钮 */
}
.index .module {
  padding: 0 32rpx 44rpx;
  border-radius: 16rpx;
  background: #fff;
  margin: 28rpx 28rpx 0 28rpx;
  box-shadow: 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.index .module .title {
  display: flex;
  padding-top: 40rpx;
  align-items: flex-start;
  flex-direction: column;
  font-family: PingFangSC-Semibold, PingFang SC;
}
.index .module .title .titles {
  font-size: 32rpx;
  font-weight: 600;
  color: #1e1e1e;
  line-height: 84rpx;
}
.index .module .title .card_car {
  display: flex;
  padding: 3rpx;
  justify-content: space-between;
  width: 100%;
}
.index .module .title .card_car .image_pic {
  width: 45%;
  border-radius: 10rpx;
  padding: 5rpx;
}
.index .module .title .card_car .image_pic.active {
  border: 3px solid #5ebe68;
}
.index .module .title .card_car .image_pic image {
  width: 100%;
  height: 100%;
  display: block;
}
.index .module .title .card_car .image_pic .text {
  margin-top: -20rpx;
  text-align: center;
  width: 100%;
  height: 56rpx;
  line-height: 56rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.index .module .title .colorBrand {
  width: 96rpx;
  height: 36rpx;
  font-size: 22rpx;
  font-weight: 400;
  color: #fff;
  line-height: 36rpx;
  text-align: center;
  margin-left: 20rpx;
  border-radius: 4rpx;
}
.index .moduleOne {
  margin-top: 42rpx;
}
.index .moduleOne .input {
  height: 80rpx;
  display: flex;
  color: #1e1e1e;
  margin-top: 18rpx;
  justify-content: space-between;
}
.index .moduleOne .input .inputSub {
  width: 64rpx;
  height: 76rpx;
  font-size: 44rpx;
  font-weight: 400;
  color: #1e1e1e;
  text-align: center;
  line-height: 72rpx;
  border-radius: 16rpx;
  background: #f7f7f7;
  border: 2rpx solid #f7f7f7;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .moduleOne .input .inputSub .newEnergy {
  font-size: 12rpx;
  font-weight: 600;
  color: #5ebe68;
  font-family: PingFangSC-Semibold, PingFang SC;
}
.index .moduleOne .input .inputSub .newEnergy .ico::before {
  display: block;
  font-size: 24rpx;
  content: '\e63d';
  color: #f19c38;
  font-weight: bold;
  margin-top: -10rpx;
  transform: rotateZ(45deg);
}
.index .moduleOne .input .inputSub .newEnergy .text {
  margin-top: -40rpx;
}
.index .moduleOne .input .special {
  background-color: #fff;
  border: 2rpx dashed #f19c38;
}
.index .moduleOne .input .special .select {
  border: 2rpx solid #5ebe68 !important;
}
.index .moduleOne .input .blueCard {
  background: #ecf7ff;
}
.index .moduleOne .input .greenCard {
  background: #ecfff4;
}
.index .moduleOne .input .noBorder {
  border: 2rpx solid #ecfff4;
}
.index .moduleOne .changeCarText {
  font-size: 34rpx;
  margin-top: 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.index .moduleOne .moduleOneTip {
  font-size: 24rpx;
  font-weight: 400;
  color: #1e1e1e;
  line-height: 34rpx;
  margin-top: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .moduleOne .moduleOneTip .tipsOne {
  color: #f19c38;
}
.index .module .tipPhone {
  width: 100%;
  text-align: right;
  font-size: 24rpx;
  font-weight: 400;
  color: #1e1e1e;
  line-height: 34rpx;
  margin-top: 16rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .module .publicCode {
  height: 75rpx;
  display: flex;
  margin-top: 18rpx;
  align-items: center;
  justify-content: space-between;
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .module .publicCode .publicCodeLeft {
  width: 140rpx;
  font-size: 32rpx;
  font-weight: 400;
  color: #1e1e1e;
  text-align: right;
}
.index .module .publicCode .publicCoderight {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2rpx solid #e0e0e0;
  position: relative;
}
.index .module .publicCode .publicCoderight .input {
  margin-left: 20rpx;
  color: #1e1e1e;
  font-size: 28rpx;
  font-weight: 600;
}
.index .module .publicCode .publicCoderight .input-phone,
.index .module .publicCode .publicCoderight .input-placeholder {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(0,0,0,0.5);
  font-family: PingFangSC-Regular, PingFang SC;
}
.index .module .publicCode .publicCoderight .get-phone,
.index .module .publicCode .publicCoderight .btns {
  height: 46rpx;
  color: #fff;
  font-size: 16rpx;
  font-weight: 400;
  text-align: center;
  line-height: 46rpx;
  padding: 0 20rpx;
  border-radius: 24rpx;
  background: #5ebe68;
  display: flex;
}
.index .module .publicCode .publicCoderight .get-phone .text,
.index .module .publicCode .publicCoderight .btns .text {
  min-width: 100rpx;
  display: block;
}
.index .module .publicCode .publicCoderight .get-phone .wChat::before,
.index .module .publicCode .publicCoderight .btns .wChat::before {
  display: block;
  content: '\e6b1';
  font-size: 25rpx;
  color: #f0f4f1;
}
.index .module .publicCode .publicCoderight .get-phone {
  width: 162rpx;
  justify-content: center;
  box-sizing: border-box;
}
.index .module .publicCode .publicCoderight .errTip {
  font-size: 18rpx;
  font-weight: 400;
  color: #f19c38;
  line-height: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  position: absolute;
  left: 0;
  bottom: -30rpx;
}
.index .module .publicCode .publicCoderight .address {
  width: 45rpx;
  height: 45rpx;
  padding: 0;
  background-color: #fff;
}
.index .module .publicCode .publicCoderight .address::before {
  display: inline-block;
  content: '\e64d';
  font-size: 30rpx;
  color: #5ebe68;
}
.index .module .publicCode:nth-of-type(4) .publicCoderight .input {
  width: 100%;
}
.index .split {
  width: 100%;
  height: 280rpx;
}
.index .floatButton {
  width: 100%;
  height: 204rpx;
  opacity: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 16px 16px 0 0;
  box-shadow: 0 -8px 30px 0 rgba(223,223,223,0.36);
  position: fixed;
  bottom: 0;
  z-index: 100;
}
.index .floatButton .handleImmediately {
  width: 662rpx;
  height: 110rpx;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 110rpx;
  background: #c0c0c0;
  border-radius: 64rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  text-shadow: 0 -8rpx 30rpx rgba(223,223,223,0.36);
  box-shadow: 0 -8rpx 30rpx 0 rgba(223,223,223,0.36), 0 4rpx 2rpx 0 rgba(212,212,212,0);
}
.index .floatButton .btn {
  background: #2a4f44;
}
