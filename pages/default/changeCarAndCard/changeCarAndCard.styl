mixinsWidthHeight($width=100%, $height=100%)
    width $width
    height $height
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
.index
    margin 0
    padding 0
    width 100vw
    min-height 100vh
    background-color #ECECEC
    padding-top   30rpx
    /* 公用模块 */
    .module
        padding 0 32rpx 44rpx
        border-radius 16rpx
        background #FFFFFF
        margin 28rpx 28rpx 0 28rpx
        box-shadow 0 4rpx 2rpx 0 rgba(212, 212, 212, 0.5)
        .title
            display flex
            padding-top 40rpx
            align-items flex-start
            flex-direction column
            font-family PingFangSC-Semibold, PingFang SC
            .titles
                font-size 32rpx
                font-weight 600
                color #1E1E1E
                line-height 84rpx
            .card_car
                display flex
                padding 3rpx
                justify-content space-between
                width 100%
                .active
                    border 2px solid #5EBE68
                .image_pic
                    width 45%
                    border-radius 10rpx
                    padding 5rpx
                    image
                        width 100%
                        height 100%
                        display block
                    .text
                        margin 0 auto
                        text-align center
                        margin-top -40rpx
                        width 210rpx
                        font-family: '苹方-简';
                        height 56rpx
                        font-size 30rpx
                        color #fff
                        line-height 56rpx
                        background-color #9BD390
                        border-radius 40rpx
            .colorBrand
                mixinsWidthHeight(96rpx,36rpx)
                font-size 22rpx
                font-weight 400
                color #FFFFFF
                line-height 36rpx
                text-align center
                margin-left 20rpx
                border-radius 4rpx
    /* 模块一   办理车牌号 */
    .moduleOne
        margin-top 42rpx
        .input
            height 80rpx
            display flex
            color #1E1E1E
            margin-top 18rpx
            justify-content space-between
            .inputSub
                mixinsWidthHeight(64rpx,76rpx)
                font-size 44rpx
                font-weight 400
                color #1E1E1E
                text-align center
                line-height 72rpx
                border-radius 16rpx
                background #F7F7F7
                border 2rpx solid #F7F7F7
                font-family PingFangSC-Regular, PingFang SC
                .newEnergy
                    font-size 12rpx
                    font-weight 600
                    color #5EBE68
                    font-family PingFangSC-Semibold, PingFang SC
                    .ico
                        &::before
                            display block
                            font-size 24rpx
                            content '\e63d'
                            color #F19C38
                            font-weight bold
                            margin-top -10rpx
                            transform rotateZ(45deg)
                    .text
                        margin-top -40rpx
            .special
                background-color #fff
                border 2rpx dashed #F19C38
                .select
                    border 2rpx solid #5EBE68 !important
            .blueCard
                background #ECF7FF
            .greenCard
                background #ECFFF4
            .noBorder
                border 2rpx solid #ECFFF4
        .changeCarText
            font-size 34rpx
            margin-top 30rpx
            height 100rpx
            line-height 100rpx
            text-align center 
        .moduleOneTip
            font-size 24rpx
            font-weight 400
            color #1E1E1E
            line-height 34rpx
            margin-top 26rpx
            font-family PingFangSC-Regular, PingFang SC
            .tipsOne
                color #F19C38
        
    /* 公共模块 */
    .module
        .tipPhone
            width 100%
            text-align right
            font-size 24rpx
            font-weight 400
            color #1E1E1E
            line-height 34rpx
            margin-top 16rpx
            font-family PingFangSC-Regular, PingFang SC
        .publicCode
            height 75rpx
            display flex
            margin-top 18rpx
            align-items center
            justify-content space-between
            font-family PingFangSC-Regular, PingFang SC
            .publicCodeLeft
                width 140rpx
                font-size 32rpx
                font-weight 400
                color #1E1E1E
                text-align right
            .publicCoderight
                //mixinsWidthHeight(520rpx,66rpx)
                flex 1
                display flex
                align-items center
                justify-content space-between
                border-bottom 2rpx solid #E0E0E0
                position relative
                .input
                    margin-left 20rpx
                    color #1E1E1E
                    font-size 28rpx
                    font-weight 600
                .input-phone,
                .input-placeholder
                    font-size 28rpx
                    font-weight 400
                    color rgba(0, 0, 0, 0.5)
                    font-family PingFangSC-Regular, PingFang SC
                .get-phone,
                .btns
                    height 46rpx
                    color #fff
                    font-size 16rpx
                    font-weight 400
                    text-align center
                    line-height 46rpx
                    padding 0 20rpx
                    border-radius 24rpx
                    background #5EBE68
                    display flex
                    .text
                        min-width 100rpx
                        display block
                    .wChat
                        &::before
                            display block
                            content '\e6b1'
                            font-size 25rpx
                            color #f0f4f1
                .get-phone
                    width 162rpx
                    justify-content center
                    box-sizing border-box
                .errTip
                    font-size 18rpx
                    font-weight 400
                    color #F19C38
                    line-height 26rpx
                    font-family PingFangSC-Regular, PingFang SC
                    position absolute
                    left 0
                    bottom -30rpx
                .address
                    width 45rpx
                    height 45rpx
                    padding 0
                    background-color #FFFFFF
                    &::before
                        display inline-block
                        content '\e64d'
                        font-size 30rpx
                        color #5EBE68
            &:nth-of-type(4)
                .publicCoderight
                    .input
                        width 100%
    /* 占位 */
    .split
        mixinsWidthHeight(,280rpx)
    /* 底部悬浮按钮 */
    .floatButton
        mixinsWidthHeight(,204rpx)
        opacity 1
        display flex
        align-items center
        justify-content center
        border-radius 16px 16px 0 0
        box-shadow 0 -8px 30px 0 rgba(223, 223, 223, 0.36)
        position fixed
        bottom 0
        z-index 100
        .handleImmediately
            mixinsWidthHeight(662rpx,110rpx)
            font-size 36rpx
            color #FFFFFF
            text-align center
            line-height 110rpx
            background #C0C0C0
            border-radius 64rpx
            font-family PingFangSC-Semibold, PingFang SC
            text-shadow 0 -8rpx 30rpx rgba(223, 223, 223, 0.36)
            box-shadow 0 -8rpx 30rpx 0 rgba(223, 223, 223, 0.36), 0 4rpx 2rpx 0 rgba(212, 212, 212, 0)
        .btn
            background #2A4F44
