mixinsWidthHeight($width=100%, $height=100%)
    width $width
    height $height
mixinsIconfont($content='',$size=16px,$color=#5EBE68)
    content $content
    font-size $size
    color $color
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
.index
    margin 0
    width 100vw
    min-height 100vh
    background-color #ECECEC
    position relative
    /* 公用模块 */
    .module
        padding 0 32rpx 44rpx
        border-radius 16rpx
        background #FFFFFF
        margin 0rpx 28rpx 0 28rpx
        box-shadow 0 4rpx 2rpx 0 rgba(212, 212, 212, 0.5)
        .title
            display flex
            align-items flex-start
            flex-direction column
            font-family PingFangSC-Semibold, PingFang SC
            .titles
                font-size 32rpx
                font-weight 600
                color #1E1E1E
                line-height 104rpx
            .card_car
                display flex
                justify-content space-around
                width 100%
                .image_pic
                    width 35%
                    margin 10rpx 20rpx
                    border-radius 10rpx
                    border 2rpx dashed #fff
                    &:nth-of-type(1)
                        margin-top 25rpx
                    image
                        width 100%
                        height 100%
                        display block
                    .text
                        margin 0 auto
                        text-align center
                        margin-top -40rpx
                        width 210rpx
                        font-family: '苹方-简';
                        height 56rpx
                        font-size 30rpx
                        color #fff
                        line-height 56rpx
                        background-color #9BD390
                        border-radius 40rpx
                .active
                    border 2rpx dashed #666
            .colorBrand
                mixinsWidthHeight(96rpx,36rpx)
                font-size 22rpx
                font-weight 400
                color #FFFFFF
                line-height 36rpx
                text-align center
                margin-left 20rpx
                border-radius 4rpx
    /* 模块一   办理车牌号 */
    .moduleOne
        .input
            height 80rpx
            display flex
            color #1E1E1E
            justify-content space-between
            .inputSub
                mixinsWidthHeight(64rpx,76rpx)
                font-size 44rpx
                font-weight 400
                color #1E1E1E
                text-align center
                line-height 72rpx
                border-radius 16rpx
                background #E8E8E8
                font-family: PingFangSC-Regular, PingFang SC;
                .newEnergy
                    font-size 12rpx
                    font-weight 600
                    color #5EBE68
                    font-family: PingFangSC-Regular, PingFang SC;
                    .ico
                        &::before
                            display block
                            font-size 24rpx
                            content '\e63d'
                            color #F19C38
                            font-weight bold
                            margin-top -10rpx
                            transform rotateZ(45deg)
                    .text
                        margin-top -40rpx
            .oldCarno
                font-weight bold
                font-size 44rpx
                .point
                    position relative
                    display flex
                    // border 1px solid #000
                    // display inline-block
                    padding-right .5ch
                    &::before
                        content: '\2022' /* 使用Unicode字符 \2022 作为间隔点 */
                        position: absolute
                        font-weight 400
                        color: #000
                        top 0
                        left: 30% 
                    .one
                        margin-right 35rpx
            .otherCarno
                color #5EBE68
                text-decoration underline
            .select
                border 2rpx solid #5EBE68 !important
            .special
                background-color #fff
                border 1rpx dashed #F19C38
                .select
                    border 2rpx solid #5EBE68 !important
            .blueCard
                background #ECF7FF
            .greenCard
                background #ECFFF4
            .noBorder
                border 2rpx solid #ECFFF4
        .changeCarText
            font-size 32rpx
            font-weight 600
            color #1E1E1E
            line-height 84rpx
            margin-top 34rpx
            height 70rpx
            text-align center 
        .moduleOneTip
            font-size 20rpx
            font-weight 400
            color #1E1E1E
            line-height 26rpx
            margin-top 45rpx
            font-family: PingFangSC-Regular, PingFang SC;

    /* 底部悬浮按钮 */
    .floatButton
        mixinsWidthHeight(,304rpx)
        opacity 1
        display flex
        flex-direction column
        align-items center
        justify-content center
        border-radius 16rpx 16rpx 0 0
        box-shadow 0 -8px 30px 0 rgba(223, 223, 223, 0.36)
        position absolute
        bottom 0
        background: #ffffff;
        .handleImmediately
            mixinsWidthHeight(662rpx,100rpx)
            font-size 36rpx
            color #FFFFFF
            text-align center
            line-height 100rpx
            background: #C0C0C0;
            border-radius 64rpx
            font-family: PingFangSC-Regular, PingFang SC;
            text-shadow 0 -8rpx 30rpx rgba(223, 223, 223, 0.36)
            box-shadow 0 -8rpx 30rpx 0 rgba(223, 223, 223, 0.36), 0 4rpx 2rpx 0 rgba(212, 212, 212, 0)
        .btn
            background #2A4F44
        .agreementConfirm
            height 104rpx
            display flex
            font-size 20rpx
            font-weight 400
            color #000000
            line-height 28rpx
            justify-content space-between
            margin 0rpx 38rpx 0 38rpx
            font-family PingFangSC-Regular, PingFang SC
            .confirm
                mixinsWidthHeight(42rpx,42rpx)
                line-height 42rpx
                border-radius 50%
                background-color #fff
                border 1px solid #ece8e8
            .tick
                &::before
                    display block
                    mixinsWidthHeight(44rpx,44rpx)
                    mixinsIconfont('\e669',30rpx,#FFFFFF)
                    text-align center
                    line-height 44rpx
                    border-radius 50%
                    background-color #39812B
            .agreement
                mixinsWidthHeight(614rpx,)
                text-align justify
                margin-left 10rpx
                font-size 22rpx
                color #333333
                .agreementColor
                    color #5EBE68
                    .agreeSpan
                        color #000000
