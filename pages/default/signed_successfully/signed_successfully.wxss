.signed_successfully > image {
  display: block;
  margin: 80rpx auto 0;
  width: 304rpx;
  height: 238rpx;
}
.signed_successfully .signed_title {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 76rpx;
  font-size: 60rpx;
}
.signed_successfully .signed_subtitle {
  margin-top: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 48rpx;
  font-size: 32rpx;
  color: #66666d;
}
.signed_successfully .btns {
  margin-top: 20rpx;
  padding: 0 60rpx;
}
.signed_successfully .btns > view {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 98rpx;
  border-radius: 49rpx;
}
.signed_successfully .btns > view.upload {
  font-size: 32rpx;
  color: #fff;
  background: linear-gradient(to right, #15ad69, #84d157);
}
.signed_successfully .btns > view.upload::before {
  margin-right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 59rpx;
  height: 49rpx;
  content: '\e65c';
  font-size: 58rpx;
}
.signed_successfully .btns > view:last-of-type {
  padding: 3rpx;
  margin-top: 40rpx;
  font-size: 28rpx;
  color: #2fb565;
  background: linear-gradient(to right, #15ad69, #84d157);
  overflow: hidden;
}
.signed_successfully .btns > view:last-of-type > view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  background: #fff;
  border-radius: 46rpx;
}
