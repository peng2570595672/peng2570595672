@import "../../../style/base.styl";
.high_speed_verification_failed
	// 失败
	& > image
		display block
		margin 112rpx auto 0
		width 302rpx
		height 206rpx
	// 文字描述开始
	.content
		text-align center
		.title
			margin-top 20rpx
			margin-bottom 26rpx
			display flex
			align-items center
			justify-content center
			height 72rpx
			font-size 60rpx
			font-weight 500
		.subtitle
			display flex
			align-items center
			justify-content center
			height 48rpx
			color $fontColor6
			& > text
				color $error
		.back-home
			margin-bottom 40rpx
			display inline-block
			align-items center
			justify-content center
			margin-top 90rpx
			height 40rpx
			line-height 40rpx
			font-size 24rpx
			color #1797F7
	// 分割
	.split
		height 20rpx
		background: #F7F7F7
	// 广告开始
	.ad
		margin-top 40rpx
		width 670rpx
		height 364rpx
