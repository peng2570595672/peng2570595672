  align-items: center;
  justify-content: center;
  margin: 20rpx 0;
  padding: 0 20rpx;
  height: 178rpx;
  box-shadow: 4rpx 4rpx 46rpx 0 rgba(126,132,156,0.1);
  border-radius: 32rpx;
}
.index .bill .bill-empty > image {
