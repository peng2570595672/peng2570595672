}
.index .nav {
  display: flex;
  justify-content: center;
  padding-top: 50rpx;
}
.index .nav .item {
  margin-left: 70rpx;
  text-align: center;
  flex: 0 0 100rpx;
}
.index .nav .item:first-of-type {
