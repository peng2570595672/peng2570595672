.index .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 98rpx;
  font-size: 40rpx;
  font-weight: 500;
  color: #fff;
  border-radius: 49rpx;
  font-weight: 500;
  background: linear-gradient(to right, #15ad69, #84d157);
  box-shadow: 0 8rpx 10rpx 0 rgba(132,209,87,0.2);
}
.index .tips {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20rpx;
  text-align: center;
  font-size: 24rpx;
  height: 40rpx;
  color: #99999d;
}
.index .tips > text {
  color: #ff563c;
}
.index .welcome {
  margin-top: 26rpx;
  padding-left: 60rpx;
  font-size: 60rpx;
  line-height: 60rpx;
  font-weight: 500;
}
.index .welcome > text {
  margin-left: 26rpx;
  font-size: 32rpx;
  color: #66666d;
}
.index .my-swiper {
  margin: 40rpx 40rpx 0;
  height: 394rpx;
  border-radius: 32rpx;
  overflow: hidden;
}
.index .my-swiper .my-swiper-item {
  height: 364rpx !important;
  background: linear-gradient(to right, #15ad69, #84d157);
  border-radius: 32rpx;
}
.index .my-swiper .my-swiper-item > image {
  width: 100%;
  height: 100%;
}
.index .my-swiper .wx-swiper-dots-horizontal {
  margin-bottom: -18rpx;
}
.index .my-swiper .wx-swiper-dots-horizontal .wx-swiper-dot {
  margin-right: 4rpx;
  width: 10rpx;
  height: 10rpx;
  background: #e3e5e6;
}
.index .my-swiper .wx-swiper-dots-horizontal .wx-swiper-dot.wx-swiper-dot-active {
  background: #979999;
}
.index .free-processing {
  margin-top: 98rpx;
  padding: 0 70rpx;
  padding-bottom: 40rpx;
}
.index .processing,
.index .viewing-process,
.index .update-info,
.index .audit-failure,
.index .confirm-receipt,
.index .resume-contract,
.index .bill {
  margin-top: 40rpx;
  padding: 0 60rpx 40rpx;
}
.index .processing .processing-title,
.index .viewing-process .processing-title,
.index .update-info .processing-title,
.index .audit-failure .processing-title,
.index .confirm-receipt .processing-title,
.index .resume-contract .processing-title,
.index .bill .processing-title {
  display: flex;
  align-items: center;
  height: 48rpx;
}
.index .processing .processing-title > view:first-of-type,
.index .viewing-process .processing-title > view:first-of-type,
.index .update-info .processing-title > view:first-of-type,
.index .audit-failure .processing-title > view:first-of-type,
.index .confirm-receipt .processing-title > view:first-of-type,
.index .resume-contract .processing-title > view:first-of-type,
.index .bill .processing-title > view:first-of-type {
  display: flex;
  align-items: center;
  height: 48rpx;
  flex: 1;
  font-size: 32rpx;
}
.index .processing .processing-title > view:last-of-type,
.index .viewing-process .processing-title > view:last-of-type,
.index .update-info .processing-title > view:last-of-type,
.index .audit-failure .processing-title > view:last-of-type,
.index .confirm-receipt .processing-title > view:last-of-type,
.index .resume-contract .processing-title > view:last-of-type,
.index .bill .processing-title > view:last-of-type {
  display: flex;
  height: 40rpx;
  align-items: center;
  font-size: 24rpx;
  color: #99999d;
}
.index .processing .processing-title > view:last-of-type::after,
.index .viewing-process .processing-title > view:last-of-type::after,
.index .update-info .processing-title > view:last-of-type::after,
.index .audit-failure .processing-title > view:last-of-type::after,
.index .confirm-receipt .processing-title > view:last-of-type::after,
.index .resume-contract .processing-title > view:last-of-type::after,
.index .bill .processing-title > view:last-of-type::after {
  margin-left: 5rpx;
  display: inline-block;
  width: 0;
  content: '';
  border-color: rgba(0,0,0,0);
  border-bottom-style: solid;
  border-right: 15rpx solid #ffec9a;
  border-width: 15rpx;
  transform: rotateZ(45deg);
}
.index .processing .btn,
.index .viewing-process .btn,
.index .update-info .btn,
.index .audit-failure .btn,
.index .confirm-receipt .btn,
.index .resume-contract .btn,
.index .bill .btn {
  margin: 20rpx 10rpx 0;
}
.index .audit-failure .failed {
  margin-top: 38rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 74rpx;
  color: #ff563c;
  font-size: 40rpx;
}
.index .audit-failure .tips {
  margin-top: 36rpx;
}
.index .bill {
  padding: 0 40rpx;
}
.index .bill .processing-title {
