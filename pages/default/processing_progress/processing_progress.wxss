.processing_progress {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
}
.processing_progress .title {
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  padding: 0 40rpx;
  height: 56rpx;
  font-size: 40rpx;
}
.processing_progress .title.other-title {
  margin-top: 40rpx;
  font-weight: bold;
}
.processing_progress .title::before {
  margin-right: 10rpx;
  content: '';
  width: 10rpx;
  height: 10rpx;
  border: 6rpx solid #2fb565;
  border-radius: 50%;
}
.processing_progress .user-info {
  margin-top: 10rpx;
  padding-left: 80rpx;
  color: #66666d;
  font-size: 24rpx;
}
.processing_progress .user-info > view {
  display: flex;
  align-items: center;
  height: 40rpx;
}
.processing_progress .user-info > view > text:first-of-type {
  margin-right: 20rpx;
}
.processing_progress .progress_wrapper {
  margin: 10rpx 50rpx 0;
  padding-top: 40rpx;
  box-shadow: 4rpx 12rpx 56rpx 0 rgba(102,102,109,0.19);
  border-radius: 12rpx;
}
.processing_progress .progress_wrapper .item {
  position: relative;
  margin-left: 36rpx;
  padding-bottom: 40rpx;
  border-left: 1rpx dashed #cdcdcf;
}
.processing_progress .progress_wrapper .item.current::before {
  background: #2fb565;
}
.processing_progress .progress_wrapper .item.finished .content-title {
  color: #2fb565;
}
.processing_progress .progress_wrapper .item.finished::before {
  background: #2fb565;
}
.processing_progress .progress_wrapper .item.finished::after {
  position: absolute;
  top: 0;
  left: -3rpx;
  content: '';
  display: inline-block;
  width: 4rpx;
  height: 100%;
  background: #2fb565;
}
.processing_progress .progress_wrapper .item::before {
  position: absolute;
  top: -8rpx;
  left: -9rpx;
  content: '';
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #99999d;
}
.processing_progress .progress_wrapper .item:last-of-type {
  border-left: 0;
}
.processing_progress .progress_wrapper .item:last-of-type::before {
  left: -8rpx;
}
.processing_progress .progress_wrapper .item .content-title {
  position: relative;
  padding-left: 26rpx;
  display: flex;
  align-items: center;
  height: 48rpx;
  font-size: 32rpx;
  font-weight: 500;
  transform: translateY(-24rpx);
}
.processing_progress .progress_wrapper .item .content-des {
  display: flex;
  margin-top: 10rpx;
  padding-left: 26rpx;
  padding-right: 40rpx;
  line-height: 40rpx;
  color: #66666d;
  font-size: 24rpx;
  transform: translateY(-24rpx);
}
.processing_progress .progress_wrapper .item .content-des.prompt {
  color: #ff3c48;
}
.processing_progress .progress_wrapper .item .content-des.error {
  color: #ff563c;
}
.processing_progress .progress_wrapper .item .content-des.error::before {
  margin-right: 8rpx;
  content: '\e617';
}
.processing_progress .progress_wrapper .item .content-des .refresh {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #39a6f8;
}
.processing_progress .progress_wrapper .item .content-des .refresh::after {
  margin-left: 12rpx;
  content: '\e616';
  font-size: 28rpx;
}
.processing_progress .progress_wrapper .item .content-des .edit {
  margin-left: 20rpx;
  display: flex;
  align-items: center;
  color: #39a6f8;
  font-size: 24rpx;
}
.processing_progress .progress_wrapper .item .content-des .edit::after {
  display: inline-block;
  content: '\e60e';
  transform: rotateZ(-180deg) translateY(2rpx);
}
.processing_progress .progress_wrapper .item .content-des .copy {
  margin-left: 20rpx;
  display: flex;
  align-items: center;
  color: #39a6f8;
  font-size: 24rpx;
}
.processing_progress .progress_wrapper .item .content-des.earnest-money {
  display: block;
}
.processing_progress .progress_wrapper .item .content-des.earnest-money .money {
  color: #00b85c;
}
.processing_progress .progress_wrapper .item .content-des.earnest-money .failure {
  color: #ff3c48;
}
.processing_progress .progress_wrapper .item .content-des.earnest-money .precharge {
  color: #00b1ff;
}
.processing_progress .progress_wrapper .item .content-des.account {
  display: block;
}
.processing_progress .progress_wrapper .item .content-des .precharge {
  color: #00b1ff;
}
.processing_progress .progress_wrapper .item .handle-the-way {
  display: flex;
  height: 148rpx;
  width: 552rpx;
  align-items: center;
  border-radius: 16rpx;
  margin-top: 12rpx;
  margin-left: 26rpx;
  background: #f6f6f6;
}
.processing_progress .progress_wrapper .item .handle-the-way .name {
  flex: 1;
}
.processing_progress .progress_wrapper .item .handle-the-way .name .top {
  font-size: 34rpx;
  line-height: 48rpx;
  color: rgba(0,0,0,0.9);
}
.processing_progress .progress_wrapper .item .handle-the-way .name .poundage {
  font-size: 24rpx;
  line-height: 40rpx;
  color: rgba(0,0,0,0.25);
}
.processing_progress .progress_wrapper .item .handle-the-way .choice {
  font-size: 24rpx;
  color: rgba(0,0,0,0.1);
  margin-right: 32rpx;
}
.processing_progress .progress_wrapper .item .handle-the-way image {
  flex: 0 0 48rpx;
  width: 48rpx;
  height: 48rpx;
  margin-left: 24rpx;
  margin-right: 12rpx;
}
.processing_progress .progress_wrapper .item .content-info {
  background: #f0f0f0;
  margin: 0rpx 40rpx 32rpx 24rpx;
  padding: 32rpx 18rpx;
  font-size: 24rpx;
  line-height: 44rpx;
}
.processing_progress .progress_wrapper .item .content-info text {
  padding-left: 12rpx;
  color: #00a9ff;
}
.processing_progress .progress_wrapper .item .already-prepaid {
  text-align: right;
  color: #00b85c;
  font-size: 24rpx;
  padding-right: 40rpx;
  padding-top: 32rpx;
}
.processing_progress .progress_wrapper .last::after {
  display: none !important;
}
.processing_progress .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx 60rpx 0;
  height: 98rpx;
  font-size: 40rpx;
  font-weight: 500;
  color: #fff;
