mixinsWidthHeight($width=100%, $height=100%)
    width $width
    height $height
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
.page
    /* 底部悬浮按钮 */
    .floatButton
        mixinsWidthHeight(,204rpx)
        opacity 1
        display flex
        align-items center
        justify-content center
        border-radius 16rpx 16rpx 0 0
        box-shadow 0 -8px 30px 0 rgba(223, 223, 223, 0.36)
        position fixed
        bottom 0
        z-index 100
        background: #ffffff;
        .handleImmediately
            mixinsWidthHeight(662rpx,110rpx)
            font-size 36rpx
            color #FFFFFF
            text-align center
            line-height 110rpx
            background: #C0C0C0;
            border-radius 64rpx
            font-family: PingFangSC-Regular, PingFang SC;
            text-shadow 0 -8rpx 30rpx rgba(223, 223, 223, 0.36)
            box-shadow 0 -8rpx 30rpx 0 rgba(223, 223, 223, 0.36), 0 4rpx 2rpx 0 rgba(212, 212, 212, 0)
        .btn
            background #2A4F44