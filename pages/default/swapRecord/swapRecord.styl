mixinsWidthHeight($width=100%, $height=100%)
    width $width
    height $height
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
image
    width 100% 
    height 100%
    display block
.page
    padding-bottom 300rpx
    .container
        margin 30rpx 38rpx
        background-color #fff
        .component
            display flex
            .top-div
                mixinsWidthHeight(24rpx,22rpx)
                margin 15rpx 
            .intru
                font-size 40rpx
                font-weight 400
        .component-custom
            // background-color pink
            .addTime
                font-size 24rpx
                line-height 44rpx
            .carNo
                padding 20rpx 0
                line-height 88rpx
                justify-content space-between
                font-size 40rpx
                display flex
                border-bottom 1rpx solid #a8a6a6
                .pic
                    mixinsWidthHeight(48rpx,48rpx)
                    margin-top 20rpx
                    border 4rpx dashed #efefef
            .result
                display flex
                font-size 26rpx
                margin-top 15rpx
                margin-bottom 30rpx
                line-height 80rpx
                justify-content space-between
                padding 10rpx 0
                // line-height 44rpx
                .bootom
                    text-align right
                    .color_font
                        color #3D3D3D
                    .finish
                        color green
                    .fail
                        color #FF6300
                    .btn
                        color white
                        text-align center
                        mixinsWidthHeight(175rpx,54rpx)
                        line-height 54rpx
                        border-radius: 26rpx;
                        background: #2A4F44;
        .text
            display flex
            flex-direction column
            padding-left 44rpx
            margin 20rpx 0
            text
                font-size 28rpx 
                line-height 60rpx
        .pic
            // mixinsWidthHeight(,380rpx)
        .applicationRecords
            mixinsWidthHeight(300rpx,68rpx)
            text-align center
            line-height 68rpx
            margin 0 auto
            margin-top 80rpx
            border-radius: 12rpx;
            border 2rpx solid #2A4F44
    .fill-line
        margin 30rpx -38rpx
        height 16rpx
        background-color #ececec
    /* 底部悬浮按钮 */
    .floatButton
        mixinsWidthHeight(,204rpx)
        opacity 1
        display flex
        align-items center
        justify-content center
        border-radius 16rpx 16rpx 0 0
        box-shadow 0 -8px 30px 0 rgba(223, 223, 223, 0.36)
        position fixed
        bottom 0
        z-index 100
        background: #ffffff;
        .handleImmediately
            mixinsWidthHeight(662rpx,110rpx)
            font-size 36rpx
            color #FFFFFF
            text-align center
            line-height 110rpx
            border-radius 64rpx
            font-family: PingFangSC-Regular, PingFang SC;
            text-shadow 0 -8rpx 30rpx rgba(223, 223, 223, 0.36)
            box-shadow 0 -8rpx 30rpx 0 rgba(223, 223, 223, 0.36), 0 4rpx 2rpx 0 rgba(212, 212, 212, 0)
            background #2A4F44