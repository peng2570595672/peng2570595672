.package-the-rights-and-interests {
  min-height: 100vh;
  padding-bottom: 170rpx;
  box-sizing: border-box;
  background: #f6f6f6;
}
.package-the-rights-and-interests .swiper-box .my-swiper {
  padding-top: 30rpx;
}
.package-the-rights-and-interests .swiper-box .my-swiper .my-swiper-item {
