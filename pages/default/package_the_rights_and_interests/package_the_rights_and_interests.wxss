/* 支付和协议说明 模块 */
.payAndAgreement {
  width: 750rpx;
  height: 330rpx;
  background: #fbfbfb;
  box-shadow: 0 -20rpx 16rpx 0 rgba(152,168,151,0.11);
  position: fixed;
  bottom: 0;
  z-index: 10;
}
.payAndAgreement .agreementConfirm {
  height: 84rpx;
  display: flex;
  font-size: 20rpx;
  font-weight: 400;
  color: #000;
  line-height: 28rpx;
  justify-content: space-between;
  margin: 30rpx 38rpx 0 38rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.payAndAgreement .agreementConfirm .confirm {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  background-color: #ccc;
}
.payAndAgreement .agreementConfirm .tick::before {
  display: block;
  width: 44rpx;
  height: 44rpx;
  content: '\e669';
  font-size: 22rpx;
  color: #fff;
  text-align: center;
  line-height: 44rpx;
  border-radius: 50%;
  background-color: #39812b;
}
.payAndAgreement .agreementConfirm .agreement {
  width: 614rpx;
  height: 100%;
  text-align: justify;
}
.payAndAgreement .agreementConfirm .agreement .agreementColor {
  color: #5ebe68;
}
.payAndAgreement .agreementConfirm .agreement .agreementColor .agreeSpan {
  color: #000;
}
.payAndAgreement .paySign .moneyPay,
.payAndAgreement .ttCounpon,
.payAndAgreement .nextSign {
  height: 90rpx;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  margin: 32rpx 38rpx 0 38rpx;
  font-family: Arial-BoldItalicMT, Arial;
}
.payAndAgreement .ttCounpon .btn,
.payAndAgreement .paySign .nextSign .btns {
  width: 600rpx;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  border-radius: 64rpx;
  background-color: #ccc;
  box-shadow: 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.payAndAgreement .paySign .confirmPackage {
  justify-content: space-between;
}
.payAndAgreement .paySign .moneyPay .money {
  font-size: 44rpx;
  color: #000;
  line-height: 44rpx;
  font-weight: bold;
  font-style: oblique;
  font-family: Arial-BoldItalicMT, Arial;
}
.payAndAgreement .paySign .moneyPay .money .mpneySpan {
  font-size: 76rpx;
  line-height: 76rpx;
  font-style: oblique;
}
.payAndAgreement .paySign .moneyPay .pay {
  width: 348rpx;
  height: 90rpx;
  font-size: 36Rpx;
  font-weight: 600;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  border-radius: 64rpx;
  background-color: #ccc;
  box-shadow: 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
}
.payAndAgreement .paySign .payWidth {
  width: 600rpx;
}
.payAndAgreement .paySign .moneyPay .payColor,
.payAndAgreement .ttCounpon .payColor,
.payAndAgreement .paySign .nextSign .payColor {
  background-color: #2a4f44;
  box-shadow: 0rpx 4rpx 2rpx 0rpx rgba(212,212,212,0.5);
}
/* 动画 */
.fadeIn {
  position: fixed;
  animation: fade_in 0.5s linear forwards;
  -webkit-animation: fade_in 0.5s linear forwards;
}
.fadeOut {
  position: fixed;
  animation: fade_out 0.5s linear forwards;
  -webkit-animation: fade_out 0.5s linear forwards;
}
@keyframes fade_in {
  0% {
    bottom: -330rpx;
  }
  100% {
    bottom: 0;
  }
}
@keyframes fade_out {
  0% {
    bottom: 0;
  }
  100% {
    bottom: -330rpx;
  }
}
@keyframes fade_in {
  0% {
    bottom: -330rpx;
  }
  100% {
    bottom: 0;
  }
}
@keyframes fade_out {
  0% {
    bottom: 0;
  }
  100% {
    bottom: -330rpx;
  }
}
