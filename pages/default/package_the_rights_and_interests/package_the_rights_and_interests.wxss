  border-radius: 16rpx;
  background-color: #f9f6ef;
  margin: 32rpx 0 0 0;
  font-family: PingFangSC-Regular, PingFang SC;
  box-shadow: 0 4rpx 2rpx 0 rgba(212,212,212,0.5);
  position: relative;
  left: 50%;
  transform: translateX(-50%);
/* 活动提示模块 */
/* 套餐模块的上半部分 */
/* 套餐模块的下半部分 */
}
.package .module .active {
  color: #fff;
  height: 36rpx;
  font-weight: 400;
  font-size: 16rpx;
  text-align: center;
  line-height: 30rpx;
  padding: 0 20rpx;
  letter-spacing: 3rpx;
  background-color: #e9ca7b;
  font-family: PingFangSC-Regular, PingFang SC;
  border-top-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 10;
}
.package .module .activeChange {
  position: absolute;
  top: -4rpx;
  right: -4rpx;
  border-top: 4rpx solid #e9ca7b;
  border-right: 4rpx solid #e9ca7b;
}
.package .module .subUpperPart {
  border-radius: 16rpx;
}
.package .module .upperPart {
  opacity: 1;
