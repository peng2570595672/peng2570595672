.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.index {
    margin: 0;
    padding: 0;
    width: 100vw;
    min-height: 100vh;
    background-color: #ECECEC;
}

/* 顶部组件占位 */
.index .topSplit {
    width: 100%;
    height: 60rpx;
}

/* 套餐模块 */
.index .package .module {
    width: 694rpx;
    border-radius: 16rpx;
    background-color: #F9F6EF;
    margin: 32rpx 0rpx 0rpx 0rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    box-shadow: 0px 4rpx 2rpx 0rpx rgba(212, 212, 212, 0.5);
    position: relative;
    left: 50%;
    transform: translateX(-50%);
}

.index .package .moduleOne {
    margin-top: 38rpx;
}

.index .package .btnHighLight {
    border: 4rpx solid #E9CA7B;
}

.index .package .btnHighLight::before {
    width: 50rpx;
    height: 30rpx;
    display: block;
    color: #FFFFFF;
    font-size: 20rpx;
    content: '\e669';
    text-align: center;
    line-height: 30rpx;
    background-color: #E9CA7B;
    border-top-left-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
    position: absolute;
    top: -4rpx;
    left: -4rpx;
    z-index: 10;

}


/* 活动提示模块 */
.index .package .module .active {
    color: #fff;
    height: 30rpx;
    font-weight: 400;
    font-size: 16rpx;
    text-align: center;
    line-height: 30rpx;
    padding: 0rpx 20rpx;
    letter-spacing: 3rpx;
    background-color: #5EBE68;
    font-family: PingFangSC-Regular, PingFang SC;
    border-top-right-radius: 16rpx;
    border-bottom-left-radius: 16rpx;
    position: absolute;
    top: 0rpx;
    right: 0rpx;
    z-index: 10;
}

.index .package .module .activeChange {
    position: absolute;
    top: -4rpx;
    right: -4rpx;
    border-top: 4rpx solid #E9CA7B;
    border-right: 4rpx solid #E9CA7B;
}


/* 套餐模块的上半部分 */
.index .package .module .upperPart {
    opacity: 1;
    overflow: hidden;
    position: relative;
    border-top-right-radius: 16rpx;
    border-top-left-radius: 16rpx;
    background-color: #fff;
    padding: 38rpx 28rpx 0rpx 28rpx;
}

.index .package .module .subUpperPart {
    border-radius: 16rpx;
}

.index .package .module .upperPart .bgImg {
    width: 234rpx;
    height: 234rpx;
    position: absolute;
    right: -50rpx;
    bottom: -100rpx;
    z-index: 1;
    opacity: 0.1;
}

.index .package .module .upperPart .bgImg image {
    width: 100%;
    height: 100%;
}

.index .package .module .titleMOney {
    height: 54rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.index .package .module .upperPart .titleMOney .title {
    font-size: 32rpx;
    font-weight: 600;
    color: #1E1E1E;
}

.index .package .module .upperPart .titleMOney .money {
    height: 54rpx;
    font-size: 48rpx;
    color: #2A4F44;
    line-height: 54rpx;
    font-weight: normal;
    font-style: oblique;
    font-family: Arial-BoldItalicMT, Arial;
}

.index .package .module .upperPart .titleMOney .titleExplain {
    width: 186rpx;
    height: 60rpx;
    font-size: 16rpx;
    font-weight: 400;
    color: #FFFDFD;
    line-height: 2r2px;
    text-align: right;
    font-family: PingFangSC-Regular, PingFang SC;
    position: absolute;
    right: 24rpx;
    top: 82rpx;
}

.index .package .module .upperPart .titleMOney .titleExplain::before {
    display: block;
    content: '';
    border: 5rpx solid;
    border-color: transparent #5EBE68 #5EBE68 transparent;
    position: absolute;
    top: -3rpx;
    right: 0;

}

.index .package .module .upperPart .titleMOney .titleExplain .innerBox1 {
    width: 100%;
    height: 55rpx;
    margin-top: 7rpx;
    border-radius: 16rpx;
    border-top-right-radius: 0rpx;
    background-color: #5EBE68;
}

.index .package .module .upperPart .titleMOney .titleExplain .innerBox1 .innerBox2 {
    display: inline-block;
    padding: 2rpx 10rpx;
}

.index .package .module .upperPart .subTitle {
    height: 40rpx;
    font-size: 28rpx;
    font-weight: 400;
    color: #262626;
    margin-top: 10rpx;
    line-height: 40rpx;
    padding-bottom: 15rpx;
    border-bottom: 2rpx solid #F6F6F6;
}

.index .package .module .upperPart .description {
    display: flex;
    flex-wrap: wrap;
    font-size: 24rpx;
    font-weight: 400;
    color: #262626;
    margin-bottom: 36rpx;
    justify-content: space-between;
}

.index .package .module .upperPart .description .descriptionModule {
    width: 242rpx;
    display: flex;
    align-items: center;
    margin: 12rpx 0 4rpx 0;
}

.index .package .module .upperPart .description .descriptionModule .ico {
    width: 40rpx;
    height: 40rpx;
    font-weight: 400;
    overflow: hidden;
    margin-right: 8rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    /* background-color: #E9CA7B; */
}

.index .package .module .upperPart .description .descriptionModule .service_of_etc::before {
    content: '\e683';
    font-size: 30rpx;
    color: #5EBE68;
}

.index .package .module .upperPart .description .descriptionModule .service_of_equipment::before {
    content: '\e685';
    font-size: 30rpx;
    color: #5EBE68;
}

.index .package .module .upperPart .description .descriptionModule .service_of_invoice::before {
    content: '\e67a';
    font-size: 30rpx;
    color: #5EBE68;
}

.index .package .module .upperPart .description .descriptionModule .service_of_discount::before {
    content: '\e680';
    font-size: 30rpx;
    color: #5EBE68;
}



/* 套餐模块的下半部分 */
.index .package .module .lowerpart {
    padding: 20rpx 28rpx 30rpx 28rpx;
    border-bottom-left-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
}


.index .package .module .lowerpart .titleDetails {
    height: 40rpx;
    display: flex;
    font-size: 28rpx;
    font-weight: 600;
    line-height: 40rpx;
    align-items: center;
    justify-content: space-between;
    font-family: PingFangSC-Semibold, PingFang SC;
}

.index .package .module .lowerpart .titleDetails .titles {
    height: 100%;
    display: flex;
}
.index .package .module .lowerpart .titleDetails .titles .isTtIcon {
    width: 40rpx;
    height: 40rpx;
    background: url('https://file.cyzl.com/g001/M01/C8/9E/oYYBAGP1z2iAZxFoAAAVlwqnjac464.svg') center no-repeat;
    background-size: 40rpx 40rpx;
}

.index .package .module .lowerpart .titleDetails .titles .icon::before {
    width: 40rpx;
    height: 40rpx;
    display: block;
    content: '\e6a1';
    color: #E9CA7B;
    font-size: 24rpx;
}

.index .package .module .lowerpart .titleDetails .titles .text {
    font-size: 28rpx;
    font-weight: 600;
    color: #E9CA7B;
    line-height: 39rpx;
    margin-left: 10rpx;
}

.index .package .module .lowerpart .titleDetails .details {
    color: #2A4F44;
    width: 60rpx;
    height: 40rpx;
    padding: 10rpx 20rpx;
}

.index .package .module .lowerpart .detailsInfo .isSignTtCoupon {
    width: 100%;
    height: 420rpx;
    margin-top: 20rpx;
    overflow: hidden;
}

.index .package .module .lowerpart .detailsInfo .isSignTtCoupon .ttImgs {
    width: 100%;
    height: 100%;
}

.index .package .module .lowerpart .detailsInfo .isSignTtCoupon .ttImgs .ttImg {
    width: 100%;
    height: 100%;
}


/* 占位 */
.index .split {
    width: 100%;
    height: 50rpx;
    height: 330rpx;
    overflow: hidden;
    margin-top: 32rpx;
    text-align: center;
    line-height: 330rpx;
    color: rgba(0, 0, 1, 0.25);
    font-family: PingFangSC-Semibold, PingFang SC;
}

.index .bigSplit {
    height: 1000rpx;
    font-size: 50rpx;
    font-weight: bold;
    line-height: 300rpx;

}

.index .littleSplit {
    height: 120rpx;
    line-height: 80rpx;
}



/* 支付和协议说明 模块 */
.index .payAndAgreement {
    width: 750rpx;
    height: 330rpx;
    background: #FBFBFB;
    /* padding: 30rpx 38rpx 94rpx 38rpx; */
    box-shadow: 0rpx -20rpx 16rpx 0rpx rgba(152, 168, 151, 0.11);
    position: fixed;
    bottom: 0rpx;
    z-index: 10;
}

.index .payAndAgreement .agreementConfirm {
    height: 84rpx;
    display: flex;
    font-size: 20rpx;
    font-weight: 400;
    color: #000000;
    line-height: 28rpx;
    justify-content: space-between;
    margin: 30rpx 38rpx 0rpx 38rpx;
    font-family: PingFangSC-Regular, PingFang SC;
}

.index .payAndAgreement .agreementConfirm .confirm {
    width: 44rpx;
    height: 44rpx;
    border-radius: 50%;
    background-color: #ccc;
}

.index .payAndAgreement .agreementConfirm .tick::before {
    display: block;
    width: 44rpx;
    height: 44rpx;
    color: #fff;
    content: '\e669';
    font-size: 22rpx;
    text-align: center;
    line-height: 44rpx;
    border-radius: 50%;
    background-color: #39812B;
}

.index .payAndAgreement .agreementConfirm .agreement {
    width: 614rpx;
    height: 100%;
    text-align: justify;
}

.index .payAndAgreement .agreementConfirm .agreement .agreementColor {
    color: #5EBE68;
}



.index .payAndAgreement .paySign .moneyPay,
.index .payAndAgreement .ttCounpon,
.index .payAndAgreement .nextSign {
    height: 90rpx;
    display: flex;
    align-items: flex-end;
    justify-content: center;
    margin: 32rpx 38rpx 0rpx 38rpx;
    font-family: Arial-BoldItalicMT, Arial;
}

.index .payAndAgreement .ttCounpon .btn,
.index .payAndAgreement .paySign .nextSign .btns {
    width: 600rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 90rpx;
    border-radius: 64rpx;
    background-color: #ccc;
    box-shadow: 0rpx 4rpx 2rpx 0rpx rgba(212, 212, 212, 0.5);
}

.index .payAndAgreement .paySign .confirmPackage {
    justify-content: space-between;
}

.index .payAndAgreement .paySign .moneyPay .money {
    font-size: 44rpx;
    color: #000000;
    line-height: 44rpx;
    font-weight: normal;
}

.index .payAndAgreement .paySign .moneyPay .money .mpneySpan {
    font-size: 76rpx;
    line-height: 76rpx;
    font-style: oblique;
}

.index .payAndAgreement .paySign .moneyPay .pay {
    width: 348rpx;
    height: 90rpx;
    font-size: 36Rpx;
    font-weight: 600;
    color: #FFFFFF;
    text-align: center;
    line-height: 90rpx;
    border-radius: 64rpx;
    background-color: #ccc;
    box-shadow: 0rpx 4rpx 2rpx 0rpx rgba(212, 212, 212, 0.5);
}

.index .payAndAgreement .paySign .moneyPay .payWidth {
    width: 600rpx;
}

.index .payAndAgreement .paySign .moneyPay .payColor,
.index .payAndAgreement .ttCounpon .payColor,
.index .payAndAgreement .paySign .nextSign .payColor {
    background-color: #2A4F44;
}


/* 动画 */
.index .fadeIn {
    position: fixed;
    animation: fade_in 0.5s linear forwards;
    -webkit-animation: fade_in 0.5s linear forwards;
}

.index .fadeOut {
    position: fixed;
    animation: fade_out 0.5s linear forwards;
    -webkit-animation: fade_out 0.5s linear forwards;
}

@keyframes fade_in {
    0% {
        bottom: -330rpx;
    }

    100% {
        bottom: 0rpx;
    }
}

@keyframes fade_out {
    0% {
        bottom: 0rpx;
    }

    100% {
        bottom: -330rpx;
    }
}

@-webkit-keyframes fade_in {
    0% {
        bottom: -330rpx;
    }

    100% {
        bottom: 0rpx;
    }
}

@-webkit-keyframes fade_out {
    0% {
        bottom: 0rpx;
    }

    100% {
        bottom: -330rpx;
    }
}