}
.package-the-rights-and-interests .swiper-box .my-swiper .my-swiper-item .my-swiper-item-box .basic-service-list .other-service-item {
  position: relative;
  width: 274rpx;
  height: 134rpx;
  border-radius: 12rpx;
  padding: 32rpx;
  box-sizing: border-box;
  background-image: url("https://file.cyzl.com/g001/M07/34/B5/oYYBAGCGFPGAMsNIAAAKIgmvm54348.svg");
  background-size: 274rpx 134rpx;
  color: rgba(0,0,0,0.8);
