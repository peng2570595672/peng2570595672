  padding: 48rpx 32rpx 32px;
  box-sizing: border-box;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  z-index: 99999;
}
.package-the-rights-and-interests .swiper-box .my-swiper .my-swiper-item .my-swiper-item-box .package {
  display: flex;
  align-items: center;
  height: 84rpx;
  font-size: 44rpx;
  padding-bottom: 30rpx;
  color: rgba(0,0,0,0.9);
  font-weight: 600;
}
.package-the-rights-and-interests .swiper-box .my-swiper .my-swiper-item .my-swiper-item-box .package image {
  width: 70rpx;
  height: 44rpx;
  margin-right: 12rpx;
}
