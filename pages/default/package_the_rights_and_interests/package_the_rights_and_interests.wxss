  bottom: 0;
  width: 100%;
  height: 160rpx;
  background: #f6f6f6;
}
.package-the-rights-and-interests .next .btn {
  width: 650rpx;
  height: 96rpx;
  line-height: 96rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #fff;
  text-align: center;
  background: linear-gradient(132deg, #16ad69 0%, #82d057 100%);
  border-radius: 48rpx;
  margin-top: 20rpx;
}
.package-the-rights-and-interests .next .btn.unavailable {
  opacity: 0.6;
}
