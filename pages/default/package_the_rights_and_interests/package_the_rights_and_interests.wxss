  margin: auto;
}
.package-the-rights-and-interests .rights-and-interests .rights-and-interests-list .rights-and-interests-item {
  display: flex;
  height: 112rpx;
  align-items: center;
  background: #fff;
  border-radius: 16rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  margin-bottom: 12rpx;
  color: rgba(0,0,0,0.5);
