  box-sizing: border-box;
}
.package-the-rights-and-interests .rights-and-interests .rights-and-interests-header {
  display: flex;
  height: 48rpx;
  align-items: center;
  font-size: 34rpx;
  color: rgba(0,0,0,0.9);
  margin-bottom: 28rpx;
}
