/* 动画 */
.fadeIn {
  position: fixed;
  animation: fade_in 0.5s linear forwards;
  -webkit-animation: fade_in 0.5s linear forwards;
}
.fadeOut {
  position: fixed;
  animation: fade_out 0.5s linear forwards;
  -webkit-animation: fade_out 0.5s linear forwards;
}
@-webkit-keyframes fade_in {
  0% {
    bottom: -330rpx;
  }
  100% {
    bottom: 0;
  }
}
@keyframes fade_in {
  0% {
    bottom: -330rpx;
  }
  100% {
    bottom: 0;
  }
}
@-webkit-keyframes fade_out {
  0% {
    bottom: 0;
  }
  100% {
    bottom: -330rpx;
  }
}
@keyframes fade_out {
  0% {
    bottom: 0;
  }
  100% {
    bottom: -330rpx;
  }
}
@keyframes fade_in {
  0% {
    bottom: -330rpx;
  }
  100% {
    bottom: 0;
  }
}
@keyframes fade_out {
  0% {
    bottom: 0;
  }
  100% {
    bottom: -330rpx;
  }
}
@-webkit-keyframes addEquityPackage {
  0% {
    opacity: 0;
  }
  71% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@keyframes addEquityPackage {
  0% {
    opacity: 0;
  }
  71% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
