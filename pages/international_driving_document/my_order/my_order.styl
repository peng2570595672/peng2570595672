.my-order
	min-height 100vh
	display flex
	flex-direction column
	.swiper-tab
		margin 40rpx 80rpx 30rpx 80rpx
		display flex
		height 70rpx
		background: #F8F8F8;
		border-radius: 12rpx;
		border: 1rpx solid rgba(231, 231, 231, 0.42);
		.tab-list
			display flex
			flex 1
			font-size 36rpx
			color #66666E
			text-align: center;
			align-items: center;
			justify-content: center;
		.tab-show
			font-family: PingFangSC-Semibold, PingFang SC;
			font-weight: 500;
			color #fff
			background: linear-gradient(49deg, #14AC69 0%, #84D157 100%);
			border-radius: 12rpx;
	.list
		.list-item
			margin 32rpx 36rpx 0rpx 34rpx
			background: #FFFFFF;
			box-shadow: 4rpx 4rpx 46rpx 0rpx rgba(125, 132, 158, 0.1);
			border-radius: 32rpx;
			.item
				display flex
				align-items center
				margin-left 30rpx
				padding-right 32rpx
				font-size 28rpx
				border-bottom 1rpx solid #F1F1F1;
				&:last-of-type
					border-bottom 0rpx
				& > view
					padding-top 30rpx
					padding-bottom 10rpx
					&:first-of-type
						flex 1
						color #66666E
						font-family: PingFangSC-Regular, PingFang SC;
						font-weight: 400;
					&:last-of-type
						font-family: PingFangSC-Medium, PingFang SC;
						font-weight: 500;
						color: #33333E;
					&.un-pay
						color #FFC000
					&.pay
						color #00B85C
		.fill
			flex 1
		.customer-service
			display flex
			align-items center
			justify-content center
			color #66666E
			font-size 28rpx
			margin 88rpx 0rpx
			& > view
				&:last-of-type
					color #00B1FF
	.empty-list
		text-align center
		color #66666D
		font-size 32rpx
		image
			width 192rpx
			margin 23.5vh 0 5vh
		  view
			line-height 44rpx
