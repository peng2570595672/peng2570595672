@keyframes btnFadeIn {
  from {
    opacity: 0.2;
  }
  to {
    opacity: 1;
  }
}
@keyframes btnFadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0.2;
  }
}
.step-temp {
  display: flex;
  height: 100rpx;
  padding: 0rpx 12rpx;
  font-size: 24rpx;
}
.step-temp > view {
  margin-top: -20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-items: center;
  justify-content: center;
  margin: 0rpx 2rpx;
}
.step-temp > view:nth-child(2) {
  flex: 1;
}
.step-temp > view:nth-child(2) > view::before {
  content: '';
  flex: 1;
  border: 1rpx dashed rgba(0,0,0,0.1);
}
.step-temp > view:last-of-type {
  flex: 1;
}
.step-temp > view:last-of-type > view::before {
  content: '';
  flex: 1;
  border: 1rpx dashed rgba(0,0,0,0.1);
}
.step-temp > view.pre > view::before {
  border: 1rpx dashed #00b85c;
}
.step-temp > view.pre > view > text {
  color: #fff;
  background: #00b85c;
}
.step-temp > view > view {
  position: relative;
  display: flex;
  align-items: center;
  width: 100%;
}
.step-temp > view > view > text {
  position: relative;
  display: flex;
  margin: 0rpx 20rpx;
  width: 36rpx;
  height: 36rpx;
  line-height: 30rpx;
  align-items: center;
  justify-content: center;
  justify-items: center;
  font-size: 22rpx;
  border-radius: 50%;
  color: #fff;
  background: rgba(0,0,0,0.2);
}
.express_information {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
}
.express_information .title {
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  height: 56rpx;
  font-size: 34rpx;
  font-weight: 500;
}
.express_information .title::before {
  margin-right: 10rpx;
  content: '';
  width: 8rpx;
  height: 44rpx;
  background: #00b85c;
}
.express_information .title > text {
  line-height: 56rpx;
  margin-left: 12rpx;
  font-size: 24rpx;
  color: #7a7f83;
}
.express_information .input-wrapper {
  padding: 0 32rpx;
}
.express_information .input-wrapper .input-item {
  position: relative;
  display: flex;
  align-items: center;
  padding-top: 28rpx;
  padding-bottom: 20rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.express_information .input-wrapper .input-item > view {
  display: flex;
  align-items: center;
  flex: 0 0 148rpx;
  height: 48rpx;
  color: #99999d;
}
.express_information .input-wrapper .input-item > input,
.express_information .input-wrapper .input-item > picker {
  display: flex;
  align-items: center;
  flex: 1;
  height: 48rpx;
  font-size: 28rpx;
}
.express_information .input-wrapper .input-item .detail-info {
  width: 580rpx !important;
}
.express_information .input-wrapper .input-item .picker {
  color: #ccc;
}
.express_information .input-wrapper .input-item .picker.have-value {
  color: #33333d;
}
.express_information .input-wrapper .input-item .auto-fill {
  height: 48rpx;
  line-height: 46rpx;
  background: linear-gradient(to right, #15ad69, #84d157);
  -webkit-background-clip: text;
  color: transparent;
  font-size: 24rpx;
  font-weight: 500;
  box-sizing: content-box;
}
.express_information .input-wrapper .input-item .iconfont::before {
  display: inline-block;
  content: '';
  flex: 0 0 28rpx;
  width: 28rpx;
  height: 40rpx;
  background: url("data:image/png;base64,") no-repeat center;
  -webkit-background-size: contain;
  background-size: contain;
}
.express_information .express-fee {
  display: flex;
  flex-direction: column;
  padding: 32rpx 32rpx 14rpx 32rpx;
  font-size: 28rpx;
  color: #99999d;
  border-bottom: 1rpx solid #e8e8e8;
}
.express_information .express-fee > view:nth-child(2) {
  margin-top: 24rpx;
}
.express_information .tips {
  margin-top: 22rpx;
  color: #ffc000;
  line-height: 40rpx;
  font-size: 20rpx;
  padding-left: 30rpx;
}
.express_information .fill {
  flex: 1;
}
.express_information .agreement {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 48rpx;
  font-size: 24rpx;
  color: #99999d;
}
.express_information .agreement > text {
  color: #2fb565;
}
.express_information .agreement::before {
  margin-right: 10rpx;
  width: 28rpx;
  height: 28rpx;
  content: '\e6e4';
  font-size: 30rpx;
  color: #99999d;
  border-radius: 50%;
}
.express_information .agreement.selected::before {
  color: #84d157;
}
.express_information .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 20rpx 60rpx 40rpx;
  height: 98rpx;
  font-size: 40rpx;
  font-weight: 500;
  color: #fff;
  border-radius: 49rpx;
  background: linear-gradient(45deg, #00af62 0%, #68d33f 100%);
}
.express_information .btn.available {
  animation: btnFadeIn 0.3s;
  animation-fill-mode: forwards;
}
.express_information .btn.unavailable {
  animation: btnFadeOut 0.3s;
  animation-fill-mode: forwards;
}
