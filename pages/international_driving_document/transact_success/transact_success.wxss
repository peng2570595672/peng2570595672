.signed_successfully {
  display: flex;
  flex-direction: column;
  background: #fff;
  min-height: 100vh;
}
.signed_successfully .title {
  margin-top: 8%;
  text-align: center;
  font-size: 34rpx;
  font-family: Helvetica;
  color: #000;
  line-height: 40rpx;
}
.signed_successfully image {
  display: block;
  margin: 28rpx auto 0;
  height: 238rpx;
  width: 304rpx;
}
.signed_successfully .signed_title {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 76rpx;
  font-size: 44rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
}
.signed_successfully .tips {
  margin: 32rpx 46rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #66666e;
}
.signed_successfully .order-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 300rpx 32rpx 32rpx 32rpx;
  height: 98rpx;
  color: #fff;
  font-size: 32rpx;
  background: linear-gradient(45deg, #00af62 0%, #68d33f 100%);
  border-radius: 50rpx;
}
.signed_successfully .return-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0rpx 32rpx;
  font-weight: 500;
  height: 98rpx;
  font-size: 32rpx;
  color: #00b85c;
  border-radius: 50rpx;
  border: 2rpx solid #00b85c;
}
.signed_successfully .customer-service {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #66666e;
  font-size: 28rpx;
  margin-top: 44rpx;
  margin-bottom: 88rpx;
}
.signed_successfully .customer-service > view:last-of-type {
  color: #00b1ff;
}
