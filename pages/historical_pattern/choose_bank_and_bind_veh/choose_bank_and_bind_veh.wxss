@keyframes btnFadeIn {
  from {
    opacity: 0.2;
  }
  to {
    opacity: 1;
  }
}
@keyframes btnFadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0.2;
  }
}
.page-wrapper {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
}
.page-wrapper .wrapper-content {
  display: flex;
  flex-direction: column;
  padding: 0 30rpx;
}
.page-wrapper .wrapper-content .title {
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  height: 56rpx;
  font-size: 34rpx;
  font-weight: 500;
  font-family: PingFangSC-Medium, PingFang SC;
  color: #000;
}
.page-wrapper .wrapper-content .title::before {
  margin-right: 12rpx;
  content: '';
  width: 8rpx;
  height: 44rpx;
  background: #2fb565;
}
.page-wrapper .wrapper-content .note {
  color: #000;
  font-weight: Regular;
  font-size: 24rpx;
  opacity: 0.3;
  margin-top: 8rpx;
}
.page-wrapper .wrapper-content .check_item {
  display: flex;
  height: 138rpx;
  align-items: center;
  margin-top: 40rpx;
  background: #f4f4f4;
  opacity: 0.63;
}
.page-wrapper .wrapper-content .check_item .icon_004 {
  margin-left: 32rpx;
  margin-right: 24rpx;
}
.page-wrapper .wrapper-content .check_item .icon_004::before {
  content: '\e672';
  font-size: 48rpx;
  color: #009c96;
}
.page-wrapper .wrapper-content .check_item .name {
  flex: 1;
}
.page-wrapper .wrapper-content .check_item .name .top {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  line-height: 40rpx;
  color: #000;
}
.page-wrapper .wrapper-content .check_item .name .bottom {
  font-size: 24rpx;
  line-height: 40rpx;
  color: rgba(0,0,0,0.5);
}
.page-wrapper .wrapper-content .check_item .choice {
  font-size: 24rpx;
  color: #526c99;
  margin-right: 32rpx;
}
.page-wrapper .wrapper-content .bottom-note {
  margin: 0 auto;
  font-size: 24rpx;
  margin-top: 34rpx;
  color: rgba(0,0,0,0.5);
}
.page-wrapper .fill {
  margin-top: 64rpx;
  height: 20rpx;
  background: #fafafa;
}
