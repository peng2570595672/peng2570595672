.install_step {
  padding: 0 50rpx;
}
.install_step > image {
  position: relative;
  left: 12rpx;
  z-index: 1;
  margin: 240rpx auto 88rpx;
  display: block;
  height: 208rpx;
}
.install_step .text-wrapper {
  position: relative;
  z-index: 10;
}
.install_step .text-wrapper > view {
  text-align: center;
  font-size: 28rpx;
}
.install_step .text-wrapper > view.ok {
  color: #84d157 !important;
}
.install_step .text-wrapper > view.title {
  font-size: 36rpx;
  font-weight: bold;
}
.install_step .text-wrapper > view.tips {
  font-size: 28rpx;
  font-weight: normal;
  line-height: 44rpx;
  color: #f19c38;
  text-align: left;
  padding-top: 58rpx;
}
.install_step .text-wrapper > view.tips.tips-last {
  text-align: center;
}
.install_step .text-wrapper > view.devName {
  margin-top: 25rpx;
}
.install_step .text-wrapper > view.devName > text.succeed {
  color: #69e2ac;
}
.install_step .text-wrapper > view.devName > text.failed {
  color: #fa5151;
}
.install_step .text-wrapper > view.cardNum {
  margin-top: 6rpx;
  color: #fa5151;
}
.install_step .text-wrapper > view.errMsg {
  margin-top: 6rpx;
  color: #fa5151;
}
.install_step .btns {
  position: fixed;
  left: 30rpx;
  right: 30rpx;
  bottom: 140rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 98rpx;
  line-height: 98rpx;
  color: #fff;
  font-size: 32rpx;
  background: #2a4f44;
  border-radius: 49rpx;
}
.install_step .btns:active {
  opacity: 0.8;
}
.install_step .btns.not-use,
.install_step .btns:active.not-use {
  opacity: 1;
  background: #ccc;
}
.install_step .btns.btn-fail {
  background: #fa5151;
}
.install_step .retry {
  position: fixed;
  width: 100%;
  text-align: center;
  bottom: 290rpx;
  font-size: 28rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #39812b;
  line-height: 36rpx;
}
.alert-for-not-finished {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0,0,0,0.6);
  z-index: 10000;
}
.alert-for-not-finished-content {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 694rpx;
  height: 482rpx;
  text-align: justify;
  background: #fff;
  box-shadow: 4rpx 12rpx 56rpx 0px rgba(102,102,109,0.19);
  border-radius: 16rpx;
  margin: auto;
  z-index: 100002;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-title {
  text-align: center;
  font-size: 44rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #1e1e1e;
  line-height: 52rpx;
  padding: 48rpx 0 34rpx;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-con {
  padding: 0 48rpx;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #262626;
  line-height: 54rpx;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-con image {
  width: 590rpx;
  height: 234rpx;
  margin-bottom: 24rpx;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-con .step1 {
  margin-bottom: 8rpx;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-con .step2 {
  color: #979797;
}
.alert-for-not-finished-content .dialog-container .fill {
  flex: 1;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-btn {
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  padding-bottom: 48rpx;
}
.alert-for-not-finished-content .dialog-container .wx-dialog-btn .confirm {
  width: 318rpx;
  height: 106rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #2a4f44;
  border-radius: 53rpx 53rpx 53rpx 53rpx;
  font-size: 36rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #fff;
}
.alert-for-not-finished-content .dialog-container .close {
  display: flex;
  color: $fontColor6;
  font-size: 16rpx;
  padding: 30rpx 0 50rpx;
}
.alert-for-not-finished-content .dialog-container .close::after {
  flex: 1;
  content: '\e603';
  text-align: right;
}
