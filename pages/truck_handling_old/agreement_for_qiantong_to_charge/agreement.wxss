.container {
  padding: 32rpx;
  font-size: 28rpx;
}
.protocol {
  padding: 20rpx;
}
.protocol .h1 {
  display: block;
  font-size: 48rpx;
  color: #4d4d4d;
  text-align: center;
  padding-bottom: 20rpx;
}
.protocol .h2 {
  display: block;
  font-size: 32rpx;
  color: #4d4d4d;
}
.protocol .h3 {
  display: block;
  font-size: 32rpx;
  color: #4d4d4d;
  text-align: center;
  padding: 32rpx;
}
.protocol .p {
  display: block;
  text-align: left;
  font-size: 24rpx;
  color: #4d4d4d;
  line-height: 40rpx;
  text-indent: 2em;
}
.protocol .p image {
  width: 70%;
}
.protocol .p1 {
  display: block;
  text-align: left;
  font-size: 24rpx;
  color: #4d4d4d;
  line-height: 40rpx;
}
.protocol .p1 image {
  width: 70%;
}
.protocol .weight {
  font-weight: bold;
}
.protocol .line {
  text-decoration: underline;
}
.protocol .inline {
  display: inline-block;
  width: 100rpx;
  border-bottom: 1rpx solid #4d4d4d;
}
.protocol .fill {
  width: 100rpx;
  display: inline-block;
}
.protocol .right {
  text-align: right;
  padding-right: 100rpx;
}
