.iconfont {
  font-family: 'iconfont' !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.index {
  margin: 0;
  padding: 0;
  width: 100vw;
  min-height: 100vh;
  background-color: #2a5044;
}
.index .top {
  position: relative;
  width: 100vw;
  height: 628rpx;
}
.index .top .swiper {
  width: 100%;
  height: 100%;
}
.index .top .swiper .swiperItem {
  width: 100%;
  height: 100%;
}
.index .top .swiper .swiperItem .image {
  width: 100%;
  height: 100%;
}
.index .top .regular {
  pointer-eventsnone: none;
}
.index .top .regular .regular-item {
  pointer-eventsauto: auto;
}
.index .top .navbar {
  width: 100vw;
  color: #fff;
  text-align: center;
  position: absolute;
  top: 0;
}
.index .top .navbar .title {
  font-size: 34rpx;
  font-family: PingFang SC-Ultralight, PingFang SC;
  font-weight: 100;
  color: #fff;
}
.index .top .navbar .title .goBack::before {
  width: 50rpx;
  height: 50rpx;
  content: "\e60e";
  color: #fff;
  font-size: 34rpx;
  position: absolute;
  left: 20rpx;
}
.index .module {
  margin: 0rpx 28rpx 28rpx 28rpx;
  background: #f8f8f8;
  border-radius: 16rpx 16rpx 16rpx 16rpx;
}
.index .moduleOne {
  height: 962rpx;
  margin-top: -100rpx;
  position: relative;
  top: 0rpx;
}
.index .moduleOne .image {
  width: 100%;
  height: 100%;
}
.index .moduleTwo {
  height: 426rpx;
}
.index .moduleTwo .image {
  width: 100%;
  height: 100%;
}
.index .moduleThree {
  height: 660rpx;
}
.index .moduleThree .image {
  width: 100%;
  height: 100%;
}
.index .describe {
  margin: 0rpx 44rpx 28rpx 44rpx;
  font-family: PingFang SC-Medium, PingFang SC;
}
.index .describe .title {
  font-size: 40rpx;
  font-weight: 500;
  color: #fff;
  line-height: 56rpx;
}
.index .describe .text {
  font-size: 22rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #fff;
  line-height: 40rpx;
  margin-bottom: 42rpx;
}
.index .describe .text .name {
  margin-right: 20rpx;
  font-size: 32rpx;
  font-weight: 500;
}
.index .favourable {
  width: 100vw;
  height: 70rpx;
  background-color: #f0d798;
  position: fixed;
  bottom: 230rpx;
  display: flex;
  align-items: center;
}
.index .favourable .leftImg {
  margin: 0rpx 14rpx 0rpx 30rpx;
  width: 38rpx;
  height: 44rpx;
  background: url("https://file.cyzl.com/g001/M01/E0/BE/oYYBAGRtqMeAUfE6AAAIks5nN7g899.png") center no-repeat;
  background-size: cover;
}
.index .favourable .textTime {
  font-size: 22rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #ec602a;
}
.index .favourable .textTime .text {
  margin-right: 16rpx;
}
.index .favourable .textTime .time {
  display: inline-block;
  color: #fff;
  width: 40rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  background: #ec602a;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
}
.index .split {
  height: 260rpx;
  overflow: hidden;
}
.index .handleImmediately {
  width: 100vw;
  height: 244rpx;
  background: #fff;
  border-radius: 16rpx 16rpx 0px 0px;
  position: fixed;
  bottom: 0;
  opacity: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.index .handleImmediately .button {
  width: 662rpx;
  height: 110rpx;
  color: #fff;
  background: #2a4f44;
  line-height: 110rpx;
  box-shadow: 0rpx 8rpx 20rpx 0rpx rgba(0,0,0,0.302);
  border-radius: 64rpx;
}
