.page
	width 100vw
	height 100vh
	overflow hidden
	background-color #69B064
	.top-div
		width 100%
		image
			width 100%
	.btn-container
		text-align center
		margin-top -20rpx
		.btn
			width 90%
			height 114rpx
			line-height 114rpx
			background rgba(247,181,0,1)
			box-shadow 0px 10rpx 16rpx 0px rgba(220,162,0,0.31)
			border-radius 69rpx
			font-size 48rpx
			font-family PingFangSC-Semibold,PingFang SC
			font-weight 600
			color rgba(255,255,255,1)
	.rules
		padding 100rpx 100rpx 20rpx 54rpx
		font-size 26rpx
		font-family PingFangSC-Regular,PingFang SC;
		font-weight 400
		color rgba(255,255,255,1)
		line-height 48rpx
