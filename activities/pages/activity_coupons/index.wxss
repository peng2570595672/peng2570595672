  display: flex;
  flex-direction: column;
  padding-right: 15rpx;
}
.page .wen-an-container .item .white-title-container .dot-left .dot-item .dot {
  height: 10rpx;
  width: 10rpx;
  border-radius: 50%;
}
.page .wen-an-container .item .white-title-container .dot-left .dot-item .dot:nth-child(1) {
  margin-bottom: 5rpx;
}
.page .wen-an-container .item .white-title-container .dot-left .dot-item:nth-child(1) .dot {
  background-color: rgba(255,255,255,0.2);
