.page {
  width: 100vw;
  height: 100vh;
  overflow-x: hidden;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  background-color: #f8393b;
}
.page .banner-container {
  position: relative;
  width: 100vw;
  padding-top: 68rpx;
}
.page .banner-container .top-div {
  position: absolute;
  top: 0;
  left: 0;
  height: 68rpx;
  width: 100vw;
  background-color: #ffab66;
}
.page .banner-container .banner-container {
  width: 100vw;
  height: 848rpx;
  background-image: url("https://file.cyzl.com/g001/M05/1E/CF/oYYBAF7diKGALTmuAB7mdKz91Kk894.png");
  background-size: 100% 100%;
  background-color: #ffab66;
}
.page .banner-container .title-container {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  width: 100vw;
  top: 302rpx;
  left: 0;
}
.page .banner-container .title-container .char {
  padding: 0 20rpx;
  font-size: 36rpx;
  color: #fff;
}
.page .banner-container .title-container .line-dot-left {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.page .banner-container .title-container .line-dot-left .dot {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: #fff;
}
.page .banner-container .title-container .line-dot-left .line {
  margin-right: 10rpx;
  width: 200rpx;
  height: 2rpx;
  background: linear-gradient(-90deg, #fff 0%, rgba(255,255,255,0) 100%);
}
.page .banner-container .title-container .line-dot-right {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.page .banner-container .title-container .line-dot-right .line {
  width: 200rpx;
  height: 2rpx;
  background: linear-gradient(90deg, #fff 0%, rgba(255,255,255,0) 100%);
}
.page .banner-container .title-container .line-dot-right .dot {
  margin-right: 10rpx;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: #fff;
}
.page .banner-container .btn-container {
  position: absolute;
  left: 0;
  top: 710rpx;
  width: 100vw;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page .banner-container .btn-container .btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 550rpx;
  font-size: 36rpx;
  color: #fff;
  height: 90rpx;
  background: linear-gradient(180deg, #ffd748 0%, #f86b28 100%);
  border-radius: 45rpx;
}
.page .banner-container .btn-container .btn.gray-btn {
  color: rgba(255,255,255,0.7);
  background: none;
  background-color: rgba(204,204,204,0.8);
}
.page .wen-an-container {
  position: relative;
  top: -60rpx;
  width: 90%;
  background-color: #f8393b;
  margin: 0 auto;
}
.page .wen-an-container .item {
