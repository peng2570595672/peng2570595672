overflow-y scroll .content .item {
  position: relative;
  padding-bottom: 40rpx;
  border-left: 4rpx solid #2a4f44;
}
overflow-y scroll .content .item::before {
  display: inline-block;
  content: '';
  width: 17rpx;
  height: 17rpx;
  border-radius: 50%;
  background: #2a4f44;
  position: absolute;
  top: -8rpx;
  left: -10rpx;
}
overflow-y scroll .content .item:last-of-type {
  border-left: 0;
  padding-bottom: 10rpx;
}
overflow-y scroll .content .item:last-of-type::before {
  left: -8rpx;
}
overflow-y scroll .content .item .content-title {
  position: relative;
  padding-left: 32rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #2a4f44;
  transform: translateY(-24rpx);
}
overflow-y scroll .content .item .content-des {
  display: flex;
  margin-top: 10rpx;
  padding-left: 32rpx;
  padding-right: 6rpx;
  line-height: 40rpx;
  color: #66666d;
  font-size: 24rpx;
  font-weight: 400;
  transform: translateY(-24rpx);
}
overflow-y scroll .close {
  width: 70rpx;
  height: 70rpx;
  overflow: hidden;
  border-radius: 50%;
  border: 7.3rpx solid #fff;
  position: absolute;
  bottom: -150rpx;
  left: 50%;
  transform: translateX(-50%);
}
overflow-y scroll .close::before {
  width: 70rpx;
  height: 70rpx;
  display: block;
  content: '\e63d';
  font-size: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 70rpx;
}
