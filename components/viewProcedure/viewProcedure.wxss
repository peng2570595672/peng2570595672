.alert-for-not-finished {
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	background: rgba(0, 0, 0, 0.6);
	z-index: 10000;
}

.alert-for-not-finished-content {
	position: fixed;
	left: 0;
	right: 0;
	top: 50%;
	width: 650rpx;
	height: auto;
	margin: 0 auto;
	overflow: hidden;
	background: #fff;
	border-radius: 16rpx;
	padding-bottom: 10rpx;
	transform: translate(0, -50%);
	box-shadow: 4rpx 12rpx 56rpx 0px rgba(102, 102, 109, 0.19);
	z-index: 100002;
}

/* 自助办理流程 */
.alert-for-not-finished-content .moduleOne {
	height: 100%;
	padding: 0 20rpx;
}

.alert-for-not-finished-content .communal .title {
	width: 100%;
	height: 100rpx;
	font-size: 34rpx;
	font-weight: bold;
	text-align: center;
	line-height: 100rpx;
}

.alert-for-not-finished-content .moduleOne .topImg {
	height: 200rpx;
	border-radius: 16rpx;
	background-color: blanchedalmond;
}

.alert-for-not-finished-content .moduleOne .content {
	height: 200rpx;
	margin-top: 20rpx;
	border-radius: 16rpx;
	background-color: rgb(98, 154, 192);
}


