@import "animate.styl"
//字体图标
@import "../../fonts/iconfont.styl"
.iconfont {
	font-family "iconfont" !important
	font-size16px
	font-stylenormal
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
}
.alert-for-not-finished
	position fixed
	left 0
	right 0
	top 0
	bottom 0
	background rgba(0, 0, 0, 0.6)
	z-index 10000
.alert-for-not-finished-content
	position absolute
	left 0
	right 0
	top 50%
	transform translate(0, -50%)
	width 650rpx
	height 850rpx
	background rgba(255,255,255,1)
	box-shadow 4rpx 12rpx 56rpx 0px rgba(102,102,109,0.19)
	border-radius 16rpx
	margin 0 auto
	z-index 100002
	/* 自助办理流程 */
	.moduleOne 
		height 100%
		padding 0 20rpx
		.topImg 
			height 200rpx
			border-radius 16rpx
			background-color blanchedalmond
		.content
			height 200rpx
			margin-top 20rpx
			border-radius 16rpx
			background-color rgb(98, 154, 192)
	.communal .title
		width 100%
		height 100rpx
		font-size 34rpx
		font-weight bold
		text-align center
		line-height 100rpx
	/* 热门问答 */
	.moduleTwo
		width 610rpx
		height 60vh
		padding 0 20rpx
		.substance
			height auto
			text-align justify
			margin-bottom 10rpx
			padding-bottom 30rpx
			.subTitle
				color #4D4D51
				font-size 28rpx
				font-weight 500
				line-height 44rpx
				font-family PingFangSC-Medium, PingFang SC
			.text
				color #4D4D51
				font-size 22rpx
				font-weight 400
				line-height 40rpx
				font-family PingFangSC-Regular, PingFang SC

