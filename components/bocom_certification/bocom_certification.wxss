.animated {
  -webkit-animation-duration: 0.4s;
  animation-duration: 0.4s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}
@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
.fadeIn {
  -webkit-animation-name: fadeIn;
  animation-name: fadeIn;
}
@-webkit-keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
.fadeOut {
  -webkit-animation-name: fadeOut;
  animation-name: fadeOut;
}
@-webkit-keyframes zoomIn {
  from {
    opacity: 0;
    -webkit-transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  50% {
    opacity: 1;
  }
}
.zoomIn {
  -webkit-animation-name: zoomIn;
  animation-name: zoomIn;
}
@-webkit-keyframes zoomOut {
  from {
    opacity: 1;
  }
  50% {
    opacity: 0;
    -webkit-transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  to {
    opacity: 0;
  }
}
.zoomOut {
  -webkit-animation-name: zoomOut;
  animation-name: zoomOut;
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@keyframes zoomIn {
  from {
    opacity: 0;
    -webkit-transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  50% {
    opacity: 1;
  }
}
@keyframes zoomOut {
  from {
    opacity: 1;
  }
  50% {
    opacity: 0;
    -webkit-transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  to {
    opacity: 0;
  }
}
.alert-for-not-finished .mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0,0,0,0.6);
  z-index: 10000;
}
.alert-for-not-finished .alert-for-not-finished-content {
  position: absolute;
  left: 0;
  right: 0;
  top: 37%;
  transform: translate(0, -50%);
  width: 640rpx;
  height: 644rpx;
  background: #fff;
  box-shadow: 4rpx 12rpx 56rpx 0px rgba(102,102,109,0.19);
  border-radius: 32rpx;
  margin: 0 auto;
  z-index: 10001;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box {
  height: 644rpx;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 2;
  color: #33333d;
  background: #fff;
  padding: 0 32rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  text-align: center;
  font-size: 40rpx;
  font-weight: 500;
  font-family: PingFangSC-Medium, PingFang SC;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .top-content {
  text-align: center;
  font-size: 34rpx;
  line-height: 48rpx;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .top-content .title {
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #000;
  padding: 48rpx 0 14rpx;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .top-content .account {
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #66666e;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .top-content .money {
  font-size: 68rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #00b85c;
  line-height: 96rpx;
  padding: 22rpx 0;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .input-item {
  position: relative;
  display: flex;
  align-items: center;
  padding-bottom: 12rpx;
  font-size: 32rpx;
  color: rgba(0,0,0,0.5);
  border-bottom: 1rpx solid #e8e8e8;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .input-item > view {
  display: flex;
  align-items: center;
  height: 48rpx;
  color: $fontColor9;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .input-item > input {
  display: flex;
  align-items: center;
  flex: 1;
  height: 48rpx;
  font-size: 32rpx;
  text-align: left;
  margin-left: 60rpx;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .input-item .validate-code {
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  height: 44rpx;
  font-size: 32rpx;
  color: $fontColor9;
  box-sizing: border-box;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .input-item .validate-code.ok {
  font-weight: 500;
  color: $fontColor;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .input-item .validate-code.acquisition {
  color: $fontColor;
  font-weight: 400 !important;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .tips {
  text-align: left;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(76,76,76,0.8);
  line-height: 34rpx;
  padding-top: 22rpx;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .alert-for-button {
  width: 240rpx;
  justify-content: center;
  font-size: 28rpx;
  height: 82rpx;
  margin: 0 auto;
  border-radius: 8rpx;
  background: #00b85c;
  line-height: 82rpx;
  margin-top: 96rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.alert-for-not-finished .alert-for-not-finished-content .alert-for-box .alert-for-button .btn {
  display: inline-block;
  color: #fff;
}
