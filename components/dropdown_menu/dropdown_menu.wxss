@font-face {
	font-family: 'iconfont';  /* project id 1514406 */
	src: url('//at.alicdn.com/t/font_1514406_rx3tpwk54p.eot');
	src: url('//at.alicdn.com/t/font_1514406_rx3tpwk54p.eot?#iefix') format('embedded-opentype'),
	url('//at.alicdn.com/t/font_1514406_rx3tpwk54p.woff2') format('woff2'),
	url('//at.alicdn.com/t/font_1514406_rx3tpwk54p.woff') format('woff'),
	url('//at.alicdn.com/t/font_1514406_rx3tpwk54p.ttf') format('truetype'),
	url('//at.alicdn.com/t/font_1514406_rx3tpwk54p.svg#iconfont') format('svg');
}
.iconfont {
	font-family:"iconfont" !important;
	font-size:16px;
	font-style:normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.icon-sanjiao:before { content: "\e63e"; }
.page {
	width: 100%;
	height: 100%;
	background: #fff;
	overflow: hidden;
}

.position {
	position: absolute;
}

.nav {
	position: fixed;
	top: 0rpx;
	z-index: 999;
	display: flex;
	width: 100%;
	padding: 26rpx 0;
	height: 36rpx;
	line-height: 36rpx;
	color: #33333D;
	/*border-bottom: 1px solid #f7f7f7;*/
	background: #E6E6E6
}
.is_owes{
	top: 120rpx;
}
.nav>view:first-child {
	border-right: 1rpx solid #979797;
}
.nav-child {
	display: flex;
	flex: 1;
	text-align: center;
	align-items: center;
	justify-content: center;
	font-size: 32rpx;
}
.nav-title {
	display: inline-block;
}

.icon{
	color: #99999D;
	font-size: 24rpx;
	transform: rotateZ(-90deg);
	margin-left: 20rpx;
}

.container {
	position: relative;
	z-index: 4;
	font-size: 14px;
}


.slidedown {
	transform: translateY(0%);
}

.district {
	position: fixed;
	top:0;
	z-index: 111;
	width: 100%;
	height: 100%;
	background-color: rgba(0, 0, 0, 0.5);
	overflow: scroll;
}

.half-center,.half-top {
	background: #fff;
}
.half-top-box{
	padding: 0 30rpx 0  68rpx;
	background: #fff;
}
.half-top{
	width:100%;
	overflow-x: auto;
	white-space:nowrap;
	font-size: 28rpx;
	height: 100rpx;
	line-height: 100rpx;
}
.half-top view{
	text-align: center;
	display: inline-block;
}
.half-bottom {
	height: 392rpx;
	line-height: 98rpx;
	font-size: 28rpx;
	background: #fff;
}

.half-bottom>view {
	position: relative;
	height: 98rpx;
	float: left;
	width: 33.2%;
	box-sizing: border-box;
	background: #fff;
	text-align: center;
	/*border-bottom: 1px solid #f7f7f7;*/
}
.half-bottom>view .line{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 2rpx;
	background: linear-gradient(to right, #fff, #E9E9EA,#fff);
}
.half-bottom>view:nth-child(3n-2) .line{
	background: linear-gradient(to right, #fff, #F4F4F5)
}
.half-bottom>view:nth-child(3n-1) .line{
	background: linear-gradient(to right, #F4F4F5, #E8E8E9, #F4F4F5);
}
.half-bottom>view:nth-child(3n) .line{
	background: linear-gradient(to right, #F4F4F5, #fff);
}
.half-top text{
	display: inline-block;
	width:158rpx;
	height:60rpx;
	line-height:60rpx;
	text-align: center;
	font-size:32rpx;
	margin-top: 20rpx;
	margin-right: 90rpx;
}
.current_left_select text{
	background:linear-gradient(90deg,rgba(22,173,105,1) 0%,rgba(144,213,107,1) 100%);
	box-shadow:0px 8rpx 10rpx 0px rgba(34,174,111,0.2);
	border-radius:30rpx;
	color:rgba(255,255,255,1);
}
.current_right_select text{
	display: inline-block;
	width:72rpx;
	height:72rpx;
	line-height:72rpx;
	border-radius:36rpx;
	text-align: center;
	font-size:32rpx;
	color:rgba(255,255,255,1);
	background:linear-gradient(180deg,rgba(23,173,105,1) 0%,rgba(153,217,119,1) 100%);
	box-shadow:0px 8rpx 10rpx 0px rgba(25,174,105,0.2);
	margin-top: 9rpx;
}
.half-top>view:last-child text{
	margin-right: 0rpx !important;
}
.year0 text{
	margin-right: 0rpx !important;
}
.nav-child.active .nav-title {
	color: #16AD69;
}

.nav-child.active .icon {
	color: #16AD69;
	transform: rotateZ(90deg);
	transition:.4s;
}

@keyframes slidown {
	from {
		transform: translateY(-100%);
	}

	to {
		transform: translateY(0%);
	}
}

.slidown {
	display: block;
	animation: slidown 0.2s ease-in both;
}

@keyframes slidup {
	from {
		transform: translateY(0%);
	}

	to {
		transform: translateY(-100%);
	}
}
.slidup {
	display: block;
	animation: slidup 0.2s ease-in both;
}

.show {
	display: block;
}

.slidowntop {
	display: flex;
	flex-direction: row;
	padding: 20rpx 24rpx 10rpx 24rpx;
}

.emptyall {
	margin-left: 475rpx;
	color: #16AD69;
}

.emptyallright {
	width: 80rpx;
	text-align: center;
}

.endselect {
	width: 350rpx;
	text-align: left;
}

.return {
	margin-left: 200rpx;
	color: #16AD69;
}

.slidowncenter {
	margin-top: 20rpx;
	padding-top: 20rpx;
	padding-left: 24rpx;
	display: flex;
	flex-direction: row;
	align-content: space-between;
	border-top: solid #d1d3d4 1rpx;
}

.slidownbottom {
	margin-top: 10rpx;
	padding: 10rpx;
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
}

.sortitem {
	position: relative;
	font-size: 28rpx;
	height: 98rpx;
	line-height: 98rpx;
	padding-left: 36rpx;
	justify-content: space-between;
}
.sortitem .line{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 2rpx;
	background: linear-gradient(to right, #fff, #E9E9EA,#fff);
}
.forbidden-choose{
	color: #ccc;
}
.sortitem.active {
	text-indent: 18rpx;
	color: #fff;
}
.sortitem text{
	display: inline-block;
	width:170rpx;
	height:60rpx;
	line-height: 60rpx;
	text-align: center;
	margin-top: 15rpx;
}
.sortitem.active text{
	font-size: 32rpx;
	background:linear-gradient(90deg,rgba(22,173,105,1) 0%,rgba(144,213,107,1) 100%);
	border-radius:30rpx;
}
.container_hd,.top,.mask-box {
	width: 100%;
	height: 100%;
	position: fixed;
	z-index: 9;
	background-color: rgba(0, 0, 0, 0.5);
}
.top{
	z-index: 10;
}
.mask-box{
	z-index: 11;
	top: 86rpx;
	height: 420rpx;
	background: #fff;
}
.z-height {
	width: 100%;
	position: fixed;
	z-index: 11;
	top: 86rpx;
}
.is_owe .mask-box,.is_owe .z-height{
	top: 206rpx;
}
.z-height>view{
	background: #fff;
}
.animated {
	color: #99999D;
	font-size: 28rpx;
	-webkit-animation-duration: 0.4s;
	animation-duration: 0.4s;
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;
}
@-webkit-keyframes show {
	from {
		opacity: 0;
	}
	to {
		opacity: 1;
	}
}
.show {
	-webkit-animation-name: fadeIn;
	animation-name: fadeIn;
}
@-webkit-keyframes disappear {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}
.disappear {
	-webkit-animation-name: fadeOut;
	animation-name: fadeOut;
}
@-webkit-keyframes zoomIn {
	from {
		opacity: 0;
		-webkit-transform: scale3d(0.3, 0.3, 0.3);
		transform: scale3d(0.3, 0.3, 0.3);
	}
	50% {
		opacity: 1;
	}
}
@-webkit-keyframes slideInDown {
	from {
		-webkit-transform: translate3d(0, -100%, 0);
		transform: translate3d(0, -100%, 0);
		visibility: visible;
	}

	to {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0);
	}
}

@keyframes slideInDown {
	from {
		-webkit-transform: translate3d(0, -100%, 0);
		transform: translate3d(0, -100%, 0);
		visibility: visible;
	}

	to {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0);
	}
}

.slideInDown {
	-webkit-animation-name: slideInDown;
	animation-name: slideInDown;
}
@-webkit-keyframes slideOutDown {
	from {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0);
	}
	to {
		visibility: hidden;
		-webkit-transform: translate3d(0, 100%, 0);
		transform: translate3d(0, -100%, 0);
	}
}
.slideOutDown {
	-webkit-animation-name: slideOutDown;
	animation-name: slideOutDown;
}
@keyframes slideOutDown {
	from {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0);
	}
	to {
		visibility: hidden;
		-webkit-transform: translate3d(0, 100%, 0);
		transform: translate3d(0, -100%, 0);
	}
}
