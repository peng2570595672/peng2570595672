@charset "UTF-8"
// 以下配置用于stylus编译后不添加前缀 如@-moz-keyframes 小程序会报错
vendors = official
.animated
    -webkit-animation-duration 0.4s
    animation-duration 0.4s
    -webkit-animation-fill-mode both
    animation-fill-mode both
@keyframes fadeIn
    from
        opacity 0
    to
        opacity 1
.fadeIn
    -webkit-animation-name fadeIn
    animation-name fadeIn
@keyframes fadeOut
    from
        opacity 1
    to
        opacity 0
.fadeOut
    -webkit-animation-name fadeOut
    animation-name fadeOut
@keyframes zoomIn
    from
        opacity 0
        -webkit-transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
        transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
    50%
        opacity 1
.zoomIn
    -webkit-animation-name zoomIn
    animation-name zoomIn
@keyframes zoomOut
    from
        opacity 1
    50%
        opacity 0
        -webkit-transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
        transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
    to
        opacity 0
.zoomOut
    -webkit-animation-name zoomOut
    animation-name zoomOut
@keyframes fadeIn
    from
        opacity 0
    to
        opacity 1
@keyframes fadeOut
    from
        opacity 1
    to
        opacity 0
@keyframes zoomIn
    from
        opacity 0
        -webkit-transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
        transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
    50%
        opacity 1
@keyframes zoomOut
    from
        opacity 1
    50%
        opacity 0
        -webkit-transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
        transform translateY(-50%) scale3d(0.3, 0.3, 0.3)
    to
        opacity 0
@font-face
    font-family 'iconfont' /* Project id 1514406 */
    src url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff2?t=1651741878795') format('woff2'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.woff?t=1651741878795') format('woff'), url('//at.alicdn.com/t/font_1514406_r2ex32p9up.ttf?t=1651741878795') format('truetype')
.iconfont
    font-family 'iconfont' !important
    font-size 16px
    font-style normal
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
.alert-for-not-finished
    position fixed
    left 0
    right 0
    top 0
    bottom 0
    background rgba(0, 0, 0, 0.6)
    z-index 100000
.alert-for-not-finished-content
    position fixed
    left 0
    right 0
    top 50%
    width 694rpx
    height auto
    margin 0 auto
    border-radius 16rpx
    transform translate(0, -50%)
    z-index 100002
    .module
        border-radius 16rpx
        padding 40rpx 48rpx
        background-color #fff
        position relative
    .moduleOne
        margin-top -200rpx
    .module 
        .title
            height 60rpx
            font-size 44rpx
            font-weight 500
            color #1E1E1E
            text-align center
            font-family PingFangSC-Medium, PingFang SC
        .contant
            font-size 28rpx
            font-weight 400
            color #262626
            text-align justify
            margin 28rpx 0rpx 64rpx 0rpx
            font-family PingFangSC-Regular, PingFang SC
            .span
                font-size 32rpx
                color #f00
        .btn
            height 106rpx
            font-size 36rpx
            font-weight 400
            color #FFFFFF
            line-height 106rpx
            border-radius 54rpx
            background #2A4F44
            font-family PingFangSC-Regular, PingFang SC
        .close
            width 70rpx
            height 70rpx
            overflow hidden
            border-radius 50%
            border 7.3rpx solid #ffffff
            position absolute
            bottom -200rpx
            left 50%
            transform translateX(-50%)
            &::before
                width 70rpx
                height 70rpx
                display block
                content '\e63d'
                font-size 40rpx
                color #FFFFFF
                text-align center
                line-height 70rpx
    .moduleTwo 
        .bottomBtn
            display flex
            font-size 36rpx
            font-weight 400
            text-align center
            justify-content space-between
            font-family PingFangSC-Regular, PingFang SC
            .cancel
                width 220rpx
                height 96rpx
                color #2A4F44
                line-height 96rpx
                border-radius 54rpx
                background-color #FFFFFF
                border 2rpx solid #2A4F44
            .ok
                width 310rpx
                height 96rpx
                color #FFFFFF
                line-height 96rpx
                border-radius 54rpx
                background #2A4F44
    /* 欠费补缴 */
    .moduleThree 
        .bottomBtn
            width 100%
            display flex
            font-size 36rpx
            font-weight 400
            text-align center
            justify-content space-between
            font-family PingFangSC-Regular, PingFang SC
            .cancel
                display block
                width 220rpx
                height 96rpx
                color #2A4F44
                line-height 96rpx
                border-radius 54rpx
                background-color #FFFFFF
                border 2rpx solid #2A4F44
            .ok
                display block
                width 310rpx
                height 96rpx
                color #FFFFFF
                line-height 96rpx
                border-radius 54rpx
                background #2A4F44
    // 设备升级
    .moduleSix
        width 100vw
        height 670rpx
        margin-left -28rpx
        position relative
        & > .image
            margin-left -10rpx
            width 100%
            height 100%
        .btn
            width 315rpx
            height 90rpx
            border-radius 45rpx
            position absolute
            bottom -23rpx
            left 50%
            transform translateX(-50%)
        .close
            width 60rpx
            height 60rpx
            border-radius 50%
            position absolute
            top 8rpx
            right 30rpx
            .image
                width 100%
                height 100%