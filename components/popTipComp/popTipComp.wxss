.alert-for-not-finished {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0,0,0,0.6);
  z-index: 100000;
}
.alert-for-not-finished-content {
  position: fixed;
  left: 0;
  right: 0;
  top: 50%;
  width: 694rpx;
  height: auto;
  margin: 0 auto;
  border-radius: 16rpx;
  transform: translate(0, -50%);
  z-index: 100002;
/* 欠费补缴 */
}
.alert-for-not-finished-content .module {
  border-radius: 16rpx;
  padding: 40rpx 48rpx;
  background-color: #fff;
  position: relative;
}
.alert-for-not-finished-content .moduleOne {
  margin-top: -200rpx;
}
.alert-for-not-finished-content .module .title {
  height: 60rpx;
  font-size: 44rpx;
  font-weight: 500;
  color: #1e1e1e;
  text-align: center;
  font-family: PingFangSC-Medium, PingFang SC;
}
.alert-for-not-finished-content .module .contant {
  font-size: 28rpx;
  font-weight: 400;
  color: #262626;
  text-align: justify;
  margin: 28rpx 0rpx 64rpx 0rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}
.alert-for-not-finished-content .module .contant .span {
  font-size: 32rpx;
  color: #f00;
}
.alert-for-not-finished-content .module .btn {
  height: 106rpx;
  font-size: 36rpx;
  font-weight: 400;
  color: #fff;
  line-height: 106rpx;
  border-radius: 54rpx;
  background: #2a4f44;
  font-family: PingFangSC-Regular, PingFang SC;
}
.alert-for-not-finished-content .module .close {
  width: 70rpx;
  height: 70rpx;
  overflow: hidden;
  border-radius: 50%;
  border: 7.3rpx solid #fff;
  position: absolute;
  bottom: -200rpx;
  left: 50%;
  transform: translateX(-50%);
}
.alert-for-not-finished-content .module .close::before {
  width: 70rpx;
  height: 70rpx;
  display: block;
  content: '\e63d';
  font-size: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 70rpx;
}
.alert-for-not-finished-content .moduleTwo .bottomBtn {
  display: flex;
  font-size: 36rpx;
  font-weight: 400;
  text-align: center;
  justify-content: space-between;
  font-family: PingFangSC-Regular, PingFang SC;
}
.alert-for-not-finished-content .moduleTwo .bottomBtn .cancel {
  width: 220rpx;
  height: 96rpx;
  color: #2a4f44;
  line-height: 96rpx;
  border-radius: 54rpx;
  background-color: #fff;
  border: 2rpx solid #2a4f44;
}
.alert-for-not-finished-content .moduleTwo .bottomBtn .ok {
  width: 310rpx;
  height: 96rpx;
  color: #fff;
  line-height: 96rpx;
  border-radius: 54rpx;
  background: #2a4f44;
}
.alert-for-not-finished-content .moduleThree .bottomBtn {
  width: 100%;
  display: flex;
  font-size: 36rpx;
  font-weight: 400;
  text-align: center;
  justify-content: space-between;
  font-family: PingFangSC-Regular, PingFang SC;
}
.alert-for-not-finished-content .moduleThree .bottomBtn .cancel {
  display: block;
  width: 220rpx;
  height: 96rpx;
  color: #2a4f44;
  line-height: 96rpx;
  border-radius: 54rpx;
  background-color: #fff;
  border: 2rpx solid #2a4f44;
}
.alert-for-not-finished-content .moduleThree .bottomBtn .ok {
  display: block;
  width: 310rpx;
  height: 96rpx;
  color: #fff;
  line-height: 96rpx;
  border-radius: 54rpx;
  background: #2a4f44;
}
.alert-for-not-finished-content .moduleSix {
  width: 100vw;
  height: 670rpx;
  margin-left: -28rpx;
  position: relative;
}
.alert-for-not-finished-content .moduleSix > .image {
  margin-left: -10rpx;
  width: 100%;
  height: 100%;
}
.alert-for-not-finished-content .moduleSix .btn {
  width: 315rpx;
  height: 90rpx;
  border-radius: 45rpx;
  position: absolute;
  bottom: -23rpx;
  left: 50%;
  transform: translateX(-50%);
}
.alert-for-not-finished-content .moduleSix .close {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  position: absolute;
  top: 8rpx;
  right: 30rpx;
}
.alert-for-not-finished-content .moduleSix .close .image {
  width: 100%;
  height: 100%;
}
@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@-webkit-keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@-webkit-keyframes zoomIn {
  from {
    opacity: 0;
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  50% {
    opacity: 1;
  }
}
@keyframes zoomIn {
  from {
    opacity: 0;
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  50% {
    opacity: 1;
  }
}
@-webkit-keyframes zoomOut {
  from {
    opacity: 1;
  }
  50% {
    opacity: 0;
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  to {
    opacity: 0;
  }
}
@keyframes zoomOut {
  from {
    opacity: 1;
  }
  50% {
    opacity: 0;
    transform: translateY(-50%) scale3d(0.3, 0.3, 0.3);
  }
  to {
    opacity: 0;
  }
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}
@keyframes fadeOut {
  from {
    opacity: 1;
  }
  to {
