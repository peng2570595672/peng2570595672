//字体图标
@import "../../fonts/iconfont.styl";
.iconfont {
    font-family:"iconfont" !important;
    font-size:16px;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.logoutExampleMask
    position fixed
    left 0
    right 0
    top 0
    bottom 0
    background: rgba(0, 0, 0, .6)
    z-index 9999999999999999
.logoutExampleDialog
    position: fixed;
    bottom 0
    left 0
    width 100%
    background: #FFFFFF;
    box-shadow: 0px 4rpx 16rpx 0rpx rgba(0, 0, 0, 0.13);
    border-radius 24rpx 24rpx 0 0
    padding 42rpx 40rpx 70rpx
    box-sizing border-box
    font-family: PingFangSC-Thin, sans-serif;
    .logoutExampleBtnClose
        position absolute
        right 30rpx
        top 28rpx
        &:before
            display block
            content '\e63d'
            font-size 30rpx
            color #979797
    .logoutExampleTitle
        font-size: 34rpx;
        font-weight: 600;
        line-height: 40rpx;
        color: #333333;
        text-align center
        margin-bottom 24rpx
    .logoutExampleContent
        font-size: 28rpx;
        font-weight: normal;
        line-height: 44rpx;
        color: #222
        text-align center
        margin-bottom 72rpx
    .logoutExampleContainer
        text-align center
        font-size: 24rpx;
        font-weight: normal;
        line-height: 40rpx;
        color: #666666;
        .item
            .imgInfo
                display flex
                align-items center
                justify-content center
                .first
                    .logoutExampleImg
                        width 132rpx
                .second
                    .logoutExampleImg
                        width 36rpx
                .third
                    .logoutExampleImg
                        width 288rpx
                &.imgInfoLast
                    justify-content space-between
                    .fourth
                    .fifth
                        padding-top 92rpx
                        padding-bottom 120rpx
                        text-align center
                        .logoutExampleImg
                            width 320rpx
            .tips
                margin-left 180rpx
                margin-top 16rpx

        .next
            margin-top 40rpx
            .logoutExampleImg
                width 320rpx
