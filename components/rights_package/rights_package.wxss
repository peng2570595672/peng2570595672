.related-service {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0,0,0,0.5);
  z-index: 9999999999999;
}
.related-service .related-service-wrapper {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  height: 1040rpx;
  padding-bottom: 20rpx;
  background: #fff;
  border-top-left-radius: 32rpx;
  border-top-right-radius: 32rpx;
}
.related-service .related-service-wrapper .related-service-box {
  overflow: scroll;
  padding: 52rpx 0 220rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-title {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 48rpx;
  font-size: 44rpx;
  font-weight: 500;
  position: relative;
  padding: 0 48rpx;
  color: rgba(0,0,0,0.9);
}
.related-service .related-service-wrapper .related-service-box .related-service-title .name {
  flex: 1;
}
.related-service .related-service-wrapper .related-service-box .related-service-list {
  padding: 0 32rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .title {
  margin-top: 50rpx;
  display: flex;
  align-items: center;
  height: 56rpx;
  font-size: 34rpx;
  font-weight: 500;
  font-family: PingFangSC-Medium, PingFang SC;
  color: #000;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .title::before {
  margin-right: 12rpx;
  content: '';
  width: 8rpx;
  height: 44rpx;
  background: #2fb565;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item {
  display: flex;
  height: 176rpx;
  align-items: center;
  background: #f7f7f7;
  margin-top: 32rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item image {
  width: 80rpx;
  height: 80rpx;
  margin-left: 48rpx;
  margin-right: 18rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item::after,
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item::before {
  display: inline-block;
  content: '';
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background: #fff;
  margin-right: -16rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item::before {
  margin-left: -16rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item .list-name {
  flex: 1;
  display: flex;
  height: 80rpx;
  align-items: center;
  color: rgba(0,0,0,0.3);
  font-size: 24rpx;
  line-height: 34rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item .list-name > view > view:first-child {
  color: rgba(0,0,0,0.5);
  font-size: 28rpx;
  line-height: 40rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item .amount {
  display: flex;
  height: 40rpx;
  align-items: center;
  color: #66666d;
  font-size: 40rpx;
  margin-right: 40rpx;
}
.related-service .related-service-wrapper .related-service-box .related-service-list .related-service-item .amount::before {
  content: '\e63d';
  font-size: 20rpx;
  margin-right: 8rpx;
}
.related-service .related-service-wrapper .bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  width: 100%;
  height: 264rpx;
  padding: 10rpx 32rpx;
  box-sizing: border-box;
}
.related-service .related-service-wrapper .bottom .tips {
  padding-bottom: 48rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #ffc300;
}
.related-service .related-service-wrapper .bottom .btn {
  display: flex;
}
.related-service .related-service-wrapper .bottom .btn > view {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 98rpx;
  border-radius: 49rpx;
}
.related-service .related-service-wrapper .bottom .btn > view:first-child {
  width: 214rpx;
  margin-right: 32rpx;
}
.related-service .related-service-wrapper .bottom .btn > view:last-child {
  flex: 1;
}
.related-service .related-service-wrapper .bottom .btn > view.close {
  font-size: 28rpx;
  color: #2fb565;
  border: 2rpx solid #2fb565;
}
.related-service .related-service-wrapper .bottom .btn > view.pay {
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #fff;
  background: linear-gradient(45deg, #14ac69 0%, #84d157 100%);
}
