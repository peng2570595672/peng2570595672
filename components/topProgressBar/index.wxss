.top {
  width: 100%;
  height: 92rpx;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  background-color: #ececec;
  z-index: 10;
}
.top .innerBox {
  width: 100%;
  height: 36rpx;
  display: flex;
  font-size: 20rpx;
  padding: 0rpx 44rpx;
  align-items: center;
  justify-content: space-between;
}
.top .innerBox .stage {
  width: 38rpx;
  height: 28rpx;
  color: #fff;
  line-height: 28rpx;
  text-align: center;
  font-weight: normal;
  line-height: 32rpx;
  border-radius: 28rpx;
  background: #e3e3e3;
  border: 4rpx solid #ececec;
  font-family: Arial-BoldItalicMT, Arial;
}
.top .innerBox .bar {
  width: 158rpx;
  height: 12rpx;
  overflow: hidden;
  border-radius: 6px;
  background: #e3e3e3;
}
.top .innerBox .bar .innerBar {
  height: 12rpx;
  border-radius: 6px;
}
