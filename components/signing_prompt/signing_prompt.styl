@import "animate.styl";
.alert-for-not-finished
	position: fixed
	left: 0
	right: 0
	top: 0
	bottom: 0
	background: rgba(0, 0, 0, 0.6)
	z-index: 10000
	.alert-for-not-finished-content
		position absolute
		left 0
		right 0
		top 50%
		transform translate(0, -50%)
		width 650rpx
		height 858rpx
		background rgba(255,255,255,1)
		box-shadow 4rpx 12rpx 56rpx 0px rgba(102,102,109,0.19)
		border-radius 32rpx
		margin 0 auto
		z-index 2
		.alert-for-box
			position absolute
			height 858rpx
			bottom 0
			left 0
			right 0
			margin auto
			z-index 2
			color #33333d
			background #fff
			border-radius 20rpx
			text-align center
			line-height 56rpx
			font-size 40rpx
			font-weight 500
			font-family PingFangSC-Medium,PingFang SC
			.code
				text-align center
				padding 72rpx 0 44rpx
				.img
					width 308rpx
			.alert-prompt
				padding 44rpx 70rpx
				color #66666D
				line-height 40rpx
				font-weight 400
				font-size 28rpx
			.alert-for-button
				position absolute
				bottom 0
				left 24rpx
				width 596rpx
				text-align center
				font-size 32rpx
				height 122rpx
				line-height 122rpx
				border-top 1rpx solid #f2f2f2
				.btn
					display inline-block
					width 50%
					&:last-child
						color #15AD69
