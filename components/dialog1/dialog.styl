@import "animate.styl";
//字体图标
@import "../../fonts/iconfont.styl";
.iconfont {
	font-family:"iconfont" !important;
	font-size:16px;
	font-style:normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.alert-for-not-finished
	position: fixed
	left: 0
	right: 0
	top: 0
	bottom: 0
	background: rgba(0, 0, 0, 0.6)
	z-index: 10000
.alert-for-not-finished-content
	position absolute
	left 0
	right 0
	top 50%
	transform translate(0, -50%)
	width 640rpx
	height auto
	background rgba(255,255,255,1)
	box-shadow 4rpx 12rpx 56rpx 0px rgba(102,102,109,0.19)
	border-radius 16rpx
	margin 0 auto
	z-index 100002
	.dialog-container
		position absolute
		left 50%
		top 50%
		width 100%
		min-height 464rpx
		border 2rpx solid #ccc
		border-radius 16rpx
		box-sizing bordre-box
		transform translate(-50%, -50%)
		overflow hidden
		background #fff
		display flex
		flex-direction column
		.wx-dialog-title
			text-align center
			color rgba(0, 0, 0, .9)
			font-size 34rpx
			padding 64rpx 0 32rpx
			font-family PingFangSC-Medium, PingFang SC
			line-height 48rpx
			font-weight 500
		.wx-dialog-con
			text-align center
			padding 0 48rpx
			font-family PingFangSC-Regular, PingFang SC
			font-weight 400
			color rgba(0, 0, 0, .5)
			font-size 34rpx
			line-height 48rpx
		.fill
			flex 1
		.wx-dialog-btn
			display flex
			height 110rpx
			align-items center
			justify-content center
			border-top 1rpx solid rgba(0, 0, 0, .1)
			margin-top 40rpx
			& > view
				flex 1
				display flex
				height 110rpx
				align-items center
				justify-content center
				font-size 34rpx
				color #576B95
				font-family PingFangSC-Medium, PingFang SC
				font-weight 500
				&.cancel
					border-right 1rpx solid rgba(0, 0, 0, .1)
					color rgba(0, 0, 0, .9)
				&.confirm
					border-right 1rpx solid rgba(0,0,0,0.1)
  				color #2FB565;
