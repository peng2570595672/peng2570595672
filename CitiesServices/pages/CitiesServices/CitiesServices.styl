@import "../../../style/base.styl";
.page
	display block
	width 100vw
	height 100vh
	overflow-x hidden
	overflow-y auto
	-webkit-overflow-scrolling touch
	background-color #F0F0F0
	.big-title
		font-size 48rpx
		font-weight 900
		color #33333D
		margin 30rpx 0 0 71rpx
	.small-title
		font-size 48rpx
		font-weight 500
		color #66666D
		margin 0 0 14rpx 71rpx
	.banner-content
		position relative
		width 691rpx
		height 662rpx
		margin 0 0 50rpx 30rpx
		background-image url("data:image/png;base64,")
		border-radius 40rpx
		background-size 100% 100%
		box-shadow 0 11rpx 20rpx 0 rgba(24,169,142,0.35)
		.char
			position absolute
			top 54rpx
			left 41rpx
			font-size 40rpx
			color #EBB137
		.btn
			position absolute
			display flex
			justify-content center
			align-items center
			left 34rpx
			top 124rpx
			width 198rpx
			height 57rpx
			background-color #fff
			border-radius 29rpx
			color #6BC95A
			font-size 30rpx
			.ltfont
				margin-left 5rpx
				transform rotate(180deg)
				-ms-transform rotate(180deg)
				-webkit-transform rotate(180deg)
			.ltfont:before
				content '\e60e'
	.btns
		display flex
		flex-direction column
		justify-content flex-start
		align-items center
		.btn
			display flex
			justify-content center
			align-items center
			width 650rpx
			height 106rpx
			box-shadow 0 8rpx 23rpx 0 rgba(201,230,213,0.13)
			border-radius 76rpx
			font-size 40rpx
			color #fff
			&.myetc
				background linear-gradient(32deg,rgba(20,172,105,1) 0%,rgba(132,209,87,1) 100%)
				margin-bottom 32rpx
			&.help-center
				background rgba(255,167,99,1)
	// 
	.bind-mobile-mask
		position fixed
		left 0
		right 0
		top 0
		bottom 0
		background: rgba(0, 0, 0, .6)
		.bind-mobile-wrapper
			position fixed
			left 0
			right 0
			bottom 0
			padding 66rpx
			background: #fff
			border-top-left-radius 20rpx
			border-top-right-radius 20rpx
			.title
				display flex
				width 100%
				align-items center
				font-size 40rpx
				&::before,
				&::after
					flex 1
					content ''
					height 2rpx
				&::before
					margin-right 30rpx
					background: linear-gradient(to right, #fff, #D9D9D9)
				&::after
					margin-left 30rpx
					background: linear-gradient(to right, #D9D9D9, #fff)
			.mobile
				display flex
				& > button
					margin-top 71rpx
					flex 1
					height 90rpx
					display flex
					align-items center
					justify-content center
					font-size 32rpx
					color #fff
					background: linear-gradient(to right , $deepThemeColor, $shallowThemeColor)
					border-radius 45rpx
					background-color #31D4B7
